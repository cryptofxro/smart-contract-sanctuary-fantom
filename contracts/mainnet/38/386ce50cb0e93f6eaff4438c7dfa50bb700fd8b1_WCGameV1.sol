     * while the token is not assigned a new owner, the `_ownedTokensIndex` mapping is _not_ updated: this allows for
     * gas optimizations e.g. when performing a transfer operation (avoiding double writes).
     * This has O(1) time complexity, but alters the order of the _ownedTokens array.
     * @param from address representing the previous owner of the given token ID
     * @param tokenId uint256 ID of the token to be removed from the tokens list of the given address
     */
    function _removeTokenFromOwnerEnumeration(address from, uint256 tokenId) private {
        // To prevent a gap in from's tokens array, we store the last token in the index of the token to delete, and
        // then delete the last slot (swap and pop).

        uint256 lastTokenIndex = ERC721.balanceOf(from) - 1;
        uint256 tokenIndex = _ownedTokensIndex[tokenId];

        // When the token to delete is the last token, the swap operation is unnecessary
        if (tokenIndex != lastTokenIndex) {
            uint256 lastTokenId = _ownedTokens[from][lastTokenIndex];

            _ownedTokens[from][tokenIndex] = lastTokenId; // Move the last token to the slot of the to-delete token
            _ownedTokensIndex[lastTokenId] = tokenIndex; // Update the moved token's index
        }

        // This also deletes the contents at the last position of the array
        delete _ownedTokensIndex[tokenId];
        delete _ownedTokens[from][lastTokenIndex];
    }

    /**
     * @dev Private function to remove a token from this extension's token tracking data structures.
     * This has O(1) time complexity, but alters the order of the _allTokens array.
     * @param tokenId uint256 ID of the token to be removed from the tokens list
     */
    function _removeTokenFromAllTokensEnumeration(uint256 tokenId) private {
        // To prevent a gap in the tokens array, we store the last token in the index of the token to delete, and
        // then delete the last slot (swap and pop).

        uint256 lastTokenIndex = _allTokens.length - 1;
        uint256 tokenIndex = _allTokensIndex[tokenId];

        // When the token to delete is the last token, the swap operation is unnecessary. However, since this occurs so
        // rarely (when the last minted token is burnt) that we still do the swap here to avoid the gas cost of adding
        // an 'if' statement (like in _removeTokenFromOwnerEnumeration)
        uint256 lastTokenId = _allTokens[lastTokenIndex];

        _allTokens[tokenIndex] = lastTokenId; // Move the last token to the slot of the to-delete token
        _allTokensIndex[lastTokenId] = tokenIndex; // Update the moved token's index

        // This also deletes the contents at the last position of the array
        delete _allTokensIndex[tokenId];
        _allTokens.pop();
    }
}


contract WCGameStats{
    uint256[][] public _weaponsHeld;
    uint256[][] public _traitScoreStats;
        
    uint256 public scoreStatIndex = 0;
    uint256 public weaponStatIndex = 0;
}

contract WCGameV1 is Ownable, ERC721Holder{

    address public Treasury = 0x3f6B955Bc6C879d00cEa84CFDc59c7091EA90720;

    ERC721 public constant _WC = ERC721(0xC031b7793F17100e9B7Ad369cA05e5ec8A0F5B5C);

    WCGameStats public constant CardStats = WCGameStats(0x19A76a66d74118Bbdd1e8d3412DEC18f0470A190);

    uint256 public damageBase = 1250;
    uint256[] public timeBuff = [100000,100180,100324,100583,101050,101890,103401,106122,111020,119836,133000];
    uint256 public timer = 20000;
    uint256 public _matchesCount = 0;
    uint256 public currentPlayers = 0;

    mapping(uint256 => MatchInfo) public _matchInfo;
    mapping(address => uint256) public currentDuels;

    mapping(address => Record) public walletRecord;
    mapping(uint256 => Record) public cardRecord;
    mapping(uint256 => History) public history;
    
    mapping(uint256 => QueueStruct) public _queue;
    mapping(uint256 => Glossary) public queueGlossary;

    struct MatchInfo{
        Duelist a;
        Duelist b;
        uint256 matchType;
        uint256 startTime;
        uint256 startTimeUnix;
        bool matchOver;
    }

    struct Duelist{
        address Owner;
        uint256 TokenID;
        uint256 Hp;
        uint256 Att;
        uint256 Def;
        uint256 Spd;
        uint256 Type;
        uint256 NextTurn;
    }

    struct History{
        uint256 duration;

        address winner;
        address loser;

        uint256 cardW;
        uint256 cardL;

        mapping(uint256 => Move) attacks;
        uint256 moveCount;
    }

    struct Move{
        uint256 attacker;
        uint256 defender;

        uint256 damage;
        uint256 advantage;

        uint256 timestamp;
    }

    struct Record {
        uint256 wins;
        uint256 losses;
        uint256 lastDuel;
        uint256 elo;
    }

    struct QueueStruct {
        uint256 TokenID;
        address Address;
        uint256 Weapon;
    }

    struct Glossary{
        uint256 price;
        address currency;
        bool enabled;
    }



    function Queue(uint256 tokenID, uint256 weaponChoice, uint256 queueType) payable public{
        require(queueGlossary[queueType].enabled == true, "This queueType is not enabled");
        require(msg.sender == _WC.ownerOf(tokenID), "WC is not yours");
        require(currentDuels[msg.sender] == 0, "Only one duel at a time per address");
        require(weaponChoice == 0 || weaponChoice == 1, "weaponChoice has to be left or right hand (0 or 1)");

        if(queueGlossary[queueType].currency != address(0)){
            bool transferComplete = ERC20(queueGlossary[queueType].currency).transferFrom(msg.sender, address(this), queueGlossary[queueType].price);
            if(transferComplete == false){
                revert("Something bad happened with the ERC20");
            }
        }else{
            if(queueGlossary[queueType].price != msg.value){
                revert("Incorrect amount sent for this duel type");
            }
        }
        _WC.safeTransferFrom(msg.sender, address(this), tokenID);
        currentPlayers++;

        if(_queue[queueType].TokenID == 0){
            _queue[queueType].TokenID = tokenID;
            _queue[queueType].Address = msg.sender;
            _queue[queueType].Weapon = weaponChoice;
            currentDuels[msg.sender] = 1;

        }else{
            uint256 aTokenID = _queue[queueType].TokenID;
            uint256 bTokenID = tokenID;
            
            _matchesCount++;
            currentDuels[msg.sender] = _matchesCount + 2;
            currentDuels[_queue[queueType].Address] = _matchesCount + 2;

            
            MatchInfo memory matchInfo;

            matchInfo.a.Owner = _queue[queueType].Address;
            matchInfo.a.TokenID = aTokenID;
            matchInfo.a.Hp = CardStats._traitScoreStats(aTokenID,0);
            matchInfo.a.Att = CardStats._traitScoreStats(aTokenID,1); 
            matchInfo.a.Def = CardStats._traitScoreStats(aTokenID,2);
            matchInfo.a.Spd = CardStats._traitScoreStats(aTokenID,3);
            matchInfo.a.Type = CardStats._weaponsHeld(aTokenID,(_queue[queueType].Weapon));
            matchInfo.a.NextTurn = timer/(CardStats._traitScoreStats(aTokenID,3)) + 8 + block.number;

            matchInfo.b.Owner = msg.sender;
            matchInfo.b.TokenID = bTokenID;
            matchInfo.b.Hp = CardStats._traitScoreStats(bTokenID,0);
            matchInfo.b.Att = CardStats._traitScoreStats(bTokenID,1); 
            matchInfo.b.Def = CardStats._traitScoreStats(bTokenID,2);
            matchInfo.b.Spd = CardStats._traitScoreStats(bTokenID,3);
            matchInfo.b.Type = CardStats._weaponsHeld(bTokenID,weaponChoice);
            matchInfo.b.NextTurn =  timer/(CardStats._traitScoreStats(bTokenID,3)) + 8 + block.number;

            matchInfo.matchType = queueType;
            matchInfo.startTime = block.number;
            matchInfo.startTimeUnix = block.timestamp;
            matchInfo.matchOver = false;

            _matchInfo[_matchesCount] = matchInfo;

            // Remove WC from queue
            _queue[queueType].TokenID = 0;
        }
    }


    //weaponChoice: 0 is left arm, 1 is right arm. This determines trait
    function Duel(uint256 weaponChoice) public{
        uint256 matchCount = currentDuels[msg.sender]-2;
        require(matchCount >= 1, "no active match can be found with this WC");
        require(weaponChoice == 0 || weaponChoice == 1, "weaponChoice has to be left or right hand (0 or 1)");
        require(msg.sender == _matchInfo[matchCount].a.Owner || msg.sender == _matchInfo[matchCount].b.Owner, "you are not the owner of this WC");
        if(msg.sender == _matchInfo[matchCount].a.Owner){
            require(_matchInfo[matchCount].a.NextTurn <= block.number, "you can't attack yet");
            
            uint256 delta = block.number - _matchInfo[matchCount].a.NextTurn;
            if(delta >= 10){
                delta = 10;
            }
            
            uint256 advantage = calculateDamageMult(CardStats._weaponsHeld(_matchInfo[matchCount].a.TokenID,weaponChoice),_matchInfo[matchCount].b.Type);
            uint256 damage = (((((_matchInfo[matchCount].a.Att*damageBase)/_matchInfo[matchCount].b.Def)*advantage)*timeBuff[delta])/200000);

            if(damage > _matchInfo[matchCount].b.Hp){
                _matchInfo[matchCount].b.Hp = 0;
            }else{
                _matchInfo[matchCount].b.Hp = _matchInfo[matchCount].b.Hp - damage;
            }
            
            _matchInfo[matchCount].a.NextTurn = (timer/_matchInfo[matchCount].a.Spd) + 8 + block.number;
            _matchInfo[matchCount].a.Type = CardStats._weaponsHeld(_matchInfo[matchCount].a.TokenID,weaponChoice);

            history[matchCount].attacks[history[matchCount].moveCount].attacker = _matchInfo[matchCount].a.TokenID;
            history[matchCount].attacks[history[matchCount].moveCount].defender = _matchInfo[matchCount].b.TokenID;
            history[matchCount].attacks[history[matchCount].moveCount].damage = damage;
            history[matchCount].attacks[history[matchCount].moveCount].advantage = advantage;
            history[matchCount].attacks[history[matchCount].moveCount].timestamp = block.number;

            history[matchCount].moveCount++;

            if(_matchInfo[matchCount].b.Hp == 0){
                _matchInfo[matchCount].matchOver = true;
                currentDuels[msg.sender] = 0;
                currentDuels[_matchInfo[matchCount].b.Owner] = 0;

                walletRecord[_matchInfo[matchCount].a.Owner].wins++;
                walletRecord[_matchInfo[matchCount].b.Owner].losses++;

                walletRecord[_matchInfo[matchCount].a.Owner].lastDuel = block.number;
                walletRecord[_matchInfo[matchCount].b.Owner].lastDuel = block.number;


                if(cardRecord[_matchInfo[matchCount].a.TokenID].elo == 0 && cardRecord[_matchInfo[matchCount].a.TokenID].losses == 0){
                    cardRecord[_matchInfo[matchCount].a.TokenID].elo = 1000;
                }
                if(cardRecord[_matchInfo[matchCount].b.TokenID].elo == 0 && cardRecord[_matchInfo[matchCount].b.TokenID].losses == 0){
                    cardRecord[_matchInfo[matchCount].b.TokenID].elo = 1000;
                }
                

                if(cardRecord[_matchInfo[matchCount].a.TokenID].elo > cardRecord[_matchInfo[matchCount].b.TokenID].elo){
                    uint256 Elo1 = cardRecord[_matchInfo[matchCount].a.TokenID].elo;
                    uint256 Elo2 = cardRecord[_matchInfo[matchCount].b.TokenID].elo;

                    uint256 Ex1 = ((Elo1**7)*100)/((Elo1**7)+(Elo2**7));
            
                    uint256 newElo = ((100-Ex1)*42)/100;
                    
                    cardRecord[_matchInfo[matchCount].a.TokenID].elo = Elo1 + newElo;
                    cardRecord[_matchInfo[matchCount].b.TokenID].elo = Elo2 - newElo;
                }else{
                    uint256 Elo1 = cardRecord[_matchInfo[matchCount].b.TokenID].elo;
                    uint256 Elo2 = cardRecord[_matchInfo[matchCount].a.TokenID].elo;
                    
                    uint256 Ex1 = ((Elo2**7)*100)/((Elo1**7)+(Elo2**7));
            
                    uint256 newElo = ((100-Ex1)*42)/100;
                    
                    cardRecord[_matchInfo[matchCount].a.TokenID].elo = Elo2 + newElo;
                    cardRecord[_matchInfo[matchCount].b.TokenID].elo = Elo1 - newElo;
                }

                cardRecord[_matchInfo[matchCount].a.TokenID].wins++;
                cardRecord[_matchInfo[matchCount].b.TokenID].losses++;

                cardRecord[_matchInfo[matchCount].a.TokenID].lastDuel = block.number;
                cardRecord[_matchInfo[matchCount].b.TokenID].lastDuel = block.number;


                history[matchCount].winner = _matchInfo[matchCount].a.Owner;
                history[matchCount].loser = _matchInfo[matchCount].b.Owner;

                history[matchCount].cardW = _matchInfo[matchCount].a.TokenID;
                history[matchCount].cardL = _matchInfo[matchCount].b.TokenID;

                history[matchCount].duration = block.timestamp - _matchInfo[matchCount].startTimeUnix;
                
                _WC.safeTransferFrom(address(this), msg.sender, _matchInfo[matchCount].a.TokenID);
                _WC.safeTransferFrom(address(this), _matchInfo[matchCount].b.Owner, _matchInfo[matchCount].b.TokenID);

                currentPlayers = currentPlayers -2;

                //Winner gets 90%, Treasury gets 10%
                uint256 prize = ((queueGlossary[_matchInfo[matchCount].matchType].price)*2)/10;
                if(queueGlossary[_matchInfo[matchCount].matchType].currency != address(0)){
                    bool transferCompleteWinner = ERC20(queueGlossary[_matchInfo[matchCount].matchType].currency).transfer(address(_matchInfo[matchCount].a.Owner), prize*9);
                    bool transferCompleteTreasury = ERC20(queueGlossary[_matchInfo[matchCount].matchType].currency).transfer(address(Treasury), prize);
                    if(transferCompleteWinner == false || transferCompleteTreasury == false){
                        revert("Something bad happened with the ERC20");
                    }
                }else{
                    payable(_matchInfo[matchCount].a.Owner).transfer(prize*9);
                    payable(Treasury).transfer(prize);
                }

            }

        }else{
            require(_matchInfo[matchCount].b.NextTurn <= block.number, "you can't attack yet");
            
            uint256 delta = block.number - _matchInfo[matchCount].b.NextTurn;
            if(delta >= 10){
                delta = 10;
            }
            
            uint256 advantage = calculateDamageMult(CardStats._weaponsHeld(_matchInfo[matchCount].b.TokenID,weaponChoice),_matchInfo[matchCount].a.Type);
            uint256 damage = (((((_matchInfo[matchCount].b.Att*damageBase)/_matchInfo[matchCount].a.Def)*advantage)*timeBuff[delta])/200000);
            
            if(damage > _matchInfo[matchCount].a.Hp){
                _matchInfo[matchCount].a.Hp = 0;
            }else{
                _matchInfo[matchCount].a.Hp = _matchInfo[matchCount].a.Hp - damage;
            }

            _matchInfo[matchCount].b.NextTurn = (timer/_matchInfo[matchCount].b.Spd) + 8 + block.number;
            _matchInfo[matchCount].b.Type = CardStats._weaponsHeld(_matchInfo[matchCount].b.TokenID,weaponChoice);

            history[matchCount].attacks[history[matchCount].moveCount].attacker = _matchInfo[matchCount].b.TokenID;
            history[matchCount].attacks[history[matchCount].moveCount].defender = _matchInfo[matchCount].a.TokenID;
            history[matchCount].attacks[history[matchCount].moveCount].damage = damage;
            history[matchCount].attacks[history[matchCount].moveCount].advantage = advantage;
            history[matchCount].attacks[history[matchCount].moveCount].timestamp = block.number;

            history[matchCount].moveCount++;

            if(_matchInfo[matchCount].a.Hp == 0){
                _matchInfo[matchCount].matchOver = true;
                currentDuels[msg.sender] = 0;
                currentDuels[_matchInfo[matchCount].a.Owner] = 0;

                walletRecord[_matchInfo[matchCount].b.Owner].wins++;
                walletRecord[_matchInfo[matchCount].a.Owner].losses++;

                walletRecord[_matchInfo[matchCount].b.Owner].lastDuel = block.number;
                walletRecord[_matchInfo[matchCount].a.Owner].lastDuel = block.number;

                if(cardRecord[_matchInfo[matchCount].a.TokenID].elo == 0 && cardRecord[_matchInfo[matchCount].a.TokenID].losses == 0){
                    cardRecord[_matchInfo[matchCount].a.TokenID].elo = 1000;
                }
                if(cardRecord[_matchInfo[matchCount].b.TokenID].elo == 0 && cardRecord[_matchInfo[matchCount].b.TokenID].losses == 0){
                    cardRecord[_matchInfo[matchCount].b.TokenID].elo = 1000;
                }
                

                if(cardRecord[_matchInfo[matchCount].b.TokenID].elo > cardRecord[_matchInfo[matchCount].a.TokenID].elo){
                    uint256 Elo1 = cardRecord[_matchInfo[matchCount].b.TokenID].elo;
                    uint256 Elo2 = cardRecord[_matchInfo[matchCount].a.TokenID].elo;

                    uint256 Ex1 = ((Elo1**7)*100)/((Elo1**7)+(Elo2**7));
            
                    uint256 newElo = ((100-Ex1)*42)/100;
                    
                    cardRecord[_matchInfo[matchCount].b.TokenID].elo = Elo1 + newElo;
                    cardRecord[_matchInfo[matchCount].a.TokenID].elo = Elo2 - newElo;
                }else{
                    uint256 Elo1 = cardRecord[_matchInfo[matchCount].a.TokenID].elo;
                    uint256 Elo2 = cardRecord[_matchInfo[matchCount].b.TokenID].elo;
                    
                    uint256 Ex1 = ((Elo2**7)*100)/((Elo1**7)+(Elo2**7));
            
                    uint256 newElo = ((100-Ex1)*42)/100;
                    
                    cardRecord[_matchInfo[matchCount].b.TokenID].elo = Elo2 + newElo;
                    cardRecord[_matchInfo[matchCount].a.TokenID].elo = Elo1 - newElo;
                }

                cardRecord[_matchInfo[matchCount].b.TokenID].wins++;
                cardRecord[_matchInfo[matchCount].a.TokenID].losses++;

                cardRecord[_matchInfo[matchCount].b.TokenID].lastDuel = block.number;
                cardRecord[_matchInfo[matchCount].a.TokenID].lastDuel = block.number;

                history[matchCount].winner = _matchInfo[matchCount].b.Owner;
                history[matchCount].loser = _matchInfo[matchCount].a.Owner;

                history[matchCount].cardW = _matchInfo[matchCount].b.TokenID;
                history[matchCount].cardL = _matchInfo[matchCount].a.TokenID;

                history[matchCount].duration = block.timestamp - _matchInfo[matchCount].startTimeUnix;
                
                _WC.safeTransferFrom(address(this), _matchInfo[matchCount].a.Owner, _matchInfo[matchCount].a.TokenID);
                _WC.safeTransferFrom(address(this), msg.sender, _matchInfo[matchCount].b.TokenID);

                currentPlayers = currentPlayers -2;

                //Winner gets 90%, Treasury gets 10%
                uint256 prize = ((queueGlossary[_matchInfo[matchCount].matchType].price)*2)/10;
                if(queueGlossary[_matchInfo[matchCount].matchType].currency != address(0)){
                    bool transferCompleteWinner = ERC20(queueGlossary[_matchInfo[matchCount].matchType].currency).transfer(address(_matchInfo[matchCount].b.Owner), prize*9);
                    bool transferCompleteTreasury = ERC20(queueGlossary[_matchInfo[matchCount].matchType].currency).transfer(address(Treasury), prize);
                    if(transferCompleteWinner == false || transferCompleteTreasury == false){
                        revert("Something bad happened with the ERC20");
                    }
                }else{
                    payable(_matchInfo[matchCount].b.Owner).transfer(prize*9);
                    payable(Treasury).transfer(prize);
                }
            }
        }
    }


    function removeFromQueue(uint256 queueType) public returns(bool){
        require(msg.sender == _queue[queueType].Address, "Your WC isn't in queue");
        
        currentDuels[msg.sender] = 0;
        currentPlayers--;
        _WC.safeTransferFrom(address(this), msg.sender, _queue[queueType].TokenID);
        _queue[queueType].TokenID = 0;
        if(queueGlossary[queueType].currency != address(0)){
            bool transferComplete = ERC20(queueGlossary[queueType].currency).transfer(msg.sender, queueGlossary[queueType].price);
            if(transferComplete == false){
                revert("Something bad happened with the ERC20");
            }
        }else{
            payable(msg.sender).transfer(queueGlossary[queueType].price);
        }
        
        return true;
    }


    //calculates damage multiplier based on type
    // 0 None, 1 Blunt, 2 Piercing, 3 Slashing
    function calculateDamageMult(uint256 attackerType, uint256 defenderType) public pure returns(uint256){
        if(attackerType == 0){
            return(1);
        }else if(defenderType == 0){
            return(2);
        }else if(attackerType == 1){
            if(defenderType == 1){
                return(2);
            }else if(defenderType == 2){
                return(1);
            }else{
                return(4);
            }
        }else if(attackerType == 2){
            if(defenderType == 1){
                return(4);
            }else if(defenderType == 2){
                return(2);
            }else{
                return(1);
            }
        }else{
            if(defenderType == 1){
                return(1);
            }else if(defenderType == 2){
                return(4);
            }else{
                return(2);
            }
        }
    }
    
    function getMoves(uint256 X) public view returns(Move[] memory){
        uint256 length = history[X].moveCount;
        Move[] memory result = new Move[](length);
        for (uint i = 0; i < length; i++) {
            result[i] = history[X].attacks[i];
        }
        return result;
    }
  
        
    function addGlossaryPrice(uint256 _price, uint256 index) public onlyOwner {
        queueGlossary[index].price = _price;
        queueGlossary[index].enabled = false;
    }

    function addGlossaryCurrency(address _currency, uint256 index) public onlyOwner {
        queueGlossary[index].currency = _currency;
        queueGlossary[index].enabled = false;
    }

    function enableGlossary(uint256 index) public onlyOwner {
        queueGlossary[index].enabled = true;
    }


    function changeTreasury(address newOwner) public onlyOwner {
        require(newOwner != address(0), "Ownable: new owner is the zero address");
        Treasury = newOwner;
    }

    //This is for worst case scenario. Can only be used after a match has been in progress for more than an hour
    function endDuel(uint256 matchCount) public onlyOwner {
        require((block.timestamp - _matchInfo[matchCount].startTimeUnix) >= 3600);

         _matchInfo[matchCount].matchOver = true;
         _matchInfo[matchCount].matchType = 10000;

        currentDuels[_matchInfo[matchCount].a.Owner] = 0;
        currentDuels[_matchInfo[matchCount].b.Owner] = 0;
        currentPlayers = currentPlayers -2;
        
        _WC.safeTransferFrom(address(this), _matchInfo[matchCount].a.Owner, _matchInfo[matchCount].a.TokenID);
        _WC.safeTransferFrom(address(this), _matchInfo[matchCount].b.Owner, _matchInfo[matchCount].b.TokenID);


        uint256 prize = ((queueGlossary[_matchInfo[matchCount].matchType].price)*2)/10;
        if(queueGlossary[_matchInfo[matchCount].matchType].currency != address(0)){
            bool transferCompleteA = ERC20(queueGlossary[_matchInfo[matchCount].matchType].currency).transfer(address(_matchInfo[matchCount].a.Owner), prize*5);
            bool transferCompleteB = ERC20(queueGlossary[_matchInfo[matchCount].matchType].currency).transfer(address(_matchInfo[matchCount].b.Owner), prize*5);
            if(transferCompleteA == false || transferCompleteB == false){
                revert("Something bad happened with the ERC20");
            }
        }else{
            payable(_matchInfo[matchCount].a.Owner).transfer(prize*5);
            payable(_matchInfo[matchCount].b.Owner).transfer(prize*5);
        }

    }
    
}