pragma solidity ^0.8.9;

import "@openzeppelin/contracts/interfaces/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import '@uniswap/v2-core/contracts/interfaces/IUniswapV2Factory.sol';
import "@layerzerolabs/contracts/contracts/interfaces/ILayerZeroEndpoint.sol";
import '@uniswap/lib/contracts/libraries/TransferHelper.sol';
import '@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol';

import "hardhat/console.sol";

import './owner/Operator.sol';
import './interfaces/IUniswapV2Router.sol';
import './interfaces/IONFT.sol';

    struct BridgeToken
    {
        bool isEnabled;
        uint256 totalAmountBridged;
    }

    struct ChainParam
    {
        bool isEnabled;
        address recieveEndpointAddress;
    }

contract OneWayNftBridge is Operator, Pausable {

    address public immutable WETH;
    address public immutable USDC;

    AggregatorV3Interface chainLinkAggregator;
    ILayerZeroEndpoint public immutable layerZeroEndpoint;

    address public bridgeTokenBoiler;
    address public feeTokenBoiler;
    address public feeToken;
    uint public feeValue;
    IUniswapV2Router public feeTokenRouter;

    mapping(address => BridgeToken) public bridgeTokens;
    mapping(uint => ChainParam) public chainEndpoints;

    event BridgeTokenSet(address token,bool isEnabled);
    event FeeTokenSet(address feeToken,address feeTokenRouter);
    event FeeValueSet(uint fee);

    event DestEndpointSet(uint endpointId,address destEndPointAddress,
        bool isEnabled);

    event Bridged(uint amount, bytes to, address token);

    constructor(address _layerZeroEndpoint,address _WETH,address _USDC,
                                            address _chainLinkAggregator) {
        require(_layerZeroEndpoint != address(0));
        require(_chainLinkAggregator != address(0));
        layerZeroEndpoint = ILayerZeroEndpoint(_layerZeroEndpoint);
        WETH = _WETH;
        USDC = _USDC;
        chainLinkAggregator = AggregatorV3Interface(_chainLinkAggregator);
    }

    function normalizePrice(uint256 _price, uint8 _decimal) internal
    view returns(uint256)
    {

        if(_decimal > chainLinkAggregator.decimals()){
            _price = _price * (10 ** (_decimal - chainLinkAggregator.decimals()));
        }
        else if(_decimal == chainLinkAggregator.decimals())
        {
            return _price;
        }
        else
        {
            _price = _price / (10 ** (chainLinkAggregator.decimals()-_decimal));
        }
        return _price;
    }

    function updateChainLinkAggregator(address _chainLinkAggregator) external onlyOperator
    {
        require(_chainLinkAggregator != address(0),"Invalid argument");
        chainLinkAggregator = AggregatorV3Interface(_chainLinkAggregator);
    }

    function setupBoilerParams(address _feeTokenBoiler,
        address _bridgeTokenBoiler) external onlyOperator
    {
        feeTokenBoiler = _feeTokenBoiler;
        bridgeTokenBoiler = _bridgeTokenBoiler;
    }

    function setupFeeToken(address _feeToken,address _feeTokenRouter) external onlyOperator
    {
        require(_feeToken != address(0),"Invalid fee token address");
        require(_feeTokenRouter != address(0),"Invalid fee token router address");

        feeToken = _feeToken;
        feeTokenRouter = IUniswapV2Router(_feeTokenRouter);
    }

    function setupFees(uint _feeValue) external onlyOperator
    {
        require(address(feeTokenRouter) != address(0),"Fee token router must be set");
        require(feeToken != address(0),"Fee token must be set");

        if(_feeValue > 0)
            require(feeTokenBoiler != address(0),"Fee fund must be set");

        feeValue = _feeValue;

        emit FeeValueSet(feeValue);
    }

    function setupBridgeToken(address _tokenAddress,
        bool _isEnabled) external onlyOperator
    {
        require(_tokenAddress != address(0),"Invalid token address");
        bridgeTokens[_tokenAddress].isEnabled = _isEnabled;

        emit BridgeTokenSet(_tokenAddress,_isEnabled);
    }

    function setupDestChainParam(uint _chainId, address _recieveEndpointAddress,
        bool _isEnabled) external onlyOperator
    {
        require( _chainId != 0, "Invalid chain ID");
        require(_recieveEndpointAddress != address(0),"Invalid endpoint address ");

        chainEndpoints[_chainId].isEnabled = _isEnabled;
        chainEndpoints[_chainId].recieveEndpointAddress = _recieveEndpointAddress;

        emit DestEndpointSet(_chainId,_recieveEndpointAddress,_isEnabled);
    }

    function estimateTransferFees(uint16 _destChainId,bytes calldata _recieverAddress,
        address _tokenAddress,
        uint _tokenId) public view returns (uint256,uint256)
    {
        require(bridgeTokens[_tokenAddress].isEnabled == true,
            "Token is not allowed");
        require(chainEndpoints[_destChainId].isEnabled == true,
            "Chain is not allowed");
        bytes memory payload = abi.encode(_recieverAddress, _tokenId);
        bytes memory adapterParams = "";
        ( uint256 layerZeroFee,) = layerZeroEndpoint.estimateFees(_destChainId,
            address(this),payload,false,adapterParams) ;
        layerZeroFee = layerZeroFee + 10**16; //For burn;
        uint256 feeInFeeToken = 0;
        if(feeValue > 0)
        {
            (, int256 price, , , ) = chainLinkAggregator.latestRoundData();
            uint256 normalizedPrice = normalizePrice(uint256(price),ERC20(WETH).decimals());
            uint256 feeinWETH = 0;
            if(normalizedPrice < 10**ERC20(WETH).decimals()) {
                feeinWETH = feeValue * 10**ERC20(WETH).decimals() / normalizedPrice;
            }
            else{
                feeinWETH = feeValue * normalizedPrice / 10**ERC20(WETH).decimals();
            }

            address[] memory path = new address[](2);
            path[0] = WETH;
            path[1] = feeToken;
            uint256[] memory amounts = feeTokenRouter.getAmountsOut(feeinWETH,path);
            feeInFeeToken =  amounts[amounts.length - 1];
        }
        return(layerZeroFee * 2,feeInFeeToken);
    }

    function bridge( uint16 _dstChainId,bytes calldata _recieverAddress,
        address _tokenAddress, uint _tokenId, address _zroPaymentAddress, bytes calldata _adapterParams) external payable whenNotPaused
    {
        require(bridgeTokens[_tokenAddress].isEnabled == true,
            "Token is not allowed");
        require(chainEndpoints[_dstChainId].isEnabled == true,
            "Chain is not allowed");
        require(_tokenId > 0, "Invalid tokenId");
        require(IERC721(_tokenAddress).ownerOf(_tokenId) == msg.sender,
            "Can't bridge tokens you don't own!");

        (uint256 layerZeroFee,uint256 feeInFeeToken) = estimateTransferFees(_dstChainId,
            _recieverAddress,_tokenAddress,_tokenId);
        require(msg.value >= layerZeroFee,"Innsuficient FTM fee");
        if(feeInFeeToken > 0)
            TransferHelper.safeTransferFrom(feeToken, msg.sender, feeTokenBoiler, feeInFeeToken);

        bytes memory destinationBurn = abi.encodePacked(super.owner(),address(0));

        IONFT(_tokenAddress).sendFrom{value:msg.value / 2}(msg.sender,_dstChainId,destinationBurn,_tokenId,payable(msg.sender),_zroPaymentAddress,_adapterParams);

        bytes memory payload = abi.encode(_recieverAddress, _tokenId);
        bytes memory destination = abi.encodePacked(chainEndpoints[_dstChainId].recieveEndpointAddress,address(this));
        layerZeroEndpoint.send{value:msg.value / 2}(_dstChainId,destination,payload,
            payable(msg.sender),_zroPaymentAddress,_adapterParams);

        bridgeTokens[_tokenAddress].totalAmountBridged =
        bridgeTokens[_tokenAddress].totalAmountBridged + 1;
        emit Bridged(_tokenId,_recieverAddress,_tokenAddress);

    }

    function pause() public onlyOperator
    {
        super._pause();
    }
    function unpause() public onlyOperator
    {
        super._unpause();
    }

    function setConfig(uint16 _version, uint16 _chainId, uint _configType, bytes calldata _config) external onlyOperator
    {
        layerZeroEndpoint.setConfig(_version,_chainId,_configType,_config);
    }

    function setSendVersion(uint16 _version) external onlyOperator
    {
        layerZeroEndpoint.setSendVersion(_version);
    }

    function setReceiveVersion(uint16 _version) external onlyOperator
    {
        layerZeroEndpoint.setReceiveVersion(_version);
    }

    function forceResumeReceive(uint16 _srcChainId, bytes calldata _srcAddress) external onlyOperator
    {
        layerZeroEndpoint.forceResumeReceive(_srcChainId,_srcAddress);
    }

}

