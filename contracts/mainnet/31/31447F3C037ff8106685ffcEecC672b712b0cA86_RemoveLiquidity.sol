interface IPancakePair {
    event Approval(address indexed owner, address indexed spender, uint value);
    event Transfer(address indexed from, address indexed to, uint value);

    function name() external pure returns (string memory);
    function symbol() external pure returns (string memory);
    function decimals() external pure returns (uint8);
    function totalSupply() external view returns (uint);
    function balanceOf(address owner) external view returns (uint);
    function allowance(address owner, address spender) external view returns (uint);

    function approve(address spender, uint value) external returns (bool);
    function transfer(address to, uint value) external returns (bool);
    function transferFrom(address from, address to, uint value) external returns (bool);

    function DOMAIN_SEPARATOR() external view returns (bytes32);
    function PERMIT_TYPEHASH() external pure returns (bytes32);
    function nonces(address owner) external view returns (uint);

    function permit(address owner, address spender, uint value, uint deadline, uint8 v, bytes32 r, bytes32 s) external;

    event Mint(address indexed sender, uint amount0, uint amount1);
    event Burn(address indexed sender, uint amount0, uint amount1, address indexed to);
    event Swap(
        address indexed sender,
        uint amount0In,
        uint amount1In,
        uint amount0Out,
        uint amount1Out,
        address indexed to
    );
    event Sync(uint112 reserve0, uint112 reserve1);

    function MINIMUM_LIQUIDITY() external pure returns (uint);
    function factory() external view returns (address);
    function token0() external view returns (address);
    function token1() external view returns (address);
    function getReserves() external view returns (uint112 reserve0, uint112 reserve1, uint32 blockTimestampLast);
    function price0CumulativeLast() external view returns (uint);
    function price1CumulativeLast() external view returns (uint);
    function kLast() external view returns (uint);

    function mint(address to) external returns (uint liquidity);
    function burn(address to) external returns (uint amount0, uint amount1);
    function swap(uint amount0Out, uint amount1Out, address to, bytes calldata data) external;
    function skim(address to) external;
    function sync() external;

    function initialize(address, address) external;
}

abstract contract Context {
    function _msgSender() internal view virtual returns (address payable) {
        return msg.sender;
    }

    function _msgData() internal view virtual returns (bytes memory) {
        this; // silence state mutability warning without generating bytecode - see https://github.com/ethereum/solidity/issues/2691
        return msg.data;
    }
}

abstract contract Ownable is Context {
    address private _owner;

    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);

    /**
     * @dev Initializes the contract setting the deployer as the initial owner.
     */
    constructor () internal {
        address msgSender = _msgSender();
        _owner = msgSender;
        emit OwnershipTransferred(address(0), msgSender);
    }

    /**
     * @dev Returns the address of the current owner.
     */
    function owner() public view virtual returns (address) {
        return _owner;
    }

    /**
     * @dev Throws if called by any account other than the owner.
     */
    modifier onlyOwner() {
        require(owner() == _msgSender(), "Ownable: caller is not the owner");
        _;
    }

    /**
     * @dev Leaves the contract without owner. It will not be possible to call
     * `onlyOwner` functions anymore. Can only be called by the current owner.
     *
     * NOTE: Renouncing ownership will leave the contract without an owner,
     * thereby removing any functionality that is only available to the owner.
     */
    function renounceOwnership() public virtual onlyOwner {
        emit OwnershipTransferred(_owner, address(0));
        _owner = address(0);
    }

    /**
     * @dev Transfers ownership of the contract to a new account (`newOwner`).
     * Can only be called by the current owner.
     */
    function transferOwnership(address newOwner) public virtual onlyOwner {
        require(newOwner != address(0), "Ownable: new owner is the zero address");
        emit OwnershipTransferred(_owner, newOwner);
        _owner = newOwner;
    }
}

/*
RemoveLiquidity.sol
Will permit remove TOMB OR TSHARE LPs from Spooky, swapped
them correspondly to SCARAB/GSCARAB and than addLiquidity
on SpiritSwap with FTM.

    https://scarab.finance
*/

contract RemoveLiquidity is Ownable{
    using SafeERC20 for IERC20;

    /* Tomb Finance Pairs */
    IUniswapV2Pair public tombpair;
    IUniswapV2Pair public tsharepair;
    /* Scarab Finance Pairs */
    IPancakePair public scarabpair;
    IPancakePair public gscarabpair;

    /* Tomb Finance Tokens */
    IERC20 public tomb;
    IERC20 public tshare;
    /* Scarab Finance Tokens */
    IERC20 public scarab;
    IERC20 public gscarab;
    /* Fantom Token */
    IERC20 public fantom;

    /* SpookyRouter */
    IUniswapV2Router02 public spookyrouter;
    /* SpiritRouter */
    IPancakeRouter02 public spiritRouter;

    struct User {
        uint256 tombpair;
        uint256 tsharepair;
        uint256 scarabpair;
        uint256 gscarabpair;
        uint256 tomb;
        uint256 tshare;
        uint256 scarab;
        uint256 gscarab;
        uint256 fantom1;
        uint256 fantom2;
    }

    mapping(address => User) public _users;

    constructor(
        address _tomb,
        address _tshare,
        address _scarab,
        address _gscarab,
        address _fantom,
        address _spookyrouter,
        address _spiritRouter,
        address _tombpair,
        address _tsharepair,
        address _scarabpair,
        address _gscarabpair
    ) public {
        tomb = IERC20(_tomb);
        tshare = IERC20(_tshare);
        scarab = IERC20(_scarab);
        gscarab = IERC20(_gscarab);
        fantom = IERC20(_fantom);
        spookyrouter = IUniswapV2Router02(_spookyrouter);
        spiritRouter = IPancakeRouter02(_spiritRouter);
        tombpair = IUniswapV2Pair(_tombpair);
        tsharepair = IUniswapV2Pair(_tsharepair);
        scarabpair = IPancakePair(_scarabpair);
        gscarabpair = IPancakePair(_gscarabpair);
    }


    /*
        2 - REMOVE LIQUIDITY OF SPOOKY
    */

    /* 2.1.1 - INJECT TOMB LP FOR REMOVE */
    function injectTombLP(uint256 amount) external notContract {
        require(tombpair.balanceOf(address(msg.sender)) >= amount ,"You don't have that amount of Tomb LP");
        IERC20(address(tombpair)).safeTransferFrom(msg.sender, address(this), amount);
        _users[address(msg.sender)].tombpair = _users[address(msg.sender)].tombpair + amount;
    }
    /* 2.1.2 - INJECT TSHARE LP FOR REMOVE */
    function injectTshareLP(uint256 amount) external notContract {
        require(tsharepair.balanceOf(address(msg.sender)) >= amount ,"You don't have that amount of Tshare LP");
        IERC20(address(tsharepair)).safeTransferFrom(msg.sender, address(this), amount);
        _users[address(msg.sender)].tsharepair = _users[address(msg.sender)].tsharepair + amount;
    }
    /* 2.2 - REMOVE TOMB LIQUIDITY OF SPOOKY */
    function removeLiquidityTomb() external notContract returns (uint256 TOMB, uint256 FTM){
        require(_users[address(msg.sender)].tombpair > 0, "You don't have Tomb LPs");
        uint256 liquidity = _users[address(msg.sender)].tombpair;
        tombpair.approve(address(spookyrouter), liquidity);
        (TOMB, FTM) = spookyrouter.removeLiquidity(
            address(tomb),
            address(fantom),
            liquidity,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            address(msg.sender),
            addMinsTimestamp(block.timestamp, 30)
        );
         _users[address(msg.sender)].tombpair = 0;
    }

    /* 2.2 - REMOVE TSHARE LIQUIDITY OF SPOOKY */
    function removeLiquidityTshare() external notContract returns (uint256 TSHARE, uint256 FTM){
        require(_users[address(msg.sender)].tsharepair > 0, "You don't have TShare LPs");
        uint256 liquidity = _users[address(msg.sender)].tsharepair;
        tombpair.approve(address(spookyrouter), liquidity);
        (TSHARE, FTM) = spookyrouter.removeLiquidity(
            address(tshare),
            address(fantom),
            liquidity,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            address(msg.sender),
            addMinsTimestamp(block.timestamp, 30)
        );
         _users[address(msg.sender)].tsharepair = 0;
    }
    
    /*
        3 - SWAP TOMB/TSHARE FOR SCARAB/GSCARAB 
    */

    /* 3.1.1 - INJECT TOMB FOR SWAP */
    function injectTomb(uint256 amount) external notContract {
        require(tomb.balanceOf(address(msg.sender)) >= amount,"You don't have that amount of Tomb");
        tomb.safeTransferFrom(msg.sender, address(this), amount);
        _users[address(msg.sender)].tomb = _users[address(msg.sender)].tomb + amount;
    }
    /* 3.2.1 - INJECT TSHARE FOR SWAP */
    function injectTshare(uint256 amount) external notContract {
        require(tshare.balanceOf(address(msg.sender)) >= amount,"You don't have that amount of Tshare");
        tshare.safeTransferFrom(msg.sender, address(this), amount);
        _users[address(msg.sender)].tshare = _users[address(msg.sender)].tshare + amount;
    }
    /* 3.2.1 - SWAP TOMB FOR SCARAB */
    function swapTombforScarab(address[] calldata tokens) external notContract returns (uint[] memory amounts){
        require(_users[address(msg.sender)].tomb > 0 , "You must inject Tomb on this contract");
        tomb.approve(address(spiritRouter), _users[address(msg.sender)].tomb);
        (amounts) = spiritRouter.swapExactTokensForTokens(
            _users[address(msg.sender)].tomb,
            0,
            tokens,
            address(msg.sender),
            addMinsTimestamp(block.timestamp, 30)
        );
        _users[address(msg.sender)].tomb = 0;
    }
    /* 3.2.2 - SWAP TSHARE FOR GSCARAB */
    function swapTshareforGScarab(address[] calldata tokens) external notContract returns (uint[] memory amounts){
        require(_users[address(msg.sender)].tshare > 0, "You must inject Tshare on this contract");
        tshare.approve(address(spiritRouter), _users[address(msg.sender)].tshare);
        (amounts) = spiritRouter.swapExactTokensForTokens(
            _users[address(msg.sender)].tshare,
            0,
            tokens,
            address(msg.sender),
            addMinsTimestamp(block.timestamp, 30)
        );
        _users[address(msg.sender)].tshare = 0;
    }

    /*
        4 - ADD LIQUIDITY ON SPIRIT 
    */

    /* 4.1.1 - INJECT SCARAB FOR ADD LIQUIDITY */
    function injectScarabAndFantom(uint256 amountSCARAB, uint256 amountFTM) external notContract{
        require(scarab.balanceOf(address(msg.sender)) >= amountSCARAB,"You don't have that amount of Scarab");
        require(fantom.balanceOf(address(msg.sender)) >= amountFTM,"You don't have that amount of Fantom");
        scarab.safeTransferFrom(msg.sender, address(this), amountSCARAB);
        fantom.safeTransferFrom(msg.sender, address(this), amountFTM);
        _users[address(msg.sender)].scarab = _users[address(msg.sender)].scarab + amountSCARAB;
        _users[address(msg.sender)].fantom1 = _users[address(msg.sender)].fantom1 + amountFTM;
    }
    /* 4.1.2 - INJECT GSCARAB FOR ADD LIQUIDITY */
    function injectGScarabAndFantom(uint256 amountGSCARAB, uint256 amountFTM) external notContract{
        require(gscarab.balanceOf(address(msg.sender)) >= amountGSCARAB,"You don't have that amount of GScarab");
        require(fantom.balanceOf(address(msg.sender)) >= amountFTM,"You don't have that amount of Fantom");
        gscarab.safeTransferFrom(msg.sender, address(this), amountGSCARAB);
        fantom.safeTransferFrom(msg.sender, address(this), amountFTM);
        _users[address(msg.sender)].gscarab = _users[address(msg.sender)].gscarab + amountGSCARAB;
        _users[address(msg.sender)].fantom2 = _users[address(msg.sender)].fantom2 + amountFTM;

    }
    /* 4.2.1 - ADD SCARAB LIQUIDITY ON SPIRIT */
    function addLiquidityScarab() external notContract returns (uint256 sentSCARAB, uint256 sentFTM, uint256 liquidity){
        require( _users[address(msg.sender)].scarab != 0, "You must inject Scarab on this contract");
        require( _users[address(msg.sender)].fantom1 != 0, "You must inject Fantom on this contract");
        scarab.approve(address(spiritRouter), _users[address(msg.sender)].scarab);
        fantom.approve(address(spiritRouter), _users[address(msg.sender)].fantom1);
        (sentSCARAB, sentFTM, liquidity) = spiritRouter.addLiquidity(
            address(scarab),
            address(fantom),
            _users[address(msg.sender)].scarab,
            _users[address(msg.sender)].fantom1,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            address(msg.sender),
            block.timestamp
        );
        _users[address(msg.sender)].scarabpair = _users[address(msg.sender)].scarabpair + liquidity;
        _users[address(msg.sender)].scarab = 0;
        _users[address(msg.sender)].fantom1 = 0;
    }
    /* 4.2.2 - ADD SCARAB LIQUIDITY ON SPIRIT */
    function addLiquidityGscarab() external notContract returns (uint256 sentGSCARAB, uint256 sentFTM, uint256 liquidity){
        require( _users[address(msg.sender)].gscarab != 0, "You must inject GScarab on this contract");
        require( _users[address(msg.sender)].fantom2 != 0, "You must inject Fantom on this contract");
        gscarab.approve(address(spiritRouter), _users[address(msg.sender)].gscarab);
        fantom.approve(address(spiritRouter), _users[address(msg.sender)].fantom2);
        (sentGSCARAB, sentFTM, liquidity) = spiritRouter.addLiquidity(
            address(gscarab),
            address(fantom),
            _users[address(msg.sender)].gscarab,
            _users[address(msg.sender)].fantom2,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            address(msg.sender),
            block.timestamp
        );
        _users[address(msg.sender)].scarabpair = _users[address(msg.sender)].scarabpair + liquidity;
        _users[address(msg.sender)].gscarab = 0;
        _users[address(msg.sender)].fantom2 = 0;
    }

    /*
        5 - HELP AND SECURITY FUNCTIONS
    */

    modifier notContract()
    {
        _notContract();
        _;
    }
	
	function _notContract() internal view
	{
		require(!checkIsContract(msg.sender), "contract");
        require(msg.sender == tx.origin, "proxy contract");	
	}	
    
    function checkIsContract(address _address) internal view returns(bool)
    {
        uint256 size;
        assembly
        {
            size := extcodesize(_address)
        }
        return size > 0;
    }
    
    function addMinsTimestamp(uint256 timestamp, uint256 mins) internal pure returns (uint256) {
        return timestamp + (mins * 60);
    }
   
    /* SCARAB FINANCE LOST FUNCTIONS */
    function withdrawLostScarab(uint256 amount) external payable onlyOwner notContract{
        scarab.transfer(msg.sender, amount);
    }

    function withdrawLostGScarab(uint256 amount) external payable onlyOwner notContract{
        gscarab.transfer(msg.sender, amount);
    }

    function withdrawLostLPsScarab(uint256 amount) external payable onlyOwner notContract{
        scarabpair.transfer(msg.sender, amount);
    }

    function withdrawLostLPsGScarab(uint256 amount) external payable onlyOwner notContract{
        tomb.transfer(msg.sender, amount);
    }

    /* TOMB FINANCE LOST FUNCTIONS */
    function withdrawLostTomb(uint256 amount) external payable onlyOwner notContract{
        tomb.transfer(msg.sender, amount);
    }

    function withdrawLostTshare(uint256 amount) external payable onlyOwner notContract{
        tshare.transfer(msg.sender, amount);
    }

    function withdrawLostLPsTomb(uint256 amount) external payable onlyOwner notContract{
        tombpair.transfer(msg.sender, amount);
    }

    function withdrawLostLPsTshare(uint256 amount) external payable onlyOwner notContract{
        tsharepair.transfer(msg.sender, amount);
    }

}