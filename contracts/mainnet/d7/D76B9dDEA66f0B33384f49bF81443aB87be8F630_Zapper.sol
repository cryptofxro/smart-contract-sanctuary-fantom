contract TokenSwapper is MultiOwnable {
    using SafeERC20 for IERC20;

    struct Entry {
        IUniswapRouterETH router;
        address[] path;
    }

    mapping(IERC20 /* input token */ => mapping(IERC20 /* output token */ => Entry)) public pathOverrides;

    IUniswapRouterETH public defaultRouter;
    IERC20            public defaultIntermediaryToken;

    constructor(IUniswapRouterETH _defaultRouter, IERC20 _defaultIntermediaryToken) {
        defaultRouter = _defaultRouter;
        defaultIntermediaryToken = _defaultIntermediaryToken;
    }

    function setDefaults(IUniswapRouterETH _defaultRouter, IERC20 _defaultIntermediaryToken) public onlyOwners {
        defaultRouter = _defaultRouter;
        defaultIntermediaryToken = _defaultIntermediaryToken;
    }

    function upsertPathOverride(
        IERC20 inputToken,
        IERC20 outputToken,
        IUniswapRouterETH router,
        address[] calldata path
    ) public onlyOwners {
        require(inputToken  != IERC20(address(0)), "!inputToken");
        require(outputToken != IERC20(address(0)), "!outputToken");
        require(path.length > 1, "!path");
        pathOverrides[inputToken][outputToken] = Entry({ router: router, path: path });
    }

    function removePathOverride(IERC20 inputToken, IERC20 outputToken) public onlyOwners {
        delete pathOverrides[inputToken][outputToken];
    }

    function getRouterAndPath(IERC20 inputToken, IERC20 outputToken) public view returns (IUniswapRouterETH router, address[] memory path) {
        return (getRouter(inputToken, outputToken), getPath(inputToken, outputToken));
    }

    function getRouter(IERC20 inputToken, IERC20 outputToken) public view returns (IUniswapRouterETH router) {
        return _getRouter(pathOverrides[inputToken][outputToken]);
    }

    function _getRouter(Entry storage entry) internal view returns (IUniswapRouterETH router) {
        return entry.router == IUniswapRouterETH(address(0)) ? defaultRouter : entry.router;
    }

    function getPath(IERC20 inputToken, IERC20 outputToken) public view returns (address[] memory path) {
        return _getPath(pathOverrides[inputToken][outputToken], inputToken, outputToken);
    }

    function _getPath(Entry storage entry, IERC20 inputToken, IERC20 outputToken) internal view returns (address[] memory path) {
        if (entry.path.length > 0) {
            path = entry.path;
        } else {
            if (inputToken == defaultIntermediaryToken || outputToken == defaultIntermediaryToken) {
                path = new address[](2);
                path[0] = address(inputToken);
                path[1] = address(outputToken);
            } else {
                path = new address[](3);
                path[0] = address(inputToken);
                path[1] = address(defaultIntermediaryToken);
                path[2] = address(outputToken);
            }
        }
        return path;
    }

    function swap(IERC20 inputToken, uint256 amount, IERC20 outputToken, address recipient)
        public
        returns (uint256 amountOutput)
    {
        Entry storage entry = pathOverrides[inputToken][outputToken];
        return swapWithRouterAndPath(_getRouter(entry), _getPath(entry, inputToken, outputToken), amount, recipient);
    }

    function swapAll(TokenAndAmount[] memory inputTokens, IERC20 outputToken, address recipient)
        public
        returns (uint256 amountOutput)
    {
        for (uint8 i = 0; i < inputTokens.length; i++) {
            if (inputTokens[i].amount == 0) {
                continue;
            }
            if (inputTokens[i].token != outputToken) {
                amountOutput += swap(inputTokens[i].token, inputTokens[i].amount, outputToken, recipient);
            } else {
                amountOutput += inputTokens[i].amount;
            }
        }
        return amountOutput;
    }

    function swapWithRouterAndPath(
        IUniswapRouterETH router,
        address[] memory path,
        uint256 inputTokenAmount,
        address recipient
    )
        public
        returns (uint256 amountOutput)
    {
        require(inputTokenAmount > 0, "!amount");

        IERC20(path[0]).safeTransferFrom(msg.sender, address(this), inputTokenAmount);
        IERC20(path[0]).safeApprove(address(router), inputTokenAmount);

        uint256[] memory amounts = router.swapExactTokensForTokens(inputTokenAmount, 0, path, recipient, type(uint256).max);

        IERC20(path[0]).safeApprove(address(router), 0);

        return amounts[amounts.length - 1];
    }
}
library FullMath {
    /// @notice Calculates floor(a×b÷denominator) with full precision. Throws if result overflows a uint256 or denominator == 0
    /// @param a The multiplicand
    /// @param b The multiplier
    /// @param denominator The divisor
    /// @return result The 256-bit result
    /// @dev Credit to Remco Bloemen under MIT license https://xn--2-umb.com/21/muldiv
    function mulDiv(
        uint256 a,
        uint256 b,
        uint256 denominator
    ) internal pure returns (uint256 result) {
        // 512-bit multiply [prod1 prod0] = a * b
        // Compute the product mod 2**256 and mod 2**256 - 1
        // then use the Chinese Remainder Theorem to reconstruct
        // the 512 bit result. The result is stored in two 256
        // variables such that product = prod1 * 2**256 + prod0
        uint256 prod0; // Least significant 256 bits of the product
        uint256 prod1; // Most significant 256 bits of the product
        assembly {
            let mm := mulmod(a, b, not(0))
            prod0 := mul(a, b)
            prod1 := sub(sub(mm, prod0), lt(mm, prod0))
        }

        // Handle non-overflow cases, 256 by 256 division
        if (prod1 == 0) {
            require(denominator > 0);
            assembly {
                result := div(prod0, denominator)
            }
            return result;
        }

        // Make sure the result is less than 2**256.
        // Also prevents denominator == 0
        require(denominator > prod1);

        ///////////////////////////////////////////////
        // 512 by 256 division.
        ///////////////////////////////////////////////

        // Make division exact by subtracting the remainder from [prod1 prod0]
        // Compute remainder using mulmod
        uint256 remainder;
        assembly {
            remainder := mulmod(a, b, denominator)
        }
        // Subtract 256 bit number from 512 bit number
        assembly {
            prod1 := sub(prod1, gt(remainder, prod0))
            prod0 := sub(prod0, remainder)
        }

        // Factor powers of two out of denominator
        // Compute largest power of two divisor of denominator.
        // Always >= 1.
        uint256 twos = (type(uint256).max - denominator + 1) & denominator;
        // Divide denominator by power of two
        assembly {
            denominator := div(denominator, twos)
        }

        // Divide [prod1 prod0] by the factors of two
        assembly {
            prod0 := div(prod0, twos)
        }
        // Shift in bits from prod1 into prod0. For this we need
        // to flip `twos` such that it is 2**256 / twos.
        // If twos is zero, then it becomes one
        assembly {
            twos := add(div(sub(0, twos), twos), 1)
        }
        prod0 |= prod1 * twos;

        // Invert denominator mod 2**256
        // Now that denominator is an odd number, it has an inverse
        // modulo 2**256 such that denominator * inv = 1 mod 2**256.
        // Compute the inverse by starting with a seed that is correct
        // correct for four bits. That is, denominator * inv = 1 mod 2**4
        uint256 inv = (3 * denominator) ^ 2;
        // Now use Newton-Raphson iteration to improve the precision.
        // Thanks to Hensel's lifting lemma, this also works in modular
        // arithmetic, doubling the correct bits in each step.
        inv *= 2 - denominator * inv; // inverse mod 2**8
        inv *= 2 - denominator * inv; // inverse mod 2**16
        inv *= 2 - denominator * inv; // inverse mod 2**32
        inv *= 2 - denominator * inv; // inverse mod 2**64
        inv *= 2 - denominator * inv; // inverse mod 2**128
        inv *= 2 - denominator * inv; // inverse mod 2**256

        // Because the division is now exact we can divide by multiplying
        // with the modular inverse of denominator. This will give us the
        // correct result modulo 2**256. Since the precoditions guarantee
        // that the outcome is less than 2**256, this is the final result.
        // We don't need to compute the high bits of the result and prod1
        // is no longer required.
        result = prod0 * inv;
        return result;
    }

    /// @notice Calculates ceil(a×b÷denominator) with full precision. Throws if result overflows a uint256 or denominator == 0
    /// @param a The multiplicand
    /// @param b The multiplier
    /// @param denominator The divisor
    /// @return result The 256-bit result
    function mulDivRoundingUp(
        uint256 a,
        uint256 b,
        uint256 denominator
    ) internal pure returns (uint256 result) {
        result = mulDiv(a, b, denominator);
        if (mulmod(a, b, denominator) > 0) {
            require(result < type(uint256).max);
            result++;
        }
    }
}
contract FeeManager {
    using SafeERC20 for IERC20;

    struct FeeRecipient {
        bool    exists;
        string  tag;
        address addr;
        uint256 feePercentTimes100;
        bytes4  callbackSelector;
    }

    uint256 constant public MAXFEE = 10000;

    TokenSwapper public tokenSwapper;
    IERC20       public feeToken;

    mapping(address => FeeRecipient) public feeRecipients;
    address[]                        public feeRecipientAddresses;
    uint256                          public msgSenderFeePercentTimes100;
    uint256                          public totalFeePercentTimes100;

    constructor(IERC20 _feeToken, TokenSwapper _tokenSwapper) {
        feeToken     = _feeToken;
        tokenSwapper = _tokenSwapper;
    }

    function getFeeRecipient(address addr) public view returns (FeeRecipient memory) {
        return feeRecipients[addr];
    }

    function allFeeRecipientAddresses() public view returns (address[] memory) {
        return feeRecipientAddresses;
    }

    error FeesTooHigh(uint256 proposed);

    function _upsertFeeRecipient(address addr, string calldata tag, uint256 feePercentTimes100, bytes4 callbackSelector) internal virtual {
        if (!feeRecipients[addr].exists) {
            feeRecipientAddresses.push(addr);
            totalFeePercentTimes100 += feePercentTimes100;
        } else {
            totalFeePercentTimes100 = (totalFeePercentTimes100 - feeRecipients[addr].feePercentTimes100) + feePercentTimes100;
        }
        if (totalFeePercentTimes100 >= MAXFEE) revert FeesTooHigh(totalFeePercentTimes100);
        feeRecipients[addr] = FeeRecipient({
            exists: true,
            tag: tag,
            addr: addr,
            feePercentTimes100: feePercentTimes100,
            callbackSelector: callbackSelector
        });
    }

    function _removeFeeRecipient(address addr) internal virtual {
        for (uint8 i = 0; i < feeRecipientAddresses.length; i++) {
            if (feeRecipientAddresses[i] == addr) {
                if (i == feeRecipientAddresses.length - 1) {
                    feeRecipientAddresses.pop();
                } else {
                    feeRecipientAddresses[i] = feeRecipientAddresses[feeRecipientAddresses.length - 1];
                    feeRecipientAddresses.pop();
                }
                break;
            }
        }
        totalFeePercentTimes100 -= feeRecipients[addr].feePercentTimes100;
        delete feeRecipients[addr];
    }

    function _setContracts(IERC20 _feeToken, TokenSwapper _tokenSwapper) internal virtual {
        feeToken     = _feeToken;
        tokenSwapper = _tokenSwapper;
    }

    function _setMsgSenderFeePercentTimes100(uint256 _msgSenderFeePercentTimes100) internal virtual {
        totalFeePercentTimes100 = (totalFeePercentTimes100 - msgSenderFeePercentTimes100) + _msgSenderFeePercentTimes100;
        if (totalFeePercentTimes100 >= MAXFEE) revert FeesTooHigh(totalFeePercentTimes100);

        msgSenderFeePercentTimes100 = _msgSenderFeePercentTimes100;
    }

    function _calculateFees(TokenAndAmount[] memory inputTokens)
        internal
        virtual
        view
        returns (TokenAndAmount[] memory fees, TokenAndAmount[] memory remaining)
    {
        fees      = new TokenAndAmount[](inputTokens.length);
        remaining = new TokenAndAmount[](inputTokens.length);

        for (uint8 i = 0; i < inputTokens.length; i++) {
            uint256 feeAmount = (inputTokens[i].amount * totalFeePercentTimes100) / MAXFEE;

            fees[i] = TokenAndAmount({
                token:  inputTokens[i].token,
                amount: feeAmount
            });

            remaining[i] = TokenAndAmount({
                token:  inputTokens[i].token,
                amount: inputTokens[i].amount - feeAmount
            });
        }
        return (fees, remaining);
    }

    function _convertToFeeTokensAndDistribute(IERC20[] memory tokensToConvert, bool disregardMsgSender)
        internal
        virtual
        returns (uint256 totalFeeTokens, TokenAndAmount[] memory remaining)
    {
        TokenAndAmount[] memory tokensAndAmounts = new TokenAndAmount[](tokensToConvert.length);

        for (uint8 i = 0; i < tokensToConvert.length; i++) {
            tokensAndAmounts[i] = TokenAndAmount({
                token: tokensToConvert[i],
                amount: tokensToConvert[i].balanceOf(address(this))
            });
        }
        return _convertToFeeTokensAndDistribute(tokensAndAmounts, disregardMsgSender);
    }

    function _convertToFeeTokensAndDistribute(TokenAndAmount[] memory tokensToConvert, bool disregardMsgSender)
        internal
        virtual
        returns (uint256 totalFeeTokens, TokenAndAmount[] memory remaining)
    {
        uint256 _totalFeePercentTimes100 = disregardMsgSender ? totalFeePercentTimes100 - msgSenderFeePercentTimes100 : totalFeePercentTimes100;

        TokenAndAmount[] memory feeAmounts;
        (feeAmounts, remaining) = _calculateFees(tokensToConvert);

        for (uint8 i = 0; i < feeAmounts.length; i++) {
            if (tokensToConvert[i].token == feeToken) {
                continue;
            }
            feeAmounts[i].token.safeApprove(address(tokenSwapper), 0);
            feeAmounts[i].token.safeApprove(address(tokenSwapper), feeAmounts[i].amount);
        }

        totalFeeTokens = tokenSwapper.swapAll(feeAmounts, feeToken, address(this));
        if (totalFeeTokens > 0) {
            for (uint8 i = 0; i < feeRecipientAddresses.length; i++) {
                FeeRecipient storage recipient = feeRecipients[feeRecipientAddresses[i]];
                uint256 amount = (totalFeeTokens * recipient.feePercentTimes100) / _totalFeePercentTimes100;
                if (amount > 0) {
                    feeToken.safeTransfer(recipient.addr, amount);
                    if (recipient.callbackSelector != bytes4(0)) {
                        address(recipient.addr).call(abi.encodeWithSelector(recipient.callbackSelector, amount));
                    }
                }
            }

            if (!disregardMsgSender && msgSenderFeePercentTimes100 > 0) {
                feeToken.safeTransfer(msg.sender, (totalFeeTokens * msgSenderFeePercentTimes100) / _totalFeePercentTimes100);
            }
        }
        return (totalFeeTokens, remaining);
    }
}

abstract contract PluginBase is MultiOwnable {
    Zapper       public zapper;
    TokenSwapper public tokenSwapper;

    function setContracts(Zapper _zapper, TokenSwapper _tokenSwapper) public onlyOwners {
        zapper = _zapper;
        tokenSwapper = _tokenSwapper;
    }

    modifier onlyZapper {
        require(msg.sender == address(zapper), "!zapper");
        _;
    }

    function pluginName() public virtual view returns (string memory);

    function exitPosition(
        address owner,
        Signature memory sig,
        IERC20 inputToken,
        uint256 inputTokenAmount
    ) public virtual returns (TokenAndAmount[] memory outputTokens);

    function enterPosition(
        address owner,
        Signature memory sig,
        TokenAndAmount[] memory inputTokens,
        IERC20 desiredToken
    ) public virtual returns (uint256 outputAmount);
}

contract Zapper is MultiOwnable, Pausable, ReentrancyGuard, FeeManager {
    using SafeERC20 for IERC20;

    constructor(IERC20 _feeToken, TokenSwapper _tokenSwapper) FeeManager(_feeToken, _tokenSwapper) {}

    event Zap(address indexed owner, IERC20 indexed inputToken, IERC20 indexed outputToken, uint256 inputTokenAmount, uint256 outputTokenAmount, uint256 minOutputTokenAmount);

    function zap(
        IERC20            inputToken,
        uint256           inputTokenAmount,
        PluginBase        inputTokenPlugin,
        IERC20            outputToken,
        uint256           minOutputTokenAmount,
        PluginBase        outputTokenPlugin,
        address           owner,
        Signature memory  sig
    ) public whenNotPaused nonReentrant returns (uint256 outputTokenAmount) {
        return _zap(inputToken, inputTokenAmount, inputTokenPlugin, outputToken, minOutputTokenAmount, outputTokenPlugin, owner, sig);
    }

    function zapWithPluginNames(
        IERC20           inputToken,
        uint256          inputTokenAmount,
        string memory    inputTokenPlugin,
        IERC20           outputToken,
        string memory    outputTokenPlugin,
        uint256          minOutputTokenAmount,
        address          owner,
        Signature memory sig
    ) public whenNotPaused nonReentrant returns (uint256 outputTokenAmount) {
        PluginBase inputPluginContract = pluginContracts[inputTokenPlugin];
        if (address(inputPluginContract) == address(0)) revert PluginContractDoesNotExist(inputTokenPlugin);

        PluginBase outputPluginContract = pluginContracts[outputTokenPlugin];
        if (address(outputPluginContract) == address(0)) revert PluginContractDoesNotExist(outputTokenPlugin);

        return _zap(inputToken, inputTokenAmount, inputPluginContract, outputToken, minOutputTokenAmount, outputPluginContract, owner, sig);
    }

    function _zap(
        IERC20            inputToken,
        uint256           inputTokenAmount,
        PluginBase        inputTokenPlugin,
        IERC20            outputToken,
        uint256           minOutputTokenAmount,
        PluginBase        outputTokenPlugin,
        address           owner,
        Signature memory  sig
    ) internal returns (uint256 outputTokenAmount) {
        {
        require(inputToken != outputToken, "same addresses");
        require(inputTokenAmount > 0, "!amount");
        require(sig.deadline >= block.timestamp, "!deadline");
        }

        TokenAndAmount memory input = TokenAndAmount({ token: inputToken, amount: inputTokenAmount });

        TokenAndAmount[] memory withdrawnTokensAndAmounts = inputTokenPlugin.exitPosition(owner, sig, input.token, input.amount);

        // Charge fees
        ( , TokenAndAmount[] memory remaining) = FeeManager._convertToFeeTokensAndDistribute(withdrawnTokensAndAmounts, true);

        for (uint8 i = 0; i < withdrawnTokensAndAmounts.length; i++) {
            withdrawnTokensAndAmounts[i].token.safeApprove(address(outputTokenPlugin), remaining[i].amount);
        }
        outputTokenAmount = outputTokenPlugin.enterPosition(owner, sig, remaining, outputToken);

        // Ensure slippage
        require(outputTokenAmount >= minOutputTokenAmount, ">slippage");

        emit Zap(owner, inputToken, outputToken, inputTokenAmount, outputTokenAmount, minOutputTokenAmount);
        return outputTokenAmount;
    }

    //
    // Plugin registry convenience functions
    //
    mapping(string => PluginBase) public pluginContracts;
    string[] public pluginContractNames;

    event PluginContractUpserted(string indexed name, PluginBase indexed pluginContract);
    event PluginContractRemoved(string indexed name, PluginBase indexed pluginContract);
    error PluginContractDoesNotExist(string name);

    function allPluginContractNames() public view returns (string[] memory) {
        return pluginContractNames;
    }

    function upsertPluginContract(PluginBase pluginContract) public onlyOwners {
        string memory name = pluginContract.pluginName();
        if (address(pluginContracts[name]) == address(0)) {
            pluginContractNames.push(name);
        }

        pluginContracts[name] = pluginContract;
        emit PluginContractUpserted(name, pluginContract);
    }

    function removePluginContract(string memory name) public onlyOwners {
        PluginBase pluginContract = pluginContracts[name];
        if (address(pluginContract) == address(0)) revert PluginContractDoesNotExist(name);

        delete pluginContracts[name];

        for (uint8 i = 0; i < pluginContractNames.length; i++) {
            if (keccak256(abi.encodePacked(pluginContractNames[i])) == keccak256(abi.encodePacked(name))) {
                if (i == pluginContractNames.length - 1) {
                    pluginContractNames.pop();
                } else {
                    pluginContractNames[i] = pluginContractNames[pluginContractNames.length - 1];
                    pluginContractNames.pop();
                }
                break;
            }
        }

        emit PluginContractRemoved(name, pluginContract);
    }

    //
    // Admin functions
    //

    function setFeeContracts(IERC20 _feeToken, TokenSwapper _tokenSwapper) external onlyOwners {
        FeeManager._setContracts(_feeToken, _tokenSwapper);
    }

    function upsertFeeRecipient(
        address addr,
        string calldata tag,
        uint256 feePercentTimes100,
        bytes4 callbackSelector
    ) external onlyOwners {
        FeeManager._upsertFeeRecipient(addr, tag, feePercentTimes100, callbackSelector);
    }

    function removeFeeRecipient(address addr) external onlyOwners {
        FeeManager._removeFeeRecipient(addr);
    }

    function withdrawFees() external onlyOwners {
        feeToken.safeTransfer(msg.sender, feeToken.balanceOf(address(this)));
    }

    function inCaseTokensGetStuck(IERC20 token) external onlyOwners {
        uint256 amount = token.balanceOf(address(this));
        token.safeTransfer(msg.sender, amount);
    }
}