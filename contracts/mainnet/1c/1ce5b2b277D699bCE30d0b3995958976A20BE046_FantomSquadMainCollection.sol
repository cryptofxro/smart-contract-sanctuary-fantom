    function takeNormalSnapshot() public onlyRole(DEFAULT_ADMIN_ROLE) {
        normalSnapshot = IERC721Snapshot(SnapshotMaker.clone());
        normalSnapshot.initialize(IERC721Enumerable(fsmp));
        _normalSnapshotCompleted = true;
        emit SnapshotComplete(address(normalSnapshot), fsmp);
    }

    /**
