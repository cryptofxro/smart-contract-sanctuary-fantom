    function takeGoldSnapshot() public onlyRole(DEFAULT_ADMIN_ROLE) {
        goldSnapshot = IERC721Snapshot(SnapshotMaker.clone());
        goldSnapshot.initialize(IERC721Enumerable(fsgmp));
        _goldSnapshotCompleted = true;
        emit SnapshotComplete(address(goldSnapshot), fsgmp);
    }

    /****** AIRDROPS ******/

    /**
     * @dev Airdrops NFTs minted to Fantom Gold Minting Pass holders - this is iterative
     */
    function airdropGold(uint256 _count)
        public
