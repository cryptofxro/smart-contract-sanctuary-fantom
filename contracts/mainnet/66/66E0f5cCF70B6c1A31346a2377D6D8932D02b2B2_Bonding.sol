import "hardhat/console.sol";
import "./Staking.sol";
import "./FundToken.sol";
import "@openzeppelin/contracts/access/Ownable.sol"; 
import "@rari-capital/solmate/src/tokens/ERC20.sol"; 
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Pair.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Factory.sol";

/**
 * Admin can allow some asset type to be received and sent to the treasury in exchange for discounted tokens
 * E.g, USDT is received and discounted bonds are issued for 
 *
 * For multihop swap calculation purposes, the bondable asset needs to have a spookyswap hop with WFTM
 * Price calculation is made with swapExactTokensForTokens() and path [assets, WFTM, INDE]
 * A discount is then applied 
 
 
 * To start with we ony make a WFTM pool and bonds are only taken in FTM
 */
contract Bonding is Ownable {

	// Discount amount in percent
	uint8 public discount = 4;
	
	// Address INDE treasury
	address public treasury;
	
	// Uniswap Factory address
	address public swapFactory = 0x152eE697f2E276fA89E96742e9bB9aB1F2E61bE3; //Spookyswap factory:
	
	// WFTM address
	address public WFTM = 0x21be370D5312f44cB42ce377BC9b8a0cEF1A4C83;
	
	// INDE address
	address public INDE;
	
	mapping ( address => bool ) public isAssetEnabled;
	address [] public bonds;

	Staking public stakingContract;
	
    event Bond(address indexed from, uint256 amount, uint256 shares);
	

	constructor (address INDE_, address treasury_, address stakingContract_) 
		Ownable()
	{
		INDE = INDE_;
		treasury = treasury_;
		stakingContract = Staking(stakingContract_);
		//Need to authorize the staking to spend this contract's INDE tokens
		ERC20(INDE_).approve(stakingContract_, ~uint256(0) );
	}


	/**
	* Bond some assets for some discounted tokens. 
	* Those tokens are locked for 1 week
	* Currently discount is manual
	* 
	*/
	function bond(address asset, uint256 amount) public returns (uint256 shares)
	{
		require ( isAssetEnabled[asset], "CANT_BOND_ASSET" );
		
		// Calculate the value of assets in INDE
		address swapPair = IUniswapV2Factory(swapFactory).getPair(asset, INDE);
		console.log("Swap pair %s", swapPair);
		(uint112 reserve0, uint112 reserve1,) = IUniswapV2Pair(swapPair).getReserves();
		console.log("Reserves %s %s", reserve0, reserve1);
		
		uint valueAssetInINDE = 0;
		if ( IUniswapV2Pair(swapPair).token0() == INDE ){
			valueAssetInINDE = reserve0 * amount / reserve1;
		}
		else  {
			require( IUniswapV2Pair(swapPair).token1() == INDE, "SANITY_CHECK" );
			valueAssetInINDE = reserve1 * amount / reserve0;
		}
		console.log("Value INDE: %s", valueAssetInINDE);
		
		// Transfer --amount-- to the treasury
		ERC20(asset).transferFrom(msg.sender, treasury, amount );
		console.log("Trasfnered asset, amount %s", amount);
		
		// calc discounted INDE and send to staking + a little for stakers
		uint tax = 2 * valueAssetInINDE / 100;
		valueAssetInINDE = (92 + discount) * valueAssetInINDE / 100;
		console.log("Value INDE after discount: %s", valueAssetInINDE);
		
		// mint the INDE then stake if for the user in stakingContract
		FundToken(INDE).mint( address(this), valueAssetInINDE + tax );
		FundToken(INDE).transfer( address(stakingContract), tax );
		console.log("Minted: ", valueAssetInINDE + tax);
		stakingContract.depositBond(valueAssetInINDE, msg.sender);
		console.log("Staked: ", valueAssetInINDE);
		
		// Transfer discounted tokens
		emit Bond(msg.sender, amount, valueAssetInINDE);
		return valueAssetInINDE;
	}


	
	
	///////////// SETTERS /////////
	
	// Enable or disable bonding of a given asset
	function enableAsset (address asset )
		public onlyOwner()
	{
		require ( !isAssetEnabled[asset], "ALREADY_ENABLED");
		isAssetEnabled[asset] = true;
		bonds.push(asset);
		
	}

	function disableAsset ( address asset )
		public onlyOwner()
	{
		require ( isAssetEnabled[asset], "ALREADY_DISABLED");
		isAssetEnabled[asset] = false;
		
		//should remove from bond asset list: loop, if last...
		for (uint k =0; k<bonds.length; k++){
			if ( k + 1 == bonds.length ) bonds.pop();
			else {
				bonds[k] = bonds[bonds.length -1];
				bonds.pop();
			}
		}
	}
	
	function setDiscount(uint8 discount_) public onlyOwner()
	{
		discount = discount_;
	}
	
	function getBondsAssetsLength() public view returns (uint256 bondsNumber){
		return bonds.length;
	}
	
}

