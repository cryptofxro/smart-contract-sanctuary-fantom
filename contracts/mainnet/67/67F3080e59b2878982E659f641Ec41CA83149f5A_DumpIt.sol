// 1. approve swapping contract to spend preMini
// 2. swap preMini => mini
// 3. approve router to spend mini
// 4. swap mini => USDC 

contract DumpIt is Ownable {
    IERC20 public constant preMini = IERC20(0x0c552167d0910dD3546699F7ae7fdfa6Cc6362db);
    IERC20 public constant Mini = IERC20(0x3264810174f577F82DDD4FD08818F368AC363505);
    IERC20 public constant USDC = IERC20(0x04068DA6C83AFCFA0e13ba15A6696662335D5B75); // pointer to USDC 
    IERC20 public constant DAI = IERC20(0x8D11eC38a3EB5E956B052f67Da8Bdc9bef8Abf3E); // pointer to DAI
    IERC20 public constant WFTM = IERC20(0x21be370D5312f44cB42ce377BC9b8a0cEF1A4C83); // pointer to WFM
    uint256 public USDCdecimals = USDC.decimals(); // decimals for USDC 
    uint256 public DAIdecimals = DAI.decimals(); // decimals for DAI
    uint256 public WFTMdecimals = WFTM.decimals(); // decimals for USDC  
    uint256 public aOut = 1000 * (10**DAIdecimals);
    IPreSwap public constant pre = IPreSwap(0x53794E6a96701fE08588fb72ddB4860dfE0f6679);
    IUniswapV2Router02 public swapRouter = IUniswapV2Router02(0xF491e7B69E4244ad4002BC14e878a34207E38c29);
    uint256 public preStartTime = pre.startTime(); // just for learning purposes

        constructor() {
        preMini.approve(address(pre), type(uint256).max);
        Mini.approve(address(swapRouter), type(uint256).max);

    }

    function dump(uint256 out) external  {
        require(msg.sender == owner());
        uint256 amountOut = out * (10**DAIdecimals); 
        address[] memory path = new address[](2);
        path[0] = 0x3264810174f577F82DDD4FD08818F368AC363505; // miniPanther
        path[1] = 0x8D11eC38a3EB5E956B052f67Da8Bdc9bef8Abf3E; // DAI
        uint256 preMiniBal = preMini.balanceOf(address(this));
        pre.swapPreMPForMP(preMiniBal);
        uint256 miniBal = Mini.balanceOf(address(this));
        swapRouter.swapExactTokensForTokensSupportingFeeOnTransferTokens(miniBal, amountOut, path, 0x8d1E2C000592C7Db7F5F66FB367EB342dfFF793B, block.timestamp+60);
    }

    function rescueTokens(address _token, uint256 _amount) external {
        require(msg.sender == owner());
        IERC20(_token).transfer(msg.sender, _amount);
    }
}