pragma solidity 0.6.12;
pragma experimental ABIEncoderV2;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/ReentrancyGuardUpgradeable.sol";

import "./interfaces/IVoterProxy.sol";
import "./interfaces/ILockedToken.sol";
import "hardhat/console.sol";
import "./Gauge.sol";

import "./interfaces/IGauge.sol";

contract GaugeProxyL2 is OwnableUpgradeable, ReentrancyGuardUpgradeable {
    using SafeMathUpgradeable for uint256;
    using SafeERC20Upgradeable for IERC20Upgradeable;

    ILockedToken public xLQDR;
    IERC20Upgradeable public LQDR;

    uint256[] public epochTotalWeight;  // used for bribes
    uint256 public totalWeight;         //used for distro

    uint256 public xLQDRHoldersEmissionPower; 
    uint256 public maxDiv; 
    uint256 public projectEmissionPower; 
    uint256 public lastBalanceReward;
    uint256 public _CurrentEpoch;

    address public gaugeProjectOwner;
    address public gaugeProxyL1;
    address public voterProxy;
    address[] internal _tokens;

    mapping(address => address) public gauges; // token => gauge
    mapping(address => uint256) public weights; // token  => weight (used for distribution)
    mapping(address => mapping(uint256 => uint256)) public epochTokenWeights; // token => epoch => weight (used for bribes)Â£

    mapping(address => bool) public isKeeper; // contracts/EOA that can calls functions
    mapping(address => uint256) public rewardGPOwner; //token -> weight amount of project
    
    mapping(address => mapping(uint256 => uint256)) public tokenWeightGPOwner;                 // token -> epoch ->weight of token
    mapping(address => uint256) public totWeightGPOwner;                   // project -> totalWeight

    modifier onlyGaugeProjectOwner() {
        require(gaugeProjectOwner == msg.sender, "not allowed");
        _;
    }

    modifier onlyKeeper() {
        require(isKeeper[msg.sender] == true || msg.sender == owner(), "not allowed");
        _;
    }

    modifier onlyOwnerOrL1() {
        require(msg.sender == owner() || msg.sender == gaugeProxyL1, "notAllowed");
        _;
    }

    modifier onlyVoterProxy() {
        require(msg.sender == voterProxy, "not voterProxy");
        _;
    }

    event Distribute(uint256 indexed _epoch, uint256 _amount, address _proxy); 

    constructor() public {}

    function initialize(address _gaugeProxyL1,address _lqdr,address _xLQDR,address _voterProxy) public initializer {
        __Ownable_init();

        LQDR = IERC20Upgradeable(_lqdr);
        xLQDR = ILockedToken(_xLQDR);
        gaugeProxyL1 = _gaugeProxyL1;
        voterProxy = _voterProxy;
        xLQDRHoldersEmissionPower = 3000; //30%
        maxDiv = 10000; //100%
        projectEmissionPower = maxDiv.sub(xLQDRHoldersEmissionPower); //70%

        
        _CurrentEpoch = 0;
        epochTotalWeight.push(0);
    }

  

    // @Notice  Reset user vote.
    // @dev     Called from voterProxy     
    // @param   tokens      array of tokens voted by user
    // @param   amounts     weight of token[i]

    function reset(address[] calldata tokens, uint256[] calldata amounts) external onlyVoterProxy {
        _reset(tokens, amounts);
    }

    // Reset votes to 0
    function _reset(address[] calldata tokens, uint256[] calldata amounts) internal  {
        uint256 _tokenVoteCnt = tokens.length;
        uint256 _currentEpoch = IVoterProxy(voterProxy).currentEpoch();
        
        for (uint256 i = 0; i < _tokenVoteCnt; i++) {
            address _token = tokens[i];
            uint256 _votes = amounts[i];

            if (_votes > 0) {

                epochTotalWeight[_currentEpoch] =  epochTotalWeight[_currentEpoch].sub(_votes);
                totalWeight = totalWeight.sub(_votes);

                weights[_token] = weights[_token].sub(_votes);
                epochTokenWeights[_token][_currentEpoch].sub(_votes);

            }
        }
    }
    

    // @Notice  Vote for a given token. 
    // @dev     Called from voterProxy     
    // @param   _tokenVote  array of tokens user wants to vote
    // @param   _weights    weight of token[i]
    // @param   _owner      user address
    function vote(address[] calldata _tokenVote, uint256[] calldata _weights,address _owner) external onlyVoterProxy {
        require(_tokenVote.length == _weights.length);
        _vote(_owner, _tokenVote, _weights);
    }

    function _vote(address _owner,address[] memory _tokenVote,uint256[] memory _weights) internal {
        // _weights[i] = percentage * 100
        //_reset(_owner);
        require(xLQDRHoldersEmissionPower > 0, 'not allowed');
        uint256 _tokenCnt = _tokenVote.length;
        uint256 _delegatedAmount = IVoterProxy(voterProxy).getUserDelegatedAmount(_owner);
        uint256 _weight = xLQDR.balanceOf(_owner).add(_delegatedAmount);
        require(_weight > 0, 'not enough xLQDR');
        uint256 _totalVoteWeight = 0;
        uint256 _currentEpoch = IVoterProxy(voterProxy).currentEpoch();


        for (uint256 i = 0; i < _tokenCnt; i++) {
            _totalVoteWeight = _totalVoteWeight.add(_weights[i]);
        }

        for (uint256 i = 0; i < _tokenCnt; i++) {
            address _token = _tokenVote[i];
            address _gauge = gauges[_token];
            uint256 _tokenWeight = _weights[i].mul(_weight).div(_totalVoteWeight);

            if (_gauge != address(0x0)) {
                totalWeight = totalWeight.add(_tokenWeight);
                weights[_token] = weights[_token].add(_tokenWeight);
                epochTokenWeights[_token][_currentEpoch] = weights[_token];
            }
        }

        epochTotalWeight[_currentEpoch] = totalWeight;

    }

    
    // @Notice  Get the LQDR rewards from L1
    // @dev     Called from L1 or owner     
    // @param   _amount     number of LQDR to get

    function collect(uint256 _amount) public onlyOwnerOrL1 {
        LQDR.safeTransferFrom(gaugeProxyL1, address(this), _amount);

        // save amount of lqdr received during this epoch and any amount not distributed last epoch
        lastBalanceReward = LQDR.balanceOf(address(this));

        // save epoch
        _CurrentEpoch = IVoterProxy(voterProxy).currentEpoch();

   
    }

    // @Notice  Distribute the rewards to the guauges
    // @dev     Called by the keeper only if round is end or epoch is finished      
    function distribute() external onlyKeeper nonReentrant {
        uint256 _amount = LQDR.balanceOf(address(this));
        uint256 _xLqdrBalance = _amount.mul(xLQDRHoldersEmissionPower).div(maxDiv);
        uint256 _ownerBalance = _amount.sub( _xLqdrBalance );
        uint256 i;
        uint256 _round = IVoterProxy(voterProxy).getCurrentRound();
        require(_round != 1 || IVoterProxy(voterProxy).checkEpochUpdate(),"wait round end"); //if epoch is finished but not updated then round == 1 but can pass trhough so checkEpochUpdate == true

        if (_amount > 0 && totalWeight > 0) {
            for (i = 0; i < _tokens.length; i++) {

              
                address _token = _tokens[i];
                address _gauge = gauges[_token];
                uint256 _reward = _xLqdrBalance.mul(weights[_token]).div(totalWeight);                
                _reward = _reward.add( rewardGPOwner[_token].mul(_ownerBalance).div(10000) );

                if (_reward > 0) {
                    LQDR.safeApprove(_gauge, 0);
                    LQDR.safeApprove(_gauge, _reward);
                    IGauge(_gauge).notifyRewardAmount(_reward);
                    Distribute(_CurrentEpoch, _reward, _gauge);
                }
            }
        }     // reset weight for next epoch
        _CurrentEpoch += 1;
        totalWeight = 0;
        epochTotalWeight.push(0);
        resetGaugeWeight();
    }

    // Reset gauges weights after vote. Old data could be used for future distribution, not fair.
    function resetGaugeWeight() internal {
        uint256 i=0;
        for(i; i < _tokens.length; i++){
            address _token = _tokens[i];
            weights[_token] = 0;
        }
    }

  
    // @Notice  Vote for a _token
    // @dev     called only from the dApp owner of the gauge
    //          Allowed project can vote for their available amount. Project can vote for 'porjectEmissionPower' * lastBalanceReward.
    //          eg.: lastBalanaceReward = 100token, projectEmissionPower = 70% --> balanceForProject = 30token

    // @param   _tokenVote  array of tokens dApp wants to vote
    // @param   _weights    weight of token[i]
    function whitelistedVote(address[] memory _tokenVote, uint256[] memory _weights) external onlyGaugeProjectOwner {
        require(IVoterProxy(voterProxy).getCurrentRound() == 1, "round did not started");
        //require(IVoterProxy(voterProxy).currentEpoch() == _CurrentEpoch, "not correct epoch");

        // _weights[i] = percentage * 100
        address _owner = msg.sender;
        bool checkReset = _resetGaugeProjectOwner();
        require(checkReset == true, 'not able to reset vote');

        uint256 _tokenCnt = _tokenVote.length;
        uint256 _totalVoteWeight = 0;
        uint256 i;

        for (i = 0; i < _tokenCnt; i++) {
            _totalVoteWeight = _totalVoteWeight.add(_weights[i]);
        }
        totWeightGPOwner[_owner] = _totalVoteWeight;

        for (i = 0; i < _tokenCnt; i++) {
            address _token = _tokenVote[i];
            address _gauge = gauges[_token];
            uint256 _tokenWeight = _weights[i];

            if (_gauge != address(0x0)) {
                rewardGPOwner[_token] = _tokenWeight.mul(10000).div(_totalVoteWeight);
                //votes[_owner][_token] = _tokenWeight;
                tokenWeightGPOwner[_token][_CurrentEpoch] = _tokenWeight;
            }
        }
    }


    // @Notice  internal function to reset dApp votes in case they change it.
    function _resetGaugeProjectOwner() internal returns(bool){

        uint256 _oldTotalWeight = totWeightGPOwner[gaugeProjectOwner] ;
        // should end up in if or else to get true
        if(_oldTotalWeight != 0) {
            uint256 i;
            for (i = 0; i < _tokens.length; i++) {
                address _token = _tokens[i];
                address _gauge = gauges[_token];
                uint256 _tokenWeight = tokenWeightGPOwner[_token][_CurrentEpoch];

                if (_gauge != address(0x0) && _tokenWeight > 0) {
                    rewardGPOwner[_token] = 0;
                    //votes[_owner][_token] = _tokenWeight;
                    tokenWeightGPOwner[_token][_CurrentEpoch] = 0;
                }
                totWeightGPOwner[gaugeProjectOwner] = 0;
            }

        } else {
            return true;
        }

        return false;   

    }

    /*
        SETTERS
    */

    // set emission Power for xLQDR holder:
    // eg.: _xLQDREmissionPower = 3000 (30%), then projectEmissionPower = maxDiv - 3000 = 7000 (70%)
    function setEmissionPowerForXLQDR(uint256 _xLQDREmissionPower) external onlyOwner {
        
        xLQDRHoldersEmissionPower = _xLQDREmissionPower; 
        projectEmissionPower = maxDiv.sub(xLQDRHoldersEmissionPower); 
    }


    function setVoterProxy(address _voterProxy) external onlyOwner {
        require(_voterProxy != address(0), 'addr 0');
        voterProxy = _voterProxy;
    }

    function setGaugeProjectOwner(address _project) external onlyOwner {
        require(gaugeProjectOwner != _project, "already in");
        require(_project != address(0), 'addr 0');
        gaugeProjectOwner = _project;

    }

    // Add new token gauge
    function addGauge(address _token, address _gauge) external onlyOwner returns (address) {
        require(gauges[_token] == address(0x0), "exists");
        gauges[_token] = _gauge; //address(new Gauge(address(LQDR), address(xLQDR), _token, governance));
        _tokens.push(_token);
        return gauges[_token];
    }

    function removeGauge(address _token, address _gauge) external onlyOwner returns (bool) {

    }

    function addKeeper(address _keeper) external onlyOwner {
        require(isKeeper[_keeper] != true, "already in");
        isKeeper[_keeper] = true;
    }

    function removeKeeper(address _keeper) external onlyOwner {
        require(isKeeper[_keeper] != false, "already in");
        isKeeper[_keeper] = false;
    }


    /*
        VIEW FUNCTIONS
    */
    function length() external view returns (uint256) {
        return _tokens.length;
    }

    function tokens() external view returns (address[] memory) {
        return _tokens;
    }

    function getGauge(address _token) external view returns (address) {
        return gauges[_token];
    }

    function epochLength() public view returns (uint256) {
        IVoterProxy(voterProxy).currentEpochLen();
    }

    function getGaugeWeight(address _token) external view returns (uint256) {
        return weights[_token];
    }

    function getGaugeWeightEpoch(address _token, uint256 epoch) external view returns (uint256) {
        return epochTokenWeights[_token][epoch];
    }

    function getTotalWeightEpoch(uint256 epoch) external view returns (uint256) {
        return epochTotalWeight[epoch];
    }

    function getProjectWeightEpoch(address _token, uint256 epoch) external view returns (uint256) {
        return tokenWeightGPOwner[_token][epoch];
    }

    function getTotalWeight() external view returns (uint256) {
        return totalWeight;
    }
}

