pragma solidity ^0.8.9;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";

import "./interfaces/IMaster.sol";
import "./interfaces/IWhitelist.sol";
import "./interfaces/IFNFT.sol";
import "./interfaces/IERC20.sol";

import {DataTypes} from "./libraries/DataTypes.sol";

contract IHO is
    Initializable,
    ERC721Upgradeable,
    ERC721URIStorageUpgradeable,
    ReentrancyGuardUpgradeable,
    PausableUpgradeable
{
    using CountersUpgradeable for CountersUpgradeable.Counter;
    CountersUpgradeable.Counter public _idCounter;

    // Used for stablecoin bond deposits
    address public supportedStablecoinAddress;
    // Addresses of contract
    address public whitelistContractAddress;
    // Track bond deposit of IHO, given the tokenId
    mapping(uint256 => uint256) public bondDeposits;

    // Track underwriter for each IHO (NFT Id)
    mapping(uint256 => address) public underwriters;

    // Track talent of each IHO (NFT Id)
    mapping(uint256 => address) public talent;

    event AgreementCreated(uint256 tokenId);
    event Redeemed();
    event UnderwriterBondPosted(address sender, uint256 amount, uint256 tokenId);
    event UnderwriterBondReturned(address sender, uint256 amount);
    event FractionalisedTokenMinted(uint256 tokenId, uint256 fractionalTokens); //

    // Struct containing data about each agreement and NFT
    struct FractionalisedNft {
        uint256 tokenId;
        uint256 totalFractionalTokens;
        uint256 fractionalTokensForAuction;
        uint256 decimals;
        uint256 bond;
        uint256 floorPrice;
        uint256 duration;
        uint256 buybackMultiplier;
        string uri;
        bool isBonded;
    }

    // Map the id of the NFT to the struct
    mapping(uint256 => FractionalisedNft) public fractionalisedMap;

    // Struct containing data about the dividends of IHOs
    struct Dividend {
        string dividendDate;
        uint8 dividendFrequencyInMonths;
        uint8 dividendFee;
    }

    // Map the id of the NFT to the struct of dividends
    mapping(uint256 => Dividend) public dividendsMap;

    //fuction modifiers
    modifier onlyAdmin() {
        require(IWhitelist(whitelistContractAddress).hasRoleAdmin(msg.sender), "not an admin");
        _;
    }

    modifier onlyUnderwriter(uint256 _id) {
        require(msg.sender == underwriters[_id], "Not an underwriter");
        _;
    }

    modifier underwriterOrAdmin(uint256 _id) {
        require(
            IWhitelist(whitelistContractAddress).hasRoleAdmin(msg.sender) || msg.sender == underwriters[_id],
            "Not an underwriter or an admin"
        );
        _;
    }

    function initialize(
        string memory nftName,
        string memory nftSymbol,
        address _supportedStablecoinAddress,
        address _whitelistContractAddress
    ) public initializer {
        supportedStablecoinAddress = _supportedStablecoinAddress;
        whitelistContractAddress = _whitelistContractAddress;
        __ERC721_init(nftName, nftSymbol);
        __ERC721URIStorage_init();
        __ReentrancyGuard_init();
        __Pausable_init();
    }

    // Buyback functionality

    /// @notice Input params in integer format, in USD.
    function calcBuybackPriceInUSD(
        uint256 _tokenId,
        uint256 trailingHigh365d,
        uint256 trailingHigh90d
    ) external view returns (uint256) {
        uint256 trailingHigh;
        if (trailingHigh365d >= trailingHigh90d) {
            trailingHigh = trailingHigh365d;
        } else {
            trailingHigh = trailingHigh90d;
        }
        return (trailingHigh * fractionalisedMap[_tokenId].buybackMultiplier);
    }

    // Pausable functions
    function pause() public onlyAdmin {
        _pause();
    }

    function unpause() public onlyAdmin {
        _unpause();
    }

    function _whenNotPaused() external view whenNotPaused returns (bool) {
        return true;
    }

    // URI Storage extension for ERC721. Overriding because it is not supported in Solidity ^0.8.0
    function _baseURI() internal pure override returns (string memory) {
        return "https://gateway.pinata.cloud/ipfs/";
    }

    // Get tokenURI given a tokenId
    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    // SafeMint function, to be used for minting
    function safeMint(address to) internal {
        _safeMint(to, _idCounter.current());
    }

    // override burn function
    function _burn(uint256 tokenId) internal override(ERC721Upgradeable, ERC721URIStorageUpgradeable) {
        super._burn(tokenId);
    }

    // Create contract agreement for owners
    /// @param tokenURI_ IPFS hash
    /// @param _totalFractionalTokens amount of fractions to be minted
    /// @param _bond amount of bond to be posted, in Wei
    function createAgreement(
        string memory tokenURI_,
        uint256 _totalFractionalTokens,
        uint256 _fractionalTokensForAuction,
        uint256 _bond,
        uint256 _floorPrice,
        uint256 _buybackMultiplier,
        uint256 _duration,
        address _underwriter,
        address _talent
    ) external onlyAdmin {
        //Increase counter
        _idCounter.increment();
        uint256 currentId = _idCounter.current();
        //Add to struct
        FractionalisedNft memory fnft;
        fnft.tokenId = currentId;
        fnft.totalFractionalTokens = _totalFractionalTokens;
        fnft.fractionalTokensForAuction = _fractionalTokensForAuction;
        fnft.bond = _bond;
        fnft.floorPrice = _floorPrice;
        fnft.decimals = 2;
        fnft.duration = _duration;
        fnft.buybackMultiplier = _buybackMultiplier;
        fnft.uri = tokenURI_;
        fnft.isBonded = false;
        //Add to mapping
        fractionalisedMap[_idCounter.current()] = fnft;

        // Set underwriter and talent addresses
        underwriters[currentId] = _underwriter;
        talent[currentId] = _talent;

        emit AgreementCreated(fnft.tokenId);
    }

    function setDividendData(
        uint256 tokenId,
        string memory _dividendDate,
        uint8 _dividendFrequencyInMonths,
        uint8 _dividendFee
    ) public onlyAdmin {
        Dividend memory dividend;
        dividend.dividendDate = _dividendDate;
        dividend.dividendFrequencyInMonths = _dividendFrequencyInMonths;
        dividend.dividendFee = _dividendFee;
        dividendsMap[tokenId] = dividend;
    }

    // Post bond for underwriters
    /// @notice Require approval first
    function postBond(uint256 tokenId) external nonReentrant whenNotPaused underwriterOrAdmin(tokenId) {
        require(fractionalisedMap[tokenId].isBonded == false, "Bond has already been posted");
        require(_exists(tokenId) == false, "Token is already minted");
        FractionalisedNft storage fnft = fractionalisedMap[tokenId];

        bool bondDeposited = IERC20Upgradeable(supportedStablecoinAddress).transferFrom(
            msg.sender,
            address(this),
            fnft.bond
        );
        require(bondDeposited, "Bond transfer failed");
        fnft.isBonded = true;
        // add underwriter's deposit in a mapping
        bondDeposits[tokenId] = fnft.bond;
        emit UnderwriterBondPosted(msg.sender, fnft.bond, tokenId);
    }

    // Return the underwriter's bond in the same address. To be called by the underwriter
    // TODO: set rules when to return
    function returnBondForUnderwriters(uint256 tokenId) external nonReentrant whenNotPaused onlyUnderwriter(tokenId) {
        uint256 bondDeposit = bondDeposits[tokenId];
        require(bondDeposit > 0, "Nothing to claim");
        // Transfer bond to underwriter
        bool bondReturned = IERC20Upgradeable(supportedStablecoinAddress).transfer(msg.sender, bondDeposit);

        require(bondReturned, "Return of bond failed");
        bondDeposits[tokenId] = 0;
        emit UnderwriterBondReturned(msg.sender, bondDeposit);
    }

    // Mint a new NFT, if bonded and minted
    function finalizeAgreement(uint256 tokenId) external onlyAdmin whenNotPaused {
        require(fractionalisedMap[tokenId].isBonded == true, "Bond has not been posted");
        require(_exists(tokenId) == false, "Token already minted");

        FractionalisedNft storage fnft = fractionalisedMap[tokenId];

        // Mint NFT and transfer to fractionalised token address
        _safeMint(msg.sender, fnft.tokenId);
        _setTokenURI(fnft.tokenId, fnft.uri);
        emit FractionalisedTokenMinted(tokenId, fnft.fractionalTokensForAuction);
    }

    // Getters
    function getDividendFee(uint256 tokenId) public view returns (uint8) {
        return dividendsMap[tokenId].dividendFee;
    }

    // Failsafe functions for owner
    function _updateTalentAddress(uint256 tokenId, address _talent) external onlyAdmin {
        talent[tokenId] = _talent;
    }

    function _updateBuybackMultiplier(uint256 tokenId, uint256 _multiplier) external onlyAdmin {
        fractionalisedMap[tokenId].buybackMultiplier = _multiplier;
    }

    function _transferOutAnyERC20(
        address _address,
        address _to,
        uint256 _amount
    ) external onlyAdmin returns (bool) {
        return IERC20Upgradeable(_address).transferFrom(address(this), _to, _amount);
    }

    function _updateFractionalisedNft(
        uint256 _tokenId,
        uint256 _totalFractionalTokens,
        uint256 _fractionalTokensForAuction,
        uint256 _bond,
        string memory _uri,
        bool _isBonded
    ) external onlyAdmin whenNotPaused {
        FractionalisedNft storage fnft = fractionalisedMap[_tokenId];
        fnft.tokenId = _tokenId;
        fnft.totalFractionalTokens = _totalFractionalTokens;
        fnft.fractionalTokensForAuction = _fractionalTokensForAuction;
        fnft.bond = _bond;
        fnft.uri = _uri;
        fnft.isBonded = _isBonded;
    }
}

