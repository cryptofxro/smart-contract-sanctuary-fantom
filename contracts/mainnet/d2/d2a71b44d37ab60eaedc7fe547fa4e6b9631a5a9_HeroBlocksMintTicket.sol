import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "../lib/Base64.sol";
import "../lib/TokenURIMetadata.sol";
import "../CryptoBlocksERC721Upgradeable.sol";
import "./lib/ExpirationItemConstants.sol";
import "./IExpirable.sol";

/** @title HeroBlocksMintTicket
 * @dev Every expiration item is the same
 * Please use this ticket to mint at https://cryptoblocksmmo.com
 */
contract HeroBlocksMintTicket is Initializable, UUPSUpgradeable, CryptoBlocksERC721Upgradeable, IExpirable {
    using CountersUpgradeable for CountersUpgradeable.Counter;
    using StringsUpgradeable for uint256;
    string private _imageURI;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

    function initialize() initializer public {
        __Context_init_unchained();
        __ERC165_init_unchained();
        __AccessControl_init_unchained();
        __AccessControlEnumerable_init_unchained();
        __ERC721_init_unchained("Hero Blocks Mint Ticket: https://cryptoblocksmmo.com", "HeroBlockMintTicket");
        __ERC721Burnable_init_unchained();
        __ERC721Enumerable_init_unchained();
        __CryptoBlocks_init_unchained();
        __CryptoBlocksERC721_init_unchained();
        __HeroBlocksMintTicket_init_unchained();
    }

    function __HeroBlocksMintTicket_init_unchained() internal initializer {}

    function _authorizeUpgrade(address) internal override onlyRole(AccessControlConstants.ADMIN_ROLE) {}

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId), "URI query for nonexistent token");

        string memory _tokenURI = _tokenURIs[tokenId];

        // If both are set, concatenate the baseURI and tokenURI (via abi.encodePacked).
        if (bytes(_tokenURI).length > 0) {
            return string(abi.encodePacked(_baseURI(), _tokenURI));
        }

        string memory json = string(abi.encodePacked(
                '{"id": ', tokenId.toString(),
                ', "name": "Hero Blocks Mint Ticket", "description": "Mint your free Hero at https://cryptoblocksmmo.com", "external_url": "https://cryptoblocksmmo.com", "attributes":['
            ));

        json = string(abi.encodePacked(json, TokenURIMetadata.getTraitStringJson('Expiration Time', _uintMapping[ExpirationItemConstants.UINT_MAPPING_EXPIRATION_TIME].toString())));

        json = string(abi.encodePacked(json,
            ']'
            ));
        // create image url
        json = string(abi.encodePacked(json,
            ',"image":"',_imageURI,'"',
            '}'
            ));

        return string(abi.encodePacked('data:application/json;base64,', Base64.encode(bytes(json))));
    }

    function expired() external view override returns(bool) {
        if(block.timestamp > _uintMapping[ExpirationItemConstants.UINT_MAPPING_EXPIRATION_TIME]) {
            return true;
        }
        return false;
    }

    function setExpirationTime(uint256 time_) external onlyRole(AccessControlConstants.ADMIN_ROLE) {
        _uintMapping[ExpirationItemConstants.UINT_MAPPING_EXPIRATION_TIME] = time_;
    }

    function getExpirationTime() external view returns(uint256) {
        return _uintMapping[ExpirationItemConstants.UINT_MAPPING_EXPIRATION_TIME];
    }

    function setImageURI(string calldata imageURI_) external onlyRole(AccessControlConstants.ADMIN_ROLE) {
        _imageURI = imageURI_;
    }

    function getImageURI() external view returns(string memory) {
        return _imageURI;
    }

    function massMint(address[] calldata accounts_) external onlyRole(AccessControlConstants.MINTER_ROLE) {
        for(uint256 i = 0; i < accounts_.length; i++) {
            _safeMint(accounts_[i], _tokenIdCounter.current());
            _tokenIdCounter.increment();
        }
    }
}

