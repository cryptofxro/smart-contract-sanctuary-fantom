import '@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/token/ERC721/IERC721ReceiverUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/utils/cryptography/ECDSAUpgradeable.sol';

// Helpers
import './helpers/WarpBase.sol';
import './interfaces/IStarship.sol';
import './interfaces/IStarshipControl.sol';
import './interfaces/IPlanets.sol';

import 'hardhat/console.sol';

contract WarpGate is WarpBase, IERC721ReceiverUpgradeable {
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.UintSet;

    event WormHole(
        address _from,
        address _to,
        uint256 _chainId,
        uint256 _timestamp,
        uint256 _shipId,
        Ship _ship
    );
    event Bridged(address _to, uint256 _chainId, uint256 _shipId, Ship _ship, bytes _signature);
    event FailSafe(address _to, uint256 _chainId, uint256 _shipId);

    // Verifiers
    mapping(address => bool) verifiers;
    mapping(uint256 => bool) public allowedChains;
    mapping(bytes32 => bool) public usedSignature;

    // Contracts
    IStarship starship;
    IStarshipControl starshipControl;

    // Variables
    uint256 cost;

    /** ======== Init ======== */
    function initialize(address _s, address _sc) public initializer {
        __WarpBase_init(); // also inits ownable

        starship = IStarship(_s);
        starshipControl = IStarshipControl(_sc);
        verifiers[msg.sender] = true;
    }

    function wormhole(
        uint256 _shipId,
        address _to,
        uint256 _chainId
    ) external payable whenNotPaused {
        require(allowedChains[_chainId], 'Chain not allowed');
        require(starship.isApprovedForAll(msg.sender, address(this)), 'Not approved');
        require(starship.ownerOf(_shipId) == msg.sender, 'You are not the owner');
        // starship can not be in a battle
        // damage starships can't fly through warpgate?

        starship.safeTransferFrom(msg.sender, address(this), _shipId);
        Ship memory ship = starshipControl.getShip(_shipId);

        emit WormHole(msg.sender, _to, _chainId, block.timestamp, _shipId, ship);
    }

    function bridge(
        uint256 _shipId,
        address _to,
        uint256 _timestamp,
        uint256 _chainId,
        Ship memory _ship,
        bytes memory _signature
    ) external {
        // ChainId must == this chain
        require(_chainId == block.chainid, 'Incorrect chain');
        // Ensure valid signature
        bytes32 messageHash = sha256(abi.encode(_to, _shipId, _timestamp, _chainId, _ship));
        require(!usedSignature[messageHash], 'Signature already used');

        address signedAddress = ECDSAUpgradeable.recover(messageHash, _signature);
        require(verifiers[signedAddress], 'Mint: Invalid Signature.');

        usedSignature[messageHash] = true;

        // If starship exists on this chain
        if (starship.exists(_shipId)) {
            // Require warp gate is the owner then transfer
            require(starship.ownerOf(_shipId) == address(this), 'Starship already bridged');
            starship.safeTransferFrom(address(this), _to, _shipId);
        }

        starshipControl.bridged(_to, _shipId, _ship);

        emit Bridged(_to, _chainId, _shipId, _ship, _signature);
    }

    function failSafeTransfer(uint256 _shipId, address _to) external onlyOwner {
        starship.safeTransferFrom(address(this), _to, _shipId);

        emit FailSafe(_to, block.chainid, _shipId);
    }

    function setVerifier(address _verifier, bool _isVerifier) external onlyOwner {
        verifiers[_verifier] = _isVerifier;
    }

    function setAllowedChain(uint256 _chain, bool _allowed) external onlyOwner {
        allowedChains[_chain] = _allowed;
    }

    function onERC721Received(
        address operator,
        address,
        uint256 tokenId,
        bytes calldata
    ) external view override returns (bytes4) {
        require(operator == address(this), 'Operator is not warpgate');
        require(starship.ownerOf(tokenId) == address(this), 'Did not receive');

        return IERC721ReceiverUpgradeable.onERC721Received.selector;
    }
}

