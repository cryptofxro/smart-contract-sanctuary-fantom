// File contracts/TokenBurner/TokenBurner.sol

contract TokenBurner is Context {
    function burn(address collection, uint256[] memory tokenIds) public {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            IERC721(collection).safeTransferFrom(_msgSender(), address(1), tokenIds[i]);
        }
    }
}