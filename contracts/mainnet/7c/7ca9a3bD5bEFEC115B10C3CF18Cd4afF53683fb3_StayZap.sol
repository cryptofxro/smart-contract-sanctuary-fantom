        IBEP20(_from).safeTransferFrom(msg.sender, address(this), amount);
        _approveTokenIfNeeded(_from, _router);
        _addRecords(msg.sender);
        IPancakePair pair = IPancakePair(flip);
        address token0 = pair.token0();
        address token1 = pair.token1();
        if (msg.value > 0) {
            IWETH(WFTM).deposit{value: msg.value}();
        }
        if (_from == WFTM) {
            amount = amount + msg.value;
        }
        if (token0 == _from || token1 == _from) {
            address tokenA = token0 == _from ? token1 : token0;
            address tokenB = token0 == _from ? token0 : token1;

            uint256 swapValue = amount.div(2);
            uint256 tokenAmount = swapTokenForToken(
                _router,
                swapValue,
                path1[path1.length - 1] == tokenA ? path1 : path2,
                address(this)
            );

            _approveTokenIfNeeded(tokenA, _router);
            pair.skim(address(this));
            IPancakeRouter02(_router).addLiquidity(
                tokenB,
                tokenA,
                swapValue,
                tokenAmount,
                0,
                0,
                receiver,
                block.timestamp + 1 minutes
            );
        } else {
            uint256 swapValue = amount.div(2);
            _approveTokenIfNeeded(token0, _router);
            _approveTokenIfNeeded(token1, _router);
            uint256 token0Amount = swapTokenForToken(
                _router,
                swapValue,
                path1,
                address(this)
            );
            uint256 token1Amount = swapTokenForToken(
                _router,
                amount.sub(swapValue),
                path2,
                address(this)
            );

            pair.skim(address(this));
            IPancakeRouter02(_router).addLiquidity(
                token0,
                token1,
                token0Amount,
                token1Amount,
                0,
                0,
                receiver,
                block.timestamp + 1 minutes
            );
        }
    }

    /* ========== Private Functions ========== */

    function _addRecords(address _user) internal {
        for (uint256 i = 0; i < records.length; i++) {
            if (records[i] == _user) {
                return;
            }
        }
        records.push(_user);
    }

    function _approveTokenIfNeeded(address token, address _router) private {
        if (IBEP20(token).allowance(address(this), _router) == 0) {
            IBEP20(token).safeApprove(_router, uint256(2**256 - 1));
        }
    }

    function _swapFTMToFlip(
        address _router,
        address flip,
        uint256 amount,
        address[] memory path,
        address receiver
    ) private {
        if (!isFlip(flip)) {
            _swapFTMForToken(_router, flip, amount, path, receiver);
        } else {
            // flip
            IPancakePair pair = IPancakePair(flip);
            address token0 = pair.token0();
            address token1 = pair.token1();
            if (token0 == WFTM || token1 == WFTM) {
                address token = token0 == WFTM ? token1 : token0;
                uint256 swapValue = amount.div(2);
                uint256 tokenAmount = _swapFTMForToken(
                    _router,
                    token,
                    swapValue,
                    path,
                    address(this)
                );

                _approveTokenIfNeeded(token, _router);
                pair.skim(address(this));
                IPancakeRouter02(_router).addLiquidityETH{
                    value: amount.sub(swapValue)
                }(
                    token,
                    tokenAmount,
                    0,
                    0,
                    receiver,
                    block.timestamp + 1 minutes
                );
            } else {
                uint256 swapValue = amount.div(2);
                uint256 token0Amount = _swapFTMForToken(
                    _router,
                    token0,
                    swapValue,
                    path,
                    address(this)
                );
                uint256 token1Amount = _swapFTMForToken(
                    _router,
                    token1,
                    amount.sub(swapValue),
                    path,
                    address(this)
                );

                _approveTokenIfNeeded(token0, _router);
                _approveTokenIfNeeded(token1, _router);
                pair.skim(address(this));
                IPancakeRouter02(_router).addLiquidity(
                    token0,
                    token1,
                    token0Amount,
                    token1Amount,
                    0,
                    0,
                    receiver,
                    block.timestamp + 1 minutes
                );
            }
        }
    }

    function swapTokenForToken(
        address _router,
        uint256 value,
        address[] memory path,
        address receiver
    ) public returns (uint256) {
        uint256[] memory amounts = IPancakeRouter02(_router)
            .swapExactTokensForTokens(
                value,
                0,
                path,
                receiver,
                block.timestamp + 1 minutes
            );
        return amounts[amounts.length - 1];
    }

    function _swapFTMForToken(
        address _router,
        address token,
        uint256 value,
        address[] memory path,
        address receiver
    ) private returns (uint256) {
        uint256[] memory amounts = IPancakeRouter02(_router)
            .swapExactETHForTokens{value: value}(
            0,
            path,
            receiver,
            block.timestamp + 1 minutes
        );
        return amounts[amounts.length - 1];
    }

    function _swapTokenForFTM(
        address _router,
        address token,
        uint256 amount,
        address[] memory path,
        address receiver
    ) private returns (uint256) {
        uint256[] memory amounts = IPancakeRouter02(_router)
            .swapExactTokensForETH(
                amount,
