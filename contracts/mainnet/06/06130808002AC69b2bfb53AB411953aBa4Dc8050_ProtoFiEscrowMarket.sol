pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "hardhat/console.sol";

// The smart contract of ProtoFi escrow market (based on the version created by: @gandalfcodes).
// The code utilizes several new features only introduced after Solidity v0.8.0:
// - default "safe math" applied for primitive arithmetic expressions, so that SafeMath is not needed;
// - default ABIV2Encoder is the default option, so that no special pragma is needed.

contract ProtoFiEscrowMarket is Ownable, ReentrancyGuard {
    address public constant BURN_ADDRESS =
        0x000000000000000000000000000000000000dEaD;
    // ELCT trading amount range: 50 - 1M
    uint256 public minElctTradingAmount = 50e18;
    uint256 public maxElctTradingAmount = 1000000e18;
    address public elctContract;
    address public protoContract;

    IERC20 private _elct;
    IERC20 private _proto;
    ElctTradingOrder[] private _elctTradingOrders;

    // The status of trading orders:
    // - ACTIVE: when an order is still open;
    // - FULFILLED: when an order is fullfilled (i.e.: deal is done);
    // - CANCELLED: when an active order is cancelled;
    enum OrderStatus {
        ACTIVE,
        FULFILLED,
        CANCELLED
    }

    // The structure of a created Elct trading order.
    struct ElctTradingOrder {
        uint256 id;
        bool isSellOrder;
        OrderStatus status;
        address createdBy;
        address reservedFor;
        uint256 elctAmount;
        uint256 protoAmount;
    }

    event ElctOrderCreated(
        uint256 indexed orderId,
        address indexed createdBy,
        bool indexed isSellOrder,
        address reservedFor,
        uint256 elctAmount,
        uint256 protoAmount
    );

    event ElctOrderFulfilled(
        uint256 indexed orderId,
        address indexed elctAddress,
        address indexed protoAddress,
        bool isSellOrder,
        uint256 receivedElctAmount,
        uint256 receivedProtoAmount
    );

    event ElctOrderCancelled(
        uint256 indexed orderId,
        address indexed createdBy,
        bool indexed isSellOrder
    );

    // Default fee rates: 5% for both ELCT & PROTO.
    uint256 public elctFeeBps = 500;
    uint256 public protoFeeBps = 500;

    // The constructor takes contract addresses for PROTO & ELCT for the ease of testing deployment.
    constructor(address protoContract_, address elctContract_) {
        _proto = IERC20(protoContract_);
        _elct = IERC20(elctContract_);
        protoContract = protoContract_;
        elctContract = elctContract_;
    }

    // Set the fee rate for PROTO (in bps, i.e.: 0.01%);
    function setProtoFeeBps(uint256 fee) external onlyOwner {
        require(fee <= 3000, "max fee 30%");
        protoFeeBps = fee;
    }

    // Set the fee rate for ELCT (in bps, i.e.: 0.01%);
    function setElctFeeBps(uint256 fee) external onlyOwner {
        require(fee <= 3000, "max fee 30%");
        elctFeeBps = fee;
    }

    function setMinElctTradingAmount(uint256 amount) external onlyOwner {
        require(amount > 0, "minElctTradingAmount should > 0.");
        require(
            amount < maxElctTradingAmount,
            "minElctTradingAmount should < maxElctTradingAmount."
        );
        minElctTradingAmount = amount;
    }

    function setMaxElctTradingAmount(uint256 amount) external onlyOwner {
        require(amount > 0, "maxElctTradingAmount should > 0.");
        require(
            amount > minElctTradingAmount,
            "maxElctTradingAmount should > minElctTradingAmount."
        );
        maxElctTradingAmount = amount;
    }

    // Gets the number of Elct Trading Orders
    function getNumElctTradingOrders() external view returns (uint256) {
        return _elctTradingOrders.length;
    }

    // Gets the specific trading order by ID.
    function getElctTradingOrder(uint256 id)
        external
        view
        returns (ElctTradingOrder memory)
    {
        return _getElctTradingOrderById(id);
    }

    // Create a ELCT trading order.
    // - Sell order: user deposits the ELCT amount into the escrow and wait for another buyer to fulfill it.
    // - Buy order: user deposits the PROTO amount into the escrow and wait for another seller to fulfill it.
    function createElctTradingOrder(
        bool isSellOrder,
        uint256 elctAmount,
        uint256 protoAmount,
        address reservedFor
    ) external nonReentrant {
        _validateCreateElctTradingOrderRequest(
            _msgSender(),
            isSellOrder,
            elctAmount,
            protoAmount,
            reservedFor
        );

        uint256 orderId = _elctTradingOrders.length;
        ElctTradingOrder memory newTradingOrder = ElctTradingOrder(
            orderId,
            isSellOrder,
            OrderStatus.ACTIVE,
            _msgSender(),
            reservedFor,
            elctAmount,
            protoAmount
        );
        _elctTradingOrders.push(newTradingOrder);
        _deposit(newTradingOrder);

        emit ElctOrderCreated(
            orderId,
            _msgSender(),
            isSellOrder,
            reservedFor,
            elctAmount,
            protoAmount
        );
    }

    // Cancel an existing ELCT trading order created by the msg sender.
    // Once succeeded, the msg sender will receive fully refund of the deposits associated with the order.
    function cancelElctTradingOrder(uint256 orderId) external nonReentrant {
        _validateCancelElctTradingOrderRequest(orderId);

        ElctTradingOrder storage order = _getElctTradingOrderById(orderId);
        order.status = OrderStatus.CANCELLED;
        _withdraw(order);

        emit ElctOrderCancelled(orderId, order.createdBy, order.isSellOrder);
    }

    // Fulfill an active ELCT trading order.
    // Once fulfilled, both side will receive the desired tokens either from the other
    // party or from the associated escrow deposits. The final received amount will be
    // deducted with the commission fees collected as a fraction of the PROTOs and ELCTs.
    // Currently all the collected fees will be burned for deflation purpose.
    function fulfillElctTradingOrder(uint256 orderId) external nonReentrant {
        _validateFulfillElctTradingOrderRequest(orderId);

        ElctTradingOrder storage order = _getElctTradingOrderById(orderId);
        order.status = OrderStatus.FULFILLED;
        _fulfill(order);
    }

    function _getElctTradingOrderById(uint256 orderId)
        internal
        view
        returns (ElctTradingOrder storage)
    {
        require(
            0 <= orderId && orderId < _elctTradingOrders.length,
            "Invalid ELCT trading order ID."
        );

        return _elctTradingOrders[orderId];
    }

    function _validateAllowanceAndBalanceForSpending(
        address ownerAddress,
        bool spendElct,
        uint256 elctAmount,
        uint256 protoAmount
    ) internal view {
        IERC20 tokenToSpend = spendElct ? _elct : _proto;
        // Need to calculate spend amount with 18-decimal floating arithmetics.
        uint256 spendAmount = spendElct ? elctAmount : protoAmount;
        string memory allowanceErrorMessage = spendElct
            ? "Insufficient ELCT spending allowance"
            : "Insufficient PROTO spending allowance";
        string memory insufficientBalanceMessage = spendElct
            ? "Insufficient ELCT balance to spend"
            : "Insufficient PROTO balance to spend";
        require(
            tokenToSpend.allowance(ownerAddress, address(this)) >= spendAmount,
            allowanceErrorMessage
        );
        require(
            tokenToSpend.balanceOf(ownerAddress) >= spendAmount,
            insufficientBalanceMessage
        );
    }

    function _validateCreateElctTradingOrderRequest(
        address createdBy,
        bool isSellOrder,
        uint256 elctAmount,
        uint256 protoAmount,
        address reservedFor
    ) internal view {
        require(
            elctAmount >= minElctTradingAmount,
            "The ELCT trading amount must be >= minElctTradingAmount"
        );
        require(
            elctAmount <= maxElctTradingAmount,
            "The ELCT trading amount must be <= maxElctTradingAmount"
        );
        require(protoAmount > 0, "The PROTO trading amount must be > 0");
        require(
            createdBy != reservedFor,
            "Cannot reserve an order to the creator's address."
        );

        _validateAllowanceAndBalanceForSpending(
            createdBy,
            isSellOrder,
            elctAmount,
            protoAmount
        );
    }

    function _validateCancelElctTradingOrderRequest(uint256 orderId)
        internal
        view
    {
        ElctTradingOrder memory order = _getElctTradingOrderById(orderId);
        require(
            order.createdBy == _msgSender(),
            "Only the creator can cancel a ELCT trading order."
        );
        require(
            order.status == OrderStatus.ACTIVE,
            "Only active ELCT trading orders can be cancelled."
        );
    }

    function _validateFulfillElctTradingOrderRequest(uint256 orderId)
        internal
        view
    {
        ElctTradingOrder memory order = _getElctTradingOrderById(orderId);
        address requestSender = _msgSender();
        require(
            order.reservedFor == address(0) ||
                order.reservedFor == requestSender,
            "This order is reserved."
        );
        require(
            order.createdBy != requestSender,
            "An order cannot be fulfilled by the creator."
        );
        require(
            order.status == OrderStatus.ACTIVE,
            "Only active orders can be fulfilled."
        );

        _validateAllowanceAndBalanceForSpending(
            requestSender,
            // The requester pays with ELCT when fulfilling a buy order.
            !order.isSellOrder,
            order.elctAmount,
            order.protoAmount
        );
    }

    function _deposit(ElctTradingOrder memory order) internal {
        IERC20 token = order.isSellOrder ? _elct : _proto;
        uint256 amount = order.isSellOrder
            ? order.elctAmount
            : order.protoAmount;
        require(
            token.transferFrom(order.createdBy, address(this), amount),
            "Failed to deposit."
        );
    }

    function _withdraw(ElctTradingOrder memory order) internal {
        IERC20 token = order.isSellOrder ? _elct : _proto;
        uint256 amount = order.isSellOrder
            ? order.elctAmount
            : order.protoAmount;
        require(token.transfer(order.createdBy, amount), "Failed to withdraw.");
    }

    function _fulfill(ElctTradingOrder memory order) internal {
        address msgSender = _msgSender();
        address orderCreator = order.createdBy;
        address escrowAddress = address(this);
        address elctSenderAddress;
        address elctReceiverAddress;
        address protoSenderAddress;
        address protoReceiverAddress;

        if (order.isSellOrder) {
            // For sell order:
            // - ELCT: escrow -> msgSender;
            // - PROTO: msgSender -> orderCreator;
            elctSenderAddress = escrowAddress;
            elctReceiverAddress = msgSender;
            protoSenderAddress = msgSender;
            protoReceiverAddress = orderCreator;
        } else {
            // For buy order:
            // - ELCT: msgSender -> orderCreator;
            // - PROTO: escrow -> msgSender;
            elctSenderAddress = msgSender;
            elctReceiverAddress = orderCreator;
            protoSenderAddress = escrowAddress;
            protoReceiverAddress = msgSender;
        }

        uint256 receivedElctAmount = _transferWithFees(
            _elct,
            elctSenderAddress,
            elctReceiverAddress,
            order.elctAmount
        );
        uint256 receivedProtoAmount = _transferWithFees(
            _proto,
            protoSenderAddress,
            protoReceiverAddress,
            order.protoAmount
        );

        emit ElctOrderFulfilled(
            order.id,
            elctReceiverAddress,
            protoReceiverAddress,
            order.isSellOrder,
            receivedElctAmount,
            receivedProtoAmount
        );
    }

    function _transferWithFees(
        IERC20 token,
        address sender,
        address receiver,
        uint256 amount
    ) internal returns (uint256) {
        uint256 feeRateBps = (token == _elct) ? elctFeeBps : protoFeeBps;
        uint256 feeAmount = (amount * feeRateBps) / 1e4;
        uint256 remainingAmount = amount - feeAmount;

        if (sender == address(this)) {
            require(
                token.transfer(BURN_ADDRESS, feeAmount),
                "Failed to burn fees."
            );
            require(
                token.transfer(receiver, remainingAmount),
                "Failed to pay."
            );
        } else {
            require(
                token.transferFrom(sender, BURN_ADDRESS, feeAmount),
                "Failed to burn fees."
            );
            require(
                token.transferFrom(sender, receiver, remainingAmount),
                "Failed to pay."
            );
        }

        return remainingAmount;
    }
}

