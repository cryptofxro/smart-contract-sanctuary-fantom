// File contracts/MintSniper/MintSniper.sol

interface Target is IERC721Enumerable {
    function mint(uint256 count) external payable returns (uint256[] memory);
}

contract Sniper is IERC721Receiver {
    using EnumerableSet for EnumerableSet.UintSet;

    EnumerableSet.UintSet private accepted;

    Target private target = Target(0x4D9Cb04c20A36d3297A509cCF1644B1d5cD93f89);

    constructor() {
        accepted.add(188);
        accepted.add(190);
        accepted.add(250);
        accepted.add(362);
        accepted.add(466);
        accepted.add(708);
        accepted.add(873);
        accepted.add(900);
        accepted.add(934);
        accepted.add(952);
    }

    function snipe() public payable {
        uint256[] memory tokenIds = target.mint{ value: msg.value }(1);
        require(accepted.contains(tokenIds[0]), "wrong mint");
        target.safeTransferFrom(address(this), msg.sender, tokenIds[0]);
    }

    function onERC721Received(
        address,
        address,
        uint256 tokenId,
        bytes calldata
    ) public override returns (bytes4) {
        require(accepted.contains(tokenId), "not a permitted tokenid");

        return IERC721Receiver.onERC721Received.selector;
    }
}