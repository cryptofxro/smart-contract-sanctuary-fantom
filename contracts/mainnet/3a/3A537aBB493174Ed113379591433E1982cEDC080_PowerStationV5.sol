pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./Interfaces/IUniswapV2Pair.sol";
import "./Interfaces/IJoeRouter02.sol";
import "./Interfaces/IJoeFactory.sol";
import 'hardhat/console.sol';

import "@openzeppelin/contracts/utils/Strings.sol";

struct NodeTier {
    uint256 rewardTime;
    uint256 rewards;
    uint256 claimTime;
    uint256 amount;
}

struct NodeType {
    uint256 price;
    uint256 reward;
    uint256 rewardDelay;
    uint256 totalCreated;
    uint256 signCost;
    uint256 stablePrice;
}



/* ERRORS
    Library :
    PL:1 = Out of bounds

    Station :
    ST:1 = Args error, not same array elements number
    ST:2 = You can't remove the TJ Pair
    ST:3 = You're blacklisted
    ST:4 = Node doesn't exist
    ST:5 = This is not sign period for your node
    ST:6 = You're node name should be between 3 and 31 characters
    ST:7 = Node name already taken
    ST:8 = Sender couldn't be zero
    ST:9 = Node tier doesn't exist
    ST:10 = You can't afford this node
    ST:11 = You're not migration contract
    ST:12 = You have nothing to claim
    ST:13 = You're not lottery contract
    ST:14 = Problem with taking funds on nodes
    ST:15 = Problem with args
 */

interface IPowerLottery {
    function powerPerTicket() external returns (uint);
}

interface ISpookyRouter {
    function getAmountsOut(uint256, address[] memory) external view returns (uint256[] memory);
}


struct NodeEntity {
    uint256 creationTime;
    uint256 lastClaimTime;
    uint256 rewardTaken;
    string tier;
    uint256 signTime;
}

interface IPowerStationV4 {
    function getRewardAmountOf(address account, string memory tier) external view returns (uint256);
    function getNodeNumberOf(address account, string memory tier) external view returns (uint256);
    function lastSignTime(address account) external view returns (uint256);
    function _claimDate(address account, string memory tier) external view returns (uint256);
    function _getNodesNames(address account, string memory tier) external view returns (string[] memory);
    function _nodesOfUser(address account, string memory tier) external view returns (NodeEntity memory);
    function getLastSign(address user) external view returns(uint256);
}

contract PowerStationV5 is Initializable, OwnableUpgradeable {
    using SafeMathUpgradeable for uint256;

    mapping(address => mapping(string => NodeTier)) public _nodesOfUser;
    mapping(string => NodeType) public nodeCollection;

    string[] public nodeCollectionCount;

    address public power;
    address public energy;
    address public stable;

    address public stableWallet;
    address public distributionPool;

    address public deadWallet;
    address public operator;

    address public oldStation;

    uint256 public _timeToNextSignature;

    uint256 public usersCount;

    mapping(address => bool) public alreadyJoined;
    mapping(address => bool) public _isBlacklisted;

    // sliding tax
    uint16[] public slidingTaxBreakDays; // e.g. [0, 7, 15, 30, 45]
    uint16[] public slidingTaxAmounts;   // e.g. [60, 30, 20, 15, 10]


    event ClaimNodes(address user, string tier, uint256 amount);
    event Sign(address user);
    event CreateNode(address user, string tier, uint256 amount);

    // E V E N T S

    function initialize(
        address _power,
        address _energy,
        address _stable,
        address _distributionPool,
        address _stableWallet,
        address _operator,
        address _oldStation
    ) public initializer {
        __Ownable_init();

        power = _power;
        energy = _energy;
        stable = _stable;

        distributionPool = _distributionPool;
        stableWallet = _stableWallet;

        oldStation = _oldStation;

        operator = _operator;

        slidingTaxBreakDays = [0, 7, 15, 30, 45];
        slidingTaxAmounts = [60, 30, 20, 15, 10];

        deadWallet = 0x000000000000000000000000000000000000dEaD;  

        _timeToNextSignature = 20 days;
    }




    /* LIB */

    function removeFromArray(string[] storage array,uint256 index) internal returns(string[] storage) {
        require(array.length > index, "PL:1");
        // move all elements to the left, starting from the `index + 1`
        for (uint256 i = index; i < array.length - 1; i++) {
            array[i] = array[i+1];
        }
        array.pop(); // delete the last item
        return array;
    }

    function uint2str(uint256 _i)
        internal
        pure
        returns (string memory _uintAsString)
    {
        if (_i == 0) {
            return "0";
        }
        uint256 j = _i;
        uint256 len;
        while (j != 0) {
            len++;
            j /= 10;
        }
        bytes memory bstr = new bytes(len);
        uint256 k = len;
        while (_i != 0) {
            k = k - 1;
            uint8 temp = (48 + uint8(_i - (_i / 10) * 10));
            bytes1 b1 = bytes1(temp);
            bstr[k] = b1;
            _i /= 10;
        }
        return string(bstr);
    }




    // Update params functions

    function updateAddresses(address _devWallet, address _distributionPool) external onlyOwner {
        stableWallet = _devWallet;
        distributionPool = _distributionPool;
    }

    function updateSlidingTax(uint16[] calldata _days, uint16[] calldata _percents) external onlyOwner {
        require (_days.length ==  _percents.length, "ST:1");
        slidingTaxBreakDays = _days;
        slidingTaxAmounts = _percents;
    } 
    
    function setTokens(address _power, address _energy, address _stable) external onlyOwner {
        power = _power;
        energy = _energy;
        stable = _stable;
    }


    // Blacklist system

    function blacklistMalicious(address account, bool value)
        external
        onlyOwner
    {
        _isBlacklisted[account] = value;
    }

    modifier noBlacklisted(address user) {
        require(!_isBlacklisted[user], "ST:3");
        _;
    }

    

    // Node


    /// Tier function

    function addTier(
        string memory name,
        uint256 price,
        uint256 reward,
        uint256 rewardDelay,
        uint256 signCost,
        uint256 stablePrice
    ) external onlyOwner {
        nodeCollection[name] = NodeType(
            price,
            reward,
            rewardDelay,
            0,
            signCost,
            stablePrice
        );
        nodeCollectionCount.push(name);
    }

    function updateTier(
        string memory name,
        uint256 price,
        uint256 reward,
        uint256 rewardDelay,
        uint256 signCost,
        uint256 stablePrice
    ) external onlyOwner {
        nodeCollection[name] =
            NodeType(
                price,
                reward,
                rewardDelay,
                nodeCollection[name].totalCreated,
                signCost,
                stablePrice
            );
    }

    function removeTier(
        string calldata name
    ) external onlyOwner {
        nodeCollection[name] =
            NodeType(
                0,
                0,
                0,
                0,
                0,
                0
            );

        for (uint256 i = 0; i < nodeCollectionCount.length; i++) {
            if (keccak256(abi.encodePacked(nodeCollectionCount[i])) == keccak256(abi.encodePacked(name))) {
                nodeCollectionCount = removeFromArray(nodeCollectionCount, i);
            }
        }
    }




    /// Node creation / compound function

    function _createNode(
        string memory tier,
        address user
    ) internal {
        if (_nodesOfUser[user][tier].claimTime == 0) _nodesOfUser[user][tier].claimTime = block.timestamp;
        if (lastSignTime[user] == 0) lastSignTime[user] = block.timestamp;

        _nodesOfUser[user][tier].rewards = getRewardOfTier(user, tier);

        _nodesOfUser[user][tier].amount += 1;

        _nodesOfUser[user][tier].rewardTime = block.timestamp;

        nodeCollection[tier].totalCreated += 1;

        if (alreadyJoined[user] == false) {
            usersCount += 1;
            alreadyJoined[user] = true;
        }

        if (stormTime[user] == 0) {
            stormTime[user] = getNextStormTime();
        }

        emit CreateNode(user, tier, 1);
    }

    function createNodeWithTokens(string memory tier)
        external noBlacklisted(_msgSender())
    {

        address sender = _msgSender();

        /* Verify if sender is valid */
        require(
            sender != address(0),
            "ST:8"
        );

        /* Verify if tier is valid */
        require(
            nodeCollection[tier].rewardDelay != 0,
            "ST:9"
        );

        uint256 nodePrice = nodeCollection[tier].price;
        uint256 nodePriceStable = nodeCollection[tier].stablePrice;

        require(
            IERC20(power).balanceOf(sender) >= nodePrice,
            "You need more PWFL"
        );

        require(
            IERC20(stable).balanceOf(sender) >= nodePriceStable,
            "You need more USDC"
        );

        IERC20(power).transferFrom(sender, distributionPool, nodePrice);
        IERC20(stable).transferFrom(sender, stableWallet, nodePriceStable);

        _createNode(
            tier,
            sender
        );
    }

    function compound(string memory nodeTier, uint256 amount, uint256[] memory amountToTaken)
        external noBlacklisted(_msgSender())
    {
        address sender = _msgSender();

        require(
            nodeCollection[nodeTier].rewardDelay != 0,
            "ST:9"
        );

        uint256 nodePriceStable = nodeCollection[nodeTier].stablePrice * amount;

        uint256 nodePrice = nodeCollection[nodeTier].price * amount;

        //require(false, Strings.toString((amountToTaken[0] + amountToTaken[1] + amountToTaken[2] + amountToTaken[3])));

        require((amountToTaken[0] + amountToTaken[1] + amountToTaken[2] + amountToTaken[3]) == nodePrice, 'ST:10 Power Start');

        uint256 totalReward;

        for (uint256 i; i < nodeCollectionCount.length; i++) {
            if (amountToTaken[i] == 0) continue;
            totalReward += getRewardOfTier(sender, nodeCollectionCount[i]);
            _nodesOfUser[sender][nodeCollectionCount[i]].rewards = getRewardOfTier(sender, nodeCollectionCount[i]).sub(amountToTaken[i]);
            _nodesOfUser[sender][nodeCollectionCount[i]].rewardTime = block.timestamp;
        }
        
        require(totalReward >= nodePrice, 'ST:10 Power');

        require(
            IERC20(stable).balanceOf(sender) >= nodePriceStable,
            "You need more USDC"
        );

        IERC20(stable).transferFrom(sender, stableWallet, nodePriceStable);

        for (uint256 i = 0; i < amount; i++) {
            _createNode(
                nodeTier,
                sender
            );
        }
    }

    function removeNode (
        string memory tier,
        address user
    ) external {
        require(operator == msg.sender, 'ERR');
        _nodesOfUser[user][tier].amount -= 1;
    }



    //Operator functions

    function setOperator(address _migrate) external onlyOwner {
        operator = _migrate;
    }

    function migrateUser (
        address user,
        string memory tier,
        uint256 rewards,
        uint256 claimDate,
        uint256 numberOfNodes
    ) external {
        require(operator == msg.sender, 'ERR');

        _nodesOfUser[user][tier] = NodeTier (
            block.timestamp,
            rewards,
            claimDate,
            numberOfNodes
        );

        emit CreateNode(user, tier, numberOfNodes);
        nodeCollection[tier].totalCreated += numberOfNodes;
    }



    //Lottery take funds

    address public lottery;

    function buyTickets(address user, uint256 amount, string memory tier) external {
        require(msg.sender == lottery, "ST:13");

        uint256 totalAmount;

        require(getRewardOfTier(user, tier) >= amount, "ST:14");

        _nodesOfUser[user][tier].rewards = getRewardOfTier(user, tier) - amount;
        _nodesOfUser[user][tier].rewardTime = block.timestamp;

        IERC20(power).transferFrom(distributionPool, lottery, totalAmount);
    }

    function setLottery(address _lottery) external onlyOwner {
        lottery = _lottery;
    }

    function _calcTax(string memory tier, address sender, uint256 rewards) public view returns (uint) {
        uint256 currentTime = block.timestamp;

        uint k = 0;
        while (currentTime - _nodesOfUser[sender][tier].claimTime >= slidingTaxBreakDays[k] * 86400) {
            k++;
            if (k >= slidingTaxBreakDays.length) break;
        }

        uint fee =  k > 0? slidingTaxAmounts[k - 1] : slidingTaxAmounts[0];

        return rewards * fee / 100;
    }

    function getRewardOfTier(address account, string memory tier)
        public
        view
        returns (uint256 rewards)
    {
        uint256 _stormTime = getStormTime(account);

        if (block.timestamp > _stormTime) {

            if (block.timestamp - _stormTime > 130 days) return 0;
            
            uint256 _time = _nodesOfUser[account][tier].rewardTime;
            uint256 i = getStormPassed(account);

            //Get rewards before storms
            if (_nodesOfUser[account][tier].rewardTime < _stormTime) {
                uint256 time = (_stormTime - _nodesOfUser[account][tier].rewardTime);
                rewards += time * nodeCollection[tier].reward / nodeCollection[tier].rewardDelay * (7 ** i) * 100_000 / (10 ** i) / 100_000;
                _time = _stormTime;
            }

            
            i += 1;

            while (_time < block.timestamp) {
                uint256 nextTime;

                if (i >= 6) {
                    return 0;
                } else if (_time + 30 days > block.timestamp) {
                    nextTime = block.timestamp;
                } else {
                    nextTime = _time + 30 days;
                }

                uint256 time = (nextTime - _time);
                rewards += (time * nodeCollection[tier].reward / nodeCollection[tier].rewardDelay) * (7 ** i) * 100_000 / (10 ** i) / 100_000;

                _time = nextTime;
                i += 1;
            }
            
            rewards = (rewards * _nodesOfUser[account][tier].amount) + _nodesOfUser[account][tier].rewards;
        } else {
            uint256 time = (block.timestamp - _nodesOfUser[account][tier].rewardTime);
            uint256 i = getStormPassed(account);
            
            uint256 _rewards = (time * nodeCollection[tier].reward / nodeCollection[tier].rewardDelay) * (7 ** i) * 100_000 / (10 ** i) / 100_000;
    
            rewards = (_rewards * _nodesOfUser[account][tier].amount) + _nodesOfUser[account][tier].rewards;
        }
    }
    
    function haveAllNodeTiers(address _user) external view returns (bool) {
        for (uint256 i; i < nodeCollectionCount.length; i++) {
            if (_nodesOfUser[_user][nodeCollectionCount[i]].amount == 0) return false;
        }
        return true;
    }

    function cashoutAll(string memory tier) external noBlacklisted(_msgSender()) {
        address _user = _msgSender();

        uint256 rewardAmount = getRewardOfTier(_user, tier);
        _nodesOfUser[_user][tier].rewardTime = block.timestamp;
        _nodesOfUser[_user][tier].rewards = 0;

        require(rewardAmount > 0, "ST:12");

        uint tax = _calcTax(tier, _user, rewardAmount);
        _nodesOfUser[_user][tier].claimTime = block.timestamp;

        rewardAmount -= tax;

        IERC20(power).transferFrom(distributionPool, _user, rewardAmount);
        emit ClaimNodes(_user, tier, rewardAmount);
    }

    function setRewardTime(address _user, string memory _tier, uint256 _date) external onlyOwner{
        _nodesOfUser[_user][_tier].rewardTime = _date;
    }

    function setRewards(address _user, string memory _tier, uint256 _rewards) external onlyOwner{
        _nodesOfUser[_user][_tier].rewards = _rewards;
    }



    ///Get nodes functions

    function getNodeNumberOf(address _user, string memory tier)
        public
        view
        returns (uint256)
    {
        return _nodesOfUser[_user][tier].amount;
    }

    function getTotalCreatedNodes() public view returns (uint256) {
        uint256 totalNodesCreated;

        for (uint256 i = 0; i < nodeCollectionCount.length; i++) {
            totalNodesCreated = totalNodesCreated.add(nodeCollection[nodeCollectionCount[i]].totalCreated);
        }

        return totalNodesCreated;
    }

    function getClaimDate(address account, string memory tier) public view returns (uint256) {
        return _nodesOfUser[account][tier].claimTime;
    }



    ///Migration

    function migrate(address who) external {
        require(msg.sender == oldStation, "You can't");

        lastSignTime[who] = IPowerStationV4(oldStation).lastSignTime(who);

        for (uint256 i; i < nodeCollectionCount.length; i++) {
            _nodesOfUser[who][nodeCollectionCount[i]] = NodeTier (
                block.timestamp,
                IPowerStationV4(oldStation).getRewardAmountOf(who, nodeCollectionCount[i]),
                IPowerStationV4(oldStation)._claimDate(who, nodeCollectionCount[i]),
                IPowerStationV4(oldStation).getNodeNumberOf(who, nodeCollectionCount[i])
            );

            emit CreateNode(who, nodeCollectionCount[i], _nodesOfUser[who][nodeCollectionCount[i]].amount);
            nodeCollection[nodeCollectionCount[i]].totalCreated += _nodesOfUser[who][nodeCollectionCount[i]].amount;
        }
        
        if (alreadyJoined[who] == false) {
            usersCount += 1;
            alreadyJoined[who] = true;
        }
    }

    function setOldStation(address _oldStation) external onlyOwner {
        oldStation = _oldStation;
    }


    // Test function

    

    function _calculateReward(uint256 lastClaimTime, string memory tier)
        public
        view
        returns (uint256)
    {
        NodeType memory tierSpec = nodeCollection[tier];
        uint256 _claims = (block.timestamp - lastClaimTime) / tierSpec.rewardDelay;
        
        return tierSpec.reward * _claims;
    }

    function migrateUser (
        address user,
        string memory tier,
        uint256[] memory claimDate
    ) external {
        require(operator == msg.sender, 'ERR');

        for (uint256 i; i < claimDate.length; i++) {
            _createNode(tier, user);
            
            _nodesOfUser[user][tier].rewards += _calculateReward(claimDate[i], tier);
        }
    }

    function fixTaxTier(
        string[] calldata tiers,
        address[] memory accounts,
        uint256[] memory lastClaimDate
    ) external {
        require(operator == msg.sender, 'ERR');
        for (uint256 i = 0; i < tiers.length; i++) {
            _nodesOfUser[accounts[i]][tiers[i]].claimTime = lastClaimDate[i];
        }
    }


    /// Signature feature

    mapping(address => uint256) public lastSignTime;

    function setLastSignTime(address _user, uint256 signTime) external onlyOwner {
        lastSignTime[_user] = signTime;
    }

    /// Storm

    mapping(address => uint256) public stormTime;
    mapping(address => uint256) public stormPassed;
    uint256 nextStorm;

    address router;

    function setFirstStormTime(address _user, uint256 _stormTime) external onlyOwner {
        stormTime[_user] = _stormTime;
    }

    function setNextStorm(uint256 _nextStorm) external onlyOwner {
        nextStorm = _nextStorm;
    }

    function setRouter(address _router) external onlyOwner {
        router = _router;
    }

    function getStormPassed(address account) public view returns (uint256 _stormPassed) {
        if (1665762491 > lastSignTime[account] && (1665762491 - lastSignTime[account]) > 60 days) {
            _stormPassed = 2;
        }
        _stormPassed += stormPassed[account];
    }

    function getNextStormTime() public view returns(uint256 _nextStorm) {
        _nextStorm = nextStorm;
        while (_nextStorm < block.timestamp) {
            _nextStorm += 30 days;
        }
    }

    function getStormTime(address _user) public view returns(uint256) {
        if (stormTime[_user] == 0) {
            return getNextStormTime();
        }
        return stormTime[_user];
    }

    function getStormStart(address _user) public view returns(uint256 _startStorm) {
        _startStorm = getStormTime(_user);
        if (block.timestamp > _startStorm) {
            while (block.timestamp - _startStorm > 30 days) {
                _startStorm += 30 days;
            }
        }
        
        return _startStorm;
    }

    function getStormEnd(address _user) public view returns(uint256) {
        return getStormStart(_user) + 10 days;
    }


    function getStormFees(address user, uint256 cycle) public view returns(uint256 fees) {
        uint256 initialAmountToTake;
        uint256 currentCyclePaid = 1;
        
        for (uint256 i; i < nodeCollectionCount.length; i++) {                
            initialAmountToTake += _nodesOfUser[user][nodeCollectionCount[i]].amount * nodeCollection[nodeCollectionCount[i]].reward * 1440 * 20 * 15 / 100;
        }
        
        if (getStormTime(user) < block.timestamp) {
            fees += initialAmountToTake;
        } else {
            currentCyclePaid += uint256(getStormTime(user) - block.timestamp) / uint256(30 days);
        }

        for (uint256 i = currentCyclePaid; i <= cycle + currentCyclePaid - 1; i++) {
            fees += initialAmountToTake * (2 ** i);
        }
    }

    function getStormFeesFtm(address user, uint256 cycle) public view returns(uint256 fees) {
        fees = getStormFees(user, cycle);

        if (router != address(0)) {
            address[] memory addresses;
            addresses[0] = 0x1f797FDeBD1C9Bf015643D0cc8BD00ecE9e709D7;
            addresses[1] = 0x21be370D5312f44cB42ce377BC9b8a0cEF1A4C83;
            addresses[2] = 0x04068DA6C83AFCFA0e13ba15A6696662335D5B75;
            uint256[] memory prices = ISpookyRouter(router).getAmountsOut(fees, addresses);
            fees = prices[2];
        } else {
            fees = fees * 104879000000000000 / 1 ether;
        }
    }

    function payStorm(uint256 cycle) external payable {
        address sender = _msgSender();
        
        uint256 amountToTake = getStormFees(sender, cycle);
        uint256 amountToTakeFtm = getStormFeesFtm(sender, cycle);

        require(cycle <= 5, "You can't pay for more than 5 cycle");
        
        nextStorm = getNextStormTime();

        if (getStormTime(sender) >= block.timestamp) {
            uint256 currentCyclePaid = uint256(getStormTime(sender) - block.timestamp) / uint256(30 days);
            require(cycle + currentCyclePaid <= 5, "You can't pay for more than 5 cycle");
        } else {
            uint256 stormsPassedCount = uint256(block.timestamp - getStormTime(sender)) / uint256(10 days);
            stormsPassedCount += 1;
            
            uint256 totalMissedStorms = 1;

            while (stormsPassedCount > 3) {
                stormsPassedCount -= 3;
                totalMissedStorms += 1;
            }

            for (uint256 i; i < nodeCollectionCount.length; i++) {
                _nodesOfUser[sender][nodeCollectionCount[i]].rewards = getRewardOfTier(sender, nodeCollectionCount[i]);
                _nodesOfUser[sender][nodeCollectionCount[i]].rewardTime = block.timestamp;
            }

            if (getStormPassed(sender) + totalMissedStorms > 5) {
                require(false, 'Youre account is locked');
            } else if (stormsPassedCount > 1) {
                stormPassed[sender] = totalMissedStorms;
            }
            
            stormTime[sender] = nextStorm;
        }
        
        require(
            IERC20(power).balanceOf(sender) >= amountToTake,
            "You need more PWFL"
        );
        
        IERC20(power).transferFrom(sender, address(this), amountToTake);

        require(
            msg.value == amountToTakeFtm,
            "Wrong FTM value"
        );

        stormTime[sender] += 30 days * cycle;
    }

    //Time in sec so you need to * by 86400 to get per day
    function updateTimeToNextSignature(uint256 timeToNextSignature) external onlyOwner {
        _timeToNextSignature = timeToNextSignature;
    }
}

