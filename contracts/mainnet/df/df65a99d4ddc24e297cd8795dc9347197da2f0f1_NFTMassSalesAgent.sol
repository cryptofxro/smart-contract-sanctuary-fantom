// File contracts/TRTLSwapHelper/TRTLSwapHelper.sol

interface INFTKey {
    function listToken(
        address token,
        uint256 tokenId,
        uint256 amount,
        uint256 expiration
    ) external;

    function delistToken(address token, uint256 tokenId) external;
}

contract NFTMassSalesAgent is Ownable, IERC721Receiver {
    event ReceivedERC721(address operator, address from, uint256 tokenId, bytes data, uint256 gas);

    INFTKey public NFTKey = INFTKey(0x1A7d6ed890b6C284271AD27E7AbE8Fb5211D0739);

    function listCollection(
        address collection,
        uint256 amount,
        uint256 expiration
    ) public onlyOwner {
        IERC721(collection).setApprovalForAll(address(NFTKey), true);
        uint256 balanceOf = IERC721Enumerable(collection).balanceOf(_msgSender());
        for (uint256 i = 0; i < balanceOf; i++) {
            uint256 tokenId = IERC721Enumerable(collection).tokenOfOwnerByIndex(_msgSender(), 0);
            IERC721(collection).safeTransferFrom(_msgSender(), address(this), tokenId);
            NFTKey.listToken(collection, tokenId, amount, expiration);
        }
    }

    function delistCollection(address collection) public onlyOwner {
        uint256 balanceOf = IERC721Enumerable(collection).balanceOf(address(this));
        for (uint256 i = 0; i < balanceOf; i++) {
            uint256 tokenId = IERC721Enumerable(collection).tokenOfOwnerByIndex(address(this), 0);
            NFTKey.delistToken(collection, tokenId);
            IERC721(collection).safeTransferFrom(address(this), _msgSender(), tokenId);
        }
        IERC721(collection).setApprovalForAll(address(NFTKey), false);
    }

    receive() external payable virtual {
        (bool sent, ) = owner().call{ value: address(this).balance }("");
        require(sent);
    }

    function onERC721Received(
        address operator,
        address from,
        uint256 tokenId,
        bytes calldata data
    ) external returns (bytes4) {
        require(operator == address(this));
        require(from == owner());
        emit ReceivedERC721(operator, from, tokenId, data, gasleft());
        return IERC721Receiver.onERC721Received.selector;
    }
}