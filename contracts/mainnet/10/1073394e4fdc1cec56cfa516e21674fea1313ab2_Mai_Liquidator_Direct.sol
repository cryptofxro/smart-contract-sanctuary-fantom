interface Vault {
    function checkCost(uint256 id) external view returns (uint256);
    function liquidateVault(uint256 id) external;
    function getPaid() external;
}

contract Mai_Liquidator_Direct {
    using SafeMath for uint256;

    constructor() public {}

    function exec(address _router, address[] calldata _tokens, address _vault, uint256[] calldata _vaults) public {
        address _token1 = _tokens[0];
        address _token2 = _tokens[_tokens.length - 1];
        
        IUniswapV2Router02 router = IUniswapV2Router02(_router);
        Vault vault = Vault(_vault);

        uint256 mai = 0;
        for (uint x = 0; x < _vaults.length; x++) {
            mai += vault.checkCost(_vaults[x]);
        }
        uint256 amount = router.getAmountsIn(mai, _tokens)[0];
        
        IERC20 token1 = IERC20(_token1);
        token1.transferFrom(msg.sender, address(this), amount);
        token1.approve(_router, amount);
        router.swapExactTokensForTokensSupportingFeeOnTransferTokens(amount, mai, _tokens, address(this), block.timestamp);
        
        IERC20 token2 = IERC20(_token2);
        token2.approve(_vault, token2.balanceOf(address(this)));
 
        for (uint i = 0; i < _vaults.length; i++) {
            vault.liquidateVault(_vaults[i]);
        }
        vault.getPaid();
        
        uint256 token1_balance = token1.balanceOf(address(this));
        require(token1_balance > amount, "Token balance is too small");

        token1.transfer(msg.sender, token1_balance);
    }
}