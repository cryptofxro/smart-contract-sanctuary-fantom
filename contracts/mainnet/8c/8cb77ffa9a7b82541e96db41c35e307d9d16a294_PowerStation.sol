library PowerLibrary {
    using strings for *;
    using SafeMath for uint256;

    function strToArrayUint256(string memory str, string memory delim)
        public
        pure
        returns (uint256[] memory)
    {
        strings.slice memory stringSlice = str.toSlice();
        strings.slice memory delimeterSlice = delim.toSlice();
        uint256[] memory result = new uint256[](
            stringSlice.count(delimeterSlice) + 1
        );
        for (uint256 i = 0; i < result.length; i++) {
            (uint256 res, bool err) = strToUint(
                stringSlice.split(delimeterSlice).toString()
            );
            if (err) {
                result[i] = res;
            }
        }
        return result;
    }

    function strToUint(string memory _str)
        internal
        pure
        returns (uint256 res, bool err)
    {
        for (uint256 i = 0; i < bytes(_str).length; i++) {
            if (
                (uint8(bytes(_str)[i]) - 48) < 0 ||
                (uint8(bytes(_str)[i]) - 48) > 9
            ) {
                return (0, false);
            }
            res +=
                (uint8(bytes(_str)[i]) - 48) *
                10**(bytes(_str).length - i - 1);
        }
        return (res, true);
    }
}

pragma solidity ^0.8.0;

contract PowerStation is Ownable, PaymentSplitter {
    using SafeMath for uint256;

    IJoeRouter02 public traderJoeV2Router =
        IJoeRouter02(0xF491e7B69E4244ad4002BC14e878a34207E38c29);

    ERC20 power = ERC20(0x131c7afb4E5f5c94A27611f7210dfEc2215E85Ae);

    address public traderJoeV2Pair = 0x8eae6AaC525e6ec6A686f77e4751D3e8f96f6A83;
    address public devWallet = 0x6982Fc7f5596Ff7aA715895a85CA0630be8E2e4F;
    address public distributionPool =
        0x0E759Ff4f4C0735c35BF0Fe87A4Ae6602C227DA9;

    address public deadWallet = 0x000000000000000000000000000000000000dEaD;

    uint256 public rewardsFee = 60;
    uint256 public liquidityPoolFee = 0;
    uint256 private treasuryFee = 0;
    uint256 public devFee = 0;
    uint256 public totalFees = 60;

    uint256 public cashoutFee;

    bool private swapping = false;
    bool private swapLiquify = true;
    uint256 public swapTokensAmount = 30 * (10**18);

    mapping(address => bool) public _isBlacklisted;
    mapping(address => bool) public automatedMarketMakerPairs;

    struct NodeTier {
        string name;
        NODERewardManagement manager;
        uint256 cashOutFee;
    }
    mapping(string => NodeTier) public tiers;
    string[] public allTiers;

    // sliding tax
    uint16[] public slidingTaxBreakDays; // e.g. [0, 7, 15, 30, 45]
    uint16[] public slidingTaxAmounts;   // e.g. [60, 30, 20, 15, 10]

    // E V E N T S

    event UpdateTraderJoeV2Router(
        address indexed newAddress,
        address indexed oldAddress
    );

    event SetAutomatedMarketMakerPair(address indexed pair, bool indexed value);

    event LiquidityWalletUpdated(
        address indexed newLiquidityWallet,
        address indexed oldLiquidityWallet
    );

    event SwapAndLiquify(
        uint256 tokensSwapped,
        uint256 ethReceived,
        uint256 tokensIntoLiqudity
    );

    constructor(address[] memory payees, uint256[] memory shares)
        PaymentSplitter(payees, shares)
    {
        _setAutomatedMarketMakerPair(traderJoeV2Pair, true);
        addTier("FLATVERSAL", 0x0033FA9888028dD4BC5905241cbf312a8d0b21B3, 10);
        addTier("MICROSCOPIC", 0xFb717Be387F0FAB42A55772ef5CC55B4c324DabD, 10);
        addTier("HUMAN", 0x928a833b65D967Fb0B785ECdCE6ccF1a867f3C28, 10);
        addTier("SUPERHUMAN", 0xC8007751603bB3E45834A59af64190Bb618b4a83, 10);

        slidingTaxBreakDays = [0, 7, 15, 30, 45];
        slidingTaxAmounts = [60, 30, 20, 15, 10];
    }

    // P U B L I C

    function updateUniswapV2Router(address newAddress) external onlyOwner {
        require(
            newAddress != address(traderJoeV2Router),
            "TKN: The router already has that address"
        );
        emit UpdateTraderJoeV2Router(newAddress, address(traderJoeV2Router));
        traderJoeV2Router = IJoeRouter02(newAddress);
    }

    function updateSwapTokensAmount(uint256 amount) external onlyOwner {
        swapTokensAmount = amount;
    }

    function updateFutureWallet(address payable wallet) external onlyOwner {
        devWallet = wallet;
    }

    function updateRewardsWallet(address payable wallet) external onlyOwner {
        distributionPool = wallet;
    }

    function updateRewardsFee(uint256 value) external onlyOwner {
        rewardsFee = value;
        totalFees = rewardsFee.add(liquidityPoolFee).add(devFee);
    }

    function updateLiquidityFee(uint256 value) external onlyOwner {
        liquidityPoolFee = value;
        totalFees = rewardsFee.add(liquidityPoolFee).add(devFee);
    }

    function updateFutureFee(uint256 value) external onlyOwner {
        devFee = value;
        totalFees = rewardsFee.add(liquidityPoolFee).add(devFee);
    }

    function updateTreasuryFee(uint256 value) external onlyOwner {
        treasuryFee = value;
    }

    function setSlidingTaxParams(uint16[] calldata _days, uint16[] calldata _percents) external onlyOwner {
        require (_days.length ==  _percents.length, "Invalid params!");
        slidingTaxBreakDays = _days;
        slidingTaxAmounts = _percents;
    }

    function _addTier(
        string memory name,
        address manager,
        uint256 cashOutFee
    ) internal {
        tiers[name] = NodeTier({
            name: name,
            manager: NODERewardManagement(manager),
            cashOutFee: cashOutFee
        });
    }

    function addTier(
        string memory name,
        address manager,
        uint256 cashOutFee
    ) public onlyOwner {
        _addTier(name, manager, cashOutFee);
        allTiers.push(name);
    }

    function updateTier(
        string memory name,
        address manager,
        uint256 cashOutFee
    ) external onlyOwner {
        _addTier(name, manager, cashOutFee);
    }

    function removeTier(string memory name) external onlyOwner {
        delete tiers[name];

        for (uint256 i = 0; i < allTiers.length; i++) {
            if (keccak256(bytes(allTiers[i])) == keccak256(bytes(name))) {
                allTiers[i] = allTiers[allTiers.length - 1];
                allTiers.pop();
                return;
            }
        }
    }

    function setAutomatedMarketMakerPair(address pair, bool value)
        external
        onlyOwner
    {
        require(
            pair != traderJoeV2Pair,
            "The TraderJoe pair cannot be removed from automatedMarketMakerPairs"
        );

        _setAutomatedMarketMakerPair(pair, value);
    }

    function blacklistMalicious(address account, bool value)
        external
        onlyOwner
    {
        _isBlacklisted[account] = value;
    }

    function createNodeWithTokens(string memory name, string memory tierName)
        external
    {
        require(
            bytes(name).length > 2 && bytes(name).length < 32,
            "NODE CREATION: Name size invalid"
        );
        address sender = _msgSender();
        require(
            sender != address(0),
            "NODE CREATION:  creation from the zero address"
        );
        require(!_isBlacklisted[sender], "NODE CREATION: Blacklisted address");

        NodeTier memory tier = tiers[tierName];
        NODERewardManagement nodeRewardManager = tier.manager;
        uint256 nodePrice = nodeRewardManager.nodePrice();

        require(
            power.balanceOf(sender) >= nodePrice,
            "NODE CREATION: Balance too low for creation."
        );
        uint256 contractTokenBalance = power.balanceOf(address(this));
        bool swapAmountOk = contractTokenBalance >= swapTokensAmount;

        if (swapAmountOk && !swapping && !automatedMarketMakerPairs[sender]) {
            swapping = true;

            if (devFee > 0) {
                uint256 futureTokens = contractTokenBalance.mul(devFee).div(100);
                swapAndSend(devWallet, futureTokens);
            }


            if (liquidityPoolFee > 0 ) {
                 uint256 swapTokens = contractTokenBalance.mul(liquidityPoolFee).div(
                100
            );
                 swapAndLiquify(swapTokens);
            }

            if (treasuryFee > 0) {
                uint256 treasuryTokens = contractTokenBalance.mul(treasuryFee).div(
                100
            );
                swapTokensForEth(treasuryTokens);
            }

            power.transfer(distributionPool, power.balanceOf(address(this)));

            swapping = false;
        }
        power.transferFrom(sender, address(this), nodePrice);
        uint256 blocktime = block.timestamp;
        nodeRewardManager.createNodeForAddress(
            sender,
            name,
            blocktime,
            blocktime,
            0
        );
    }

    function toAsciiString(address x) internal pure returns (string memory) {
        bytes memory s = new bytes(40);
        for (uint256 i = 0; i < 20; i++) {
            bytes1 b = bytes1(uint8(uint256(uint160(x)) / (2**(8 * (19 - i)))));
            bytes1 hi = bytes1(uint8(b) / 16);
            bytes1 lo = bytes1(uint8(b) - 16 * uint8(hi));
            s[2 * i] = char(hi);
            s[2 * i + 1] = char(lo);
        }
        return string(s);
    }

    function char(bytes1 b) internal pure returns (bytes1 c) {
        if (uint8(b) < 10) return bytes1(uint8(b) + 0x30);
        else return bytes1(uint8(b) + 0x57);
    }

    function _getNodeId(
        string memory nodeName,
        uint256 blocktime,
        string memory tierName
    ) public view returns (string memory) {
        return
            string(
                abi.encodePacked(
                    nodeName,
                    uint2str(blocktime),
                    tierName,
                    toAsciiString(msg.sender)
                )
            );
    }

    function compoundInto(string memory tierName, string memory nodeName)
        external
    {
        address sender = _msgSender();
        uint256 totalRewards;
        uint totalTax;

        for (uint256 i = 0; i < allTiers.length; i++) {
            NodeTier memory tier = tiers[allTiers[i]];
            NODERewardManagement nodeRewardManager = tier.manager;
            uint256 rewards = nodeRewardManager._cashoutAllNodesReward(sender);
            totalTax += _calcTax(nodeRewardManager, sender);
            totalRewards = totalRewards.add(rewards);
        }

        _compound(sender, totalRewards, tierName, nodeName, totalTax);
    }

    function compoundTierInto(
        string memory tierName,
        string memory compoundTierName,
        string memory nodeName
    ) external {
        address sender = _msgSender();

        NodeTier memory tier = tiers[tierName];
        NODERewardManagement rewardManager = tier.manager;

        uint256 totalRewards = rewardManager._cashoutAllNodesReward(sender);
        uint tax = _calcTax(rewardManager, sender);

        _compound(sender, totalRewards, compoundTierName, nodeName, tax);
    }

    function _calcTax(NODERewardManagement tier, address sender) public view returns (uint) {
        string memory nodesLastClaimTimes = tier._getNodesLastClaimTime(sender);
        string memory nodesCreationTimes = tier._getNodesCreationTime(sender);

        uint256[] memory nodesLastClaimAt = PowerLibrary.strToArrayUint256(
            nodesLastClaimTimes,
            "#"
        );
        uint[] memory nodesCreationTime = PowerLibrary.strToArrayUint256(nodesCreationTimes, "#");
        uint256 currentTime = block.timestamp;

        uint tax = 0;

        uint i = 0;
        for (i = 0; i < nodesLastClaimAt.length; i+=1) {
            uint k = 0;
            while (currentTime - nodesLastClaimAt[i] >= slidingTaxBreakDays[k] * 86400) {
                k++;
                if (k >= slidingTaxBreakDays.length) break;
            }
            
            uint fee =  k > 0? slidingTaxAmounts[k - 1] : slidingTaxAmounts[0];

            tax += tier._getNodeRewardAmountOf(sender, nodesCreationTime[i]) * fee / 100;
        }

        return tax;
    }

    function cashoutAll(string memory tierName) public {
        address sender = _msgSender();

        NodeTier memory tier = tiers[tierName];
        NODERewardManagement nodeRewardManager = tier.manager;
        uint256 rewardAmount = nodeRewardManager._getRewardAmountOf(sender);
        require(rewardAmount > 0, "You don't have enough reward to cash out");

        uint tax = _calcTax(nodeRewardManager, sender);

        rewardAmount -= tax;
        power.transferFrom(distributionPool, sender, rewardAmount);
        nodeRewardManager._cashoutAllNodesReward(sender);
    }

    function boostReward(uint256 amount) public onlyOwner {
        if (amount > address(this).balance) amount = address(this).balance;
        payable(owner()).transfer(amount);
    }

    function changeSwapLiquify(bool newVal) public onlyOwner {
        swapLiquify = newVal;
    }

    function getNodeNumberOf(address account) public view returns (uint256) {
        uint256 nodes;

        for (uint256 i = 0; i < allTiers.length; i++) {
            NodeTier memory tier = tiers[allTiers[i]];
            NODERewardManagement nodeRewardManager = tier.manager;
            nodes = nodes.add(nodeRewardManager._getNodeNumberOf(account));
        }

        return nodes;
    }

    function getNodeNumberOf(address account, string memory tierName)
        public
        view
        returns (uint256)
    {
        NodeTier memory tier = tiers[tierName];
        NODERewardManagement nodeRewardManager = tier.manager;

        return nodeRewardManager._getNodeNumberOf(account);
    }

    function getRewardAmountOf(address account) public view returns (uint256) {
        uint256 totalRewards;

        for (uint256 i = 0; i < allTiers.length; i++) {
            NodeTier memory tier = tiers[allTiers[i]];
            NODERewardManagement nodeRewardManager = tier.manager;
            uint256 rewards = nodeRewardManager._getRewardAmountOf(account);
            totalRewards = totalRewards.add(rewards);
        }

        return totalRewards;
    }

    function getRewardAmountOf(address account, string memory tierName)
        public
        view
        returns (uint256)
    {
        NodeTier memory tier = tiers[tierName];
        NODERewardManagement nodeRewardManager = tier.manager;

        return nodeRewardManager._getRewardAmountOf(account);
    }

    function changeNodePrice(uint256 newNodePrice, string memory tierName)
        public
        onlyOwner
    {
        NodeTier memory tier = tiers[tierName];
        NODERewardManagement nodeRewardManager = tier.manager;
        nodeRewardManager._changeNodePrice(newNodePrice);
    }

    function changeRewardPerNode(uint256 newPrice, string memory tierName)
        public
        onlyOwner
    {
        NodeTier memory tier = tiers[tierName];
        NODERewardManagement nodeRewardManager = tier.manager;
        nodeRewardManager._changeRewardPerNode(newPrice);
    }

    function changeClaimTime(uint256 newTime, string memory tierName)
        public
        onlyOwner
    {
        NodeTier memory tier = tiers[tierName];
        NODERewardManagement nodeRewardManager = tier.manager;
        nodeRewardManager._changeClaimTime(newTime);
    }

    function getTotalCreatedNodes() public view returns (uint256) {
        uint256 totalNodesCreated;

        for (uint256 i = 0; i < allTiers.length; i++) {
            NodeTier memory tier = tiers[allTiers[i]];
            NODERewardManagement nodeRewardManager = tier.manager;
            uint256 created = nodeRewardManager.totalNodesCreated();
            totalNodesCreated = totalNodesCreated.add(created);
        }

        return totalNodesCreated;
    }

    // P R I V A T E

    function _setAutomatedMarketMakerPair(address pair, bool value) private {
        require(
            automatedMarketMakerPairs[pair] != value,
            "Automated market maker pair is already set to that value"
        );
        automatedMarketMakerPairs[pair] = value;

        emit SetAutomatedMarketMakerPair(pair, value);
    }

    function swapAndSend(address destination, uint256 tokens) private {
        uint256 initialETHBalance = address(this).balance;
        swapTokensForEth(tokens);
        uint256 newBalance = (address(this).balance).sub(initialETHBalance);
        payable(destination).transfer(newBalance);
    }

    function swapAndLiquify(uint256 tokens) private {
        uint256 half = tokens.div(2);

        uint256 initialBalance = address(this).balance;

        swapTokensForEth(half);

        uint256 newBalance = address(this).balance.sub(initialBalance);

        addLiquidity(half, newBalance);

        emit SwapAndLiquify(half, newBalance, half);
    }

    function swapTokensForEth(uint256 tokenAmount) private {
        address[] memory path = new address[](2);
        path[0] = address(power);
        path[1] = traderJoeV2Router.WETH();

        power.approve(address(traderJoeV2Router), tokenAmount);

        traderJoeV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );
    }

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        // approve token transfer to cover all possible scenarios
        power.approve(address(traderJoeV2Router), tokenAmount);

        // add the liquidity
        traderJoeV2Router.addLiquidityETH{value: ethAmount}(
            address(power),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            address(0),
            block.timestamp
        );
    }

    function uint2str(uint256 _i)
        internal
        pure
        returns (string memory _uintAsString)
    {
        if (_i == 0) {
            return "0";
        }
        uint256 j = _i;
        uint256 len;
        while (j != 0) {
            len++;
            j /= 10;
        }
        bytes memory bstr = new bytes(len);
        uint256 k = len;
        while (_i != 0) {
            k = k - 1;
            uint8 temp = (48 + uint8(_i - (_i / 10) * 10));
            bytes1 b1 = bytes1(temp);
            bstr[k] = b1;
            _i /= 10;
        }
        return string(bstr);
    }

    function _compound(
        address sender,
        uint256 totalRewards,
        string memory tierName,
        string memory nodeName,
        uint tax
    ) internal {
        NodeTier memory compoundTier = tiers[tierName];
        NODERewardManagement compoundNodeRewardManager = compoundTier.manager;

        uint256 nodePrice = compoundNodeRewardManager.nodePrice();
        bool isCompoundable = totalRewards >= nodePrice;

        if (!isCompoundable && totalRewards >= nodePrice.div(2)) {
            uint256 pending = nodePrice.sub(totalRewards);
            power.transferFrom(sender, address(this), pending);
            totalRewards = nodePrice;
        }

        uint256 nodesToCreate = totalRewards.div(nodePrice);
        require(nodesToCreate > 0, "Insufficient rewards to compound");

        uint256 rewardsToUser = totalRewards.sub(
            compoundNodeRewardManager.nodePrice().mul(nodesToCreate)
        );

        for (uint256 i = 0; i < nodesToCreate; i++) {
            string memory name = string(
                abi.encodePacked(nodeName, uint2str(i))
            );
            uint256 createdAt = block.timestamp + i;
            compoundNodeRewardManager.createNodeForAddress(
                sender,
                name,
                createdAt,
                createdAt,
                0
            );
        }

        if (rewardsToUser - tax > 0) {
            rewardsToUser -= tax;

            power.transferFrom(distributionPool, sender, rewardsToUser);
        }
    }
}