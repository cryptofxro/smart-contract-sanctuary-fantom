pragma solidity ^0.8.16;

import '@openzeppelin/contracts/access/AccessControl.sol';
import './interfaces/IFraxlendPair.sol';
import './interfaces/IFraxlendPairCore.sol';
import 'hardhat/console.sol';

contract FujinManager is AccessControl {
  event LendingDeployed(address lendingAddress, address deployer);
  event CapManagerDeployed(address capManagerAddress, address deployer);

  address[] public lendings;
  mapping(address => address) public lendingDeployer;
  mapping(address => address[]) public deployerLendings;

  address[] public capManagers;

  bytes public lendingCreationCode;
  bytes public fTokenCreationCode;
  bytes public capManagerCreationCode;

  uint256 public pauseIndex;
  bool public globalPause;
  mapping(uint256 => mapping(address => bool)) public isPauseExcluded; // pauseIndex => lending => bool

  error ZeroAddress();
  error ProtocolOrOwnerOnly();

  bytes32 public SETTER_ROLE = keccak256('SETTER_ROLE');
  bytes32 public GLOBAL_PAUSER_ROLE = keccak256('GLOBAL_PAUSER_ROLE');
  bytes32 public GLOBAL_UNPAUSER_ROLE = keccak256('GLOBAL_UNPAUSER_ROLE');

  constructor(
    address _setter,
    address _globalPauser,
    address _globalUnpauser,
    address _admin
  ) {
    if (_setter == address(0) || _admin == address(0)) {
      revert ZeroAddress();
    }

    _grantRole(SETTER_ROLE, _setter);
    _grantRole(DEFAULT_ADMIN_ROLE, _admin);
    _grantRole(GLOBAL_PAUSER_ROLE, _globalPauser);
    _grantRole(GLOBAL_UNPAUSER_ROLE, _globalUnpauser);
  }

  function userDeployedLendingsCount(address deployer) external view returns (uint256) {
    return deployerLendings[deployer].length;
  }

  function deployedLendingsCount() external view returns (uint256) {
    return lendings.length;
  }

  function deployedCapManagersCount() external view returns (uint256) {
    return capManagers.length;
  }

  function isLendingPaused(address lending) external view returns (bool) {
    return globalPause && !isPauseExcluded[pauseIndex][lending];
  }

  event AllLendingsPaused(uint256 pauseIndex);

  function pauseAllLendings() external onlyRole(GLOBAL_PAUSER_ROLE) {
    globalPause = true;
    pauseIndex++;
    emit AllLendingsPaused(pauseIndex);
  }

  event AllLendingsUnpaused(uint256 pauseIndex);

  function unpauseAllLendings() external onlyRole(GLOBAL_UNPAUSER_ROLE) {
    globalPause = false;
    emit AllLendingsUnpaused(pauseIndex);
  }

  event ExcludedFromGlobalPause(address lending, uint256 pauseIndex);

  function excludeFromGlobalPause(address lending) external {
    IFraxlendPair lendingContract = IFraxlendPair(lending);
    if (msg.sender != lendingContract.COMPTROLLER_ADDRESS() && msg.sender != lendingContract.owner()) {
      revert ProtocolOrOwnerOnly();
    }

    isPauseExcluded[pauseIndex][lending] = true;
    emit ExcludedFromGlobalPause(lending, pauseIndex);
  }

  function getLendingImmutables(
    address deployer,
    address circuitBreaker,
    address comptrollerAddress,
    address timeLockAddress
  ) external pure returns (bytes memory immutables) {
    immutables = abi.encode(deployer, circuitBreaker, comptrollerAddress, timeLockAddress);
  }

  function deployLending(
    string memory _name,
    address _rateContract,
    address _capManager,
    bytes memory _immutables,
    uint256 _liquidationFee,
    bool _isBorrowerWhitelistActive,
    bool _isLenderWhitelistActive
  ) external returns (address lendingAddress) {
    bytes memory bytecode = abi.encodePacked(
      lendingCreationCode,
      abi.encode(
        _name,
        address(this),
        _rateContract,
        _capManager,
        _immutables,
        _liquidationFee,
        _isBorrowerWhitelistActive,
        _isLenderWhitelistActive
      )
    );

    bytes32 salt = keccak256(abi.encode(block.number, msg.sender, _name));

    assembly {
      lendingAddress := create2(0, add(bytecode, 32), mload(bytecode), salt)
    }

    lendings.push(lendingAddress);
    lendingDeployer[lendingAddress] = msg.sender;
    deployerLendings[msg.sender].push(lendingAddress);

    emit LendingDeployed(lendingAddress, msg.sender);
  }

  function deployCapManager(address setter, address admin) external returns (address capManagerAddress) {
    bytes memory bytecode = abi.encodePacked(capManagerCreationCode, abi.encode(setter, admin));

    bytes32 salt = keccak256(abi.encode(block.number, msg.sender));

    assembly {
      capManagerAddress := create2(0, add(bytecode, 32), mload(bytecode), salt)
    }

    capManagers.push(capManagerAddress);

    emit CapManagerDeployed(capManagerAddress, msg.sender);
  }

  error OnlyLendingDeployer(address fraxlendPairCore);

  modifier onlyLendingDeployer(address lendingAddress) {
    if (msg.sender != IFraxlendPairCore(lendingAddress).DEPLOYER_ADDRESS()) {
      revert OnlyLendingDeployer(lendingAddress);
    }
    _;
  }

  function defineLendingCollaterals(
    address lendingAddress,
    address[] memory tokens,
    address[] memory oracles,
    uint256[] memory ltvs
  ) external onlyLendingDeployer(lendingAddress) {
    IFraxlendPairCore.Collateral[] memory collaterals = new IFraxlendPairCore.Collateral[](tokens.length);

    for (uint256 i; i < tokens.length; i++) {
      address token = tokens[i];
      address oracle = oracles[i];
      uint256 ltv = ltvs[i];

      collaterals[i] = IFraxlendPairCore.Collateral({
        token: IERC20(token),
        tokenDecimals: IERC20Metadata(token).decimals(),
        oracle: AggregatorV3Interface(oracle),
        oracleDecimals: AggregatorV3Interface(oracle).decimals(),
        ltv: ltv,
        total: 0
      });
    }

    IFraxlendPairCore(lendingAddress).defineCollaterals(collaterals);
  }

  function defineLendingAssets(
    address lendingAddress,
    address[] memory tokens,
    address[] memory oracles
  ) external onlyLendingDeployer(lendingAddress) {
    IFraxlendPairCore.Asset[] memory assets = new IFraxlendPairCore.Asset[](tokens.length);
    for (uint256 i; i < tokens.length; i++) {
      address token = tokens[i];
      address oracle = oracles[i];

      string memory fTokenName = string.concat(
        'Fujin ',
        IFraxlendPairCore(lendingAddress).name(),
        ' ',
        IERC20Metadata(token).name()
      );
      string memory fTokenSymbol = string.concat(IFraxlendPairCore(lendingAddress).name(), ' f', IERC20Metadata(token).symbol());

      bytes memory bytecode = abi.encodePacked(fTokenCreationCode, abi.encode(fTokenName, fTokenSymbol, lendingAddress, token));

      bytes32 salt = keccak256(abi.encode(block.number, i));
      address fTokenAddress;
      assembly {
        fTokenAddress := create2(0, add(bytecode, 32), mload(bytecode), salt)
      }

      assets[i] = IFraxlendPairCore.Asset({
        token: IERC20(token),
        tokenDecimals: IERC20Metadata(token).decimals(),
        oracle: AggregatorV3Interface(oracle),
        oracleDecimals: AggregatorV3Interface(oracle).decimals(),
        totalAsset: VaultAccount(0, 0),
        totalBorrow: VaultAccount(0, 0),
        fToken: IFToken(fTokenAddress),
        currentRateInfo: IFraxlendPairCore.CurrentRateInfo({lastBlock: 0, feeToProtocolRate: 0, lastTimestamp: 0, ratePerSec: 0})
      });
    }

    IFraxlendPairCore(lendingAddress).defineAssets(assets);
  }

  function setLendingCreationCode(bytes memory _creationCode, bool reset) external onlyRole(SETTER_ROLE) {
    if (reset) lendingCreationCode = bytes('');
    lendingCreationCode = abi.encodePacked(lendingCreationCode, _creationCode);
    // todo event;
  }

  function setFTokenCreationCode(bytes memory _creationCode) external onlyRole(SETTER_ROLE) {
    fTokenCreationCode = _creationCode;
    // todo event;
  }

  function setCapManagerCreationCode(bytes memory _creationCode) external onlyRole(SETTER_ROLE) {
    capManagerCreationCode = _creationCode;
    // todo event;
  }
}

