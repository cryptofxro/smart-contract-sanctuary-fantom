pragma solidity ^0.8.4;

import "../heroMints/FantomHeroes.sol";
import "../../tokens/HeroToken.sol";

import "@openzeppelin/contracts/security/Pausable.sol";
import "hardhat/console.sol";

contract HeroTraining is AccessControl, Pausable {
    // *************************************************
    // ROLES
    // *************************************************
    bytes32 public constant HERO_ADMIN_ROLE = keccak256("HERO_ADMIN_ROLE");

    // *************************************************
    // Interfaces
    // *************************************************
    HeroFactory private _heroFactory;
    HeroToken internal _heroToken;
    FantomHeroes internal _FantomHeroes;

    // *************************************************
    // Contract Params
    // *************************************************
    // costs
    mapping(string => uint256) public _trainingCosts;
    mapping(string => uint256) public _trainingAmounts;

    // other variables
    uint256 public _trainingCountLimit = 100;
    uint256 public EXP_PROC_PROB = 3;

    // *************************************************
    // Events
    // *************************************************
    event HeroTrainEvent(
        uint256 _heroId,
        uint256 _geneId,
        uint256 _trainCount,
        uint256 _newPowerLevel,
        uint256 _timestamp
    );
    event HeroExperimentalProcedureEvent(
        uint256 _heroId,
        uint256 _geneId,
        uint256 _trainCount,
        uint256 _newPowerLevel,
        bool _isIncrease,
        uint256 _timestamp
    );

    constructor(
        address _heroFactoryAddress,
        address _heroTokenAddress,
        address _fantomHeroesAddress
    ) {
        // set up interfaces
        _heroFactory = HeroFactory(_heroFactoryAddress);
        _heroToken = HeroToken(_heroTokenAddress);
        _FantomHeroes = FantomHeroes(_fantomHeroesAddress);

        // setup roles
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(HERO_ADMIN_ROLE, msg.sender);

        // setup costs
        _trainingCosts["basic"] = 30;
        _trainingCosts["experimental"] = 40;

        // setup amounts
        _trainingAmounts["basic"] = 100;
        _trainingAmounts["experimentalUp"] = 200;
        _trainingAmounts["experimentalDown"] = 100;
    }

    // *************************************************
    // Modifiers
    // *************************************************
    modifier onlyHeroOwner(uint256 _heroId) {
        require(_FantomHeroes.ownerOf(_heroId) == msg.sender, "Must own hero");
        _;
    }

    // *************************************************
    // Main Training Functions
    // *************************************************
    function trainHero(
        uint256 _heroId,
        uint256 _geneIndex,
        uint256 _numTimes
    ) public whenNotPaused onlyHeroOwner(_heroId) {
        // get hero
        SharedStructs.Hero memory hero = _heroFactory.getHero(_heroId);

        require(
            hero.trainingCounts[_geneIndex] + _numTimes <= _trainingCountLimit,
            "Training count limit reached"
        );

        uint256 cost = _calculateTrainingCost(hero, _numTimes, _geneIndex);
        require(_heroToken.balanceOf(msg.sender) >= cost, "Not enough $HERO");

        _heroToken.burnFrom(msg.sender, cost);

        uint256 trainAmount = _trainingAmounts["basic"] * _numTimes;

        _heroFactory.updateGenePower(_heroId, _geneIndex, trainAmount, false);
        _heroFactory.updateTrainingCount(_heroId, _geneIndex, _numTimes);

        emit HeroTrainEvent(
            _heroId,
            _geneIndex,
            hero.trainingCounts[_geneIndex] + _numTimes,
            hero.powerlevels[_geneIndex] + trainAmount,
            block.timestamp
        );
    }

    function experimentalProcedure(
        uint256 _heroId,
        uint256 _geneIndex,
        bytes32 _seed
    ) public whenNotPaused onlyHeroOwner(_heroId) {
        // get hero
        SharedStructs.Hero memory hero = _heroFactory.getHero(_heroId);

        require(
            hero.trainingCounts[_geneIndex] + 1 <= _trainingCountLimit,
            "Training count limit reached"
        );

        // if not admin, get cost and burn payment tokens
        uint256 cost = (hero.trainingCounts[_geneIndex] + 1) *
            _getTrainingCost("experimental");

        require(_heroToken.balanceOf(msg.sender) >= cost, "Not enough $HERO");

        _heroToken.burnFrom(msg.sender, cost);

        uint256 randomNum = _generateRandomUint(_seed);
        uint256 trainAmount;
        uint256 newPowerLevel;

        if (randomNum % EXP_PROC_PROB == 0) {
            trainAmount = _trainingAmounts["experimentalUp"];
            _heroFactory.updateGenePower(
                _heroId,
                _geneIndex,
                trainAmount,
                false
            );
            newPowerLevel = hero.powerlevels[_geneIndex] + trainAmount;
        } else {
            trainAmount = _trainingAmounts["experimentalDown"];
            _heroFactory.updateGenePower(
                _heroId,
                _geneIndex,
                trainAmount,
                true
            );

            newPowerLevel = hero.powerlevels[_geneIndex] > trainAmount
                ? hero.powerlevels[_geneIndex] - trainAmount
                : 0;
        }

        _heroFactory.updateTrainingCount(_heroId, _geneIndex, 1);

        emit HeroExperimentalProcedureEvent(
            _heroId,
            _geneIndex,
            hero.trainingCounts[_geneIndex] + 1,
            newPowerLevel,
            randomNum % EXP_PROC_PROB == 0,
            block.timestamp
        );
    }

    // *************************************************
    // Pricing
    // *************************************************
    function _getTrainingCost(string memory _trainingType)
        internal
        view
        returns (uint256)
    {
        return _trainingCosts[_trainingType] * 10**18;
    }

    function _calculateTrainingCost(
        SharedStructs.Hero memory _hero,
        uint256 _numTimes,
        uint256 _geneIndex
    ) internal view returns (uint256) {
        uint256 cost = 0;

        for (uint256 i = 0; i < _numTimes; i++) {
            cost +=
                (_hero.trainingCounts[_geneIndex] + i + 1) *
                _getTrainingCost("basic");
        }

        return cost;
    }

    // *************************************************
    // Getters/Setters
    // *************************************************
    function updateTrainingCost(string memory _item, uint256 _newCost)
        public
        whenNotPaused
        onlyRole(HERO_ADMIN_ROLE)
    {
        _trainingCosts[_item] = _newCost;
    }

    function updateTrainingAmount(string memory _item, uint256 _newAmount)
        public
        whenNotPaused
        onlyRole(HERO_ADMIN_ROLE)
    {
        _trainingAmounts[_item] = _newAmount;
    }

    function updateExperimentalProcProb(uint256 _newProb)
        public
        whenNotPaused
        onlyRole(HERO_ADMIN_ROLE)
    {
        EXP_PROC_PROB = _newProb;
    }

    function updateTrainingCountLimit(uint256 _newLimit)
        public
        whenNotPaused
        onlyRole(HERO_ADMIN_ROLE)
    {
        _trainingCountLimit = _newLimit;
    }

    // *************************************************
    // Access Control
    // *************************************************
    function makeAdmin(address _newAdmin) public onlyRole(DEFAULT_ADMIN_ROLE) {
        grantRole(HERO_ADMIN_ROLE, _newAdmin);
    }

    function removeAdmin(address _newAdmin)
        public
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        revokeRole(HERO_ADMIN_ROLE, _newAdmin);
    }

    function userIsAdmin(address _user) public view returns (bool) {
        return hasRole(HERO_ADMIN_ROLE, _user);
    }

    function pause() public onlyRole(HERO_ADMIN_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(HERO_ADMIN_ROLE) {
        _unpause();
    }

    // *************************************************
    // Other
    // *************************************************
    function _generateRandomUint(bytes32 _seed) public view returns (uint256) {
        uint256 _rnd = uint256(
            keccak256(
                abi.encodePacked(
                    uint256(_seed) +
                        block.timestamp +
                        block.difficulty +
                        ((
                            uint256(keccak256(abi.encodePacked(block.coinbase)))
                        ) / (block.timestamp)) +
                        block.gaslimit +
                        ((uint256(keccak256(abi.encodePacked(msg.sender)))) /
                            (block.timestamp)) +
                        block.number
                )
            )
        );
        return _rnd;
    }

    function _sliceNumber(
        uint256 _n,
        uint256 _nbits,
        uint256 _offset
    ) private pure returns (uint256) {
        // mask is made by shifting left an offset number of times
        uint256 mask = uint256((2**_nbits) - 1) << _offset;
        // AND n with mask, and trim to max of _nbits bits
        return uint256((_n & mask) >> _offset);
    }
}

