library PowerLib {

    function _calculateReward(NodeEntity memory node, uint256 claimTime, uint256 dailyReward)
        external
        view
        returns (uint256)
    {
        //if (node.signTime + (_timeToNextSignature * 2) <  block.timestamp) return 0;

        uint256 lastClaimTime = node.lastClaimTime;
        if (lastClaimTime == 0) lastClaimTime = node.creationTime;

        uint256 _claims = (block.timestamp - node.lastClaimTime) / claimTime;

       /*  if (node.signTime + _timeToNextSignature <  block.timestamp) {
            _claims = (node.signTime + _timeToNextSignature - lastClaim) / claimTime;
        } */

        return dailyReward * _claims;
    }

    function uint2str(uint256 _i)
        external
        pure
        returns (string memory _uintAsString)
    {
        if (_i == 0) {
            return "0";
        }
        uint256 j = _i;
        uint256 len;
        while (j != 0) {
            len++;
            j /= 10;
        }
        bytes memory bstr = new bytes(len);
        uint256 k = len;
        while (_i != 0) {
            k = k - 1;
            uint8 temp = (48 + uint8(_i - (_i / 10) * 10));
            bytes1 b1 = bytes1(temp);
            bstr[k] = b1;
            _i /= 10;
        }
        return string(bstr);
    }

    function removeFromArray(string[] storage array,uint256 index) external returns(string[] storage) {
        require(array.length > index, "PL:1");
        // move all elements to the left, starting from the `index + 1`
        for (uint256 i = index; i < array.length - 1; i++) {
            array[i] = array[i+1];
        }
        array.pop(); // delete the last item
        return array;
    }

