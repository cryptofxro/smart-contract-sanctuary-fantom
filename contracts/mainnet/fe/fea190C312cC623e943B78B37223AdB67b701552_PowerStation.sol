        string memory tier = _nodesOfUser[sender][newName].tier;


        for (uint256 i = 0; i < _nameNodesOfUser[sender][tier].length; i++) {
            if (keccak256(abi.encodePacked(_nameNodesOfUser[sender][tier][i])) == keccak256(abi.encodePacked(oldName))) {
                _nameNodesOfUser[sender][tier][i] = newName;
                break;
            }
        }
    }



    /// Node claim function

    function _getNodeRewardAmountOf(address account, string memory nodeName)
        public
        view
        returns (uint256)
    {
        NodeEntity memory _node = _nodesOfUser[account][nodeName];
        NodeType memory tierSpec = nodeCollection[_node.tier];

        return (PowerLib._calculateReward(_node, tierSpec.claimTime, tierSpec.reward)).sub(_node.rewardTaken);
    }

