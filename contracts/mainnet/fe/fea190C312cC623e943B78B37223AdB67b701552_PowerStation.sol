    function compound(string memory newNodeName, string memory newNodeTier, uint256 amountOfNodes, string[] memory nodesNames, uint256[] memory amount)
        external noBlacklisted(_msgSender())
    {
        address sender = _msgSender();

        require(
            nodeCollection[newNodeTier].claimTime != 0,
            "ST:9"
        );

        require(
            nodesNames.length == amount.length,
            "ST:15"
        );
        
        uint256 nodePrice = nodeCollection[newNodeTier].price * amountOfNodes;
        uint256 remainAmount = nodePrice;

        for (uint256 i; i < nodesNames.length; i++) {
            if (_nodesOfUser[sender][nodesNames[i]].creationTime != 0 && _getNodeRewardAmountOf(sender, nodesNames[i]) >= amount[i]) {
                _nodesOfUser[sender][nodesNames[i]].rewardTaken += amount[i];
                remainAmount -= amount[i];
            }
        }

        if (remainAmount != 0) {
            require(false, 'ST:17');
        }

        uint256 createdAt = block.timestamp;

        for (uint256 i = 0; i < amountOfNodes; i++) {
            string memory name = string(
                abi.encodePacked(newNodeName, PowerLib.uint2str(i))
            );

            _createNode(
                name,
                newNodeTier,
                sender,
                createdAt,
                createdAt,
                0
            );
        }
    }

