    function _calcTax(string memory tier, address sender, uint256 rewards) public view returns (uint) {
        uint256 currentTime = block.timestamp;

        if (_claimDate[sender][tier] == 0) return rewards * 10 / 100;

        uint k = 0;
        while (currentTime - _claimDate[sender][tier] >= slidingTaxBreakDays[k] * 86400) {
            k++;
            if (k >= slidingTaxBreakDays.length) break;
        }
        
        uint fee =  k > 0? slidingTaxAmounts[k - 1] : slidingTaxAmounts[0];

        return rewards * fee / 100;
    }

    function getRewardAmountOf(address account, string memory tier)
        public
        view
        returns (uint256)
    {
        /* Todo: Do optimisation with local import */

        uint256 rewardCount;
        string[] storage nodeNames = _nameNodesOfUser[account][tier];
        NodeEntity memory node;
        NodeType memory tierSpec = nodeCollection[tier];

        for (uint256 i = 0; i < _nameNodesOfUser[account][tier].length; i++) {
            node = _nodesOfUser[account][nodeNames[i]];
            rewardCount += PowerLib._calculateReward(node, tierSpec.claimTime, tierSpec.reward).sub(
                node.rewardTaken
            );
        }

        return rewardCount;
    }

    function _cashoutAllNodesReward(address account, string memory tier)
        internal
        returns (uint256)
    {
        /* Todo: Do optimisation with local import */
        
        uint256 rewardCount;
        string[] storage nodeNames = _nameNodesOfUser[account][tier];
        NodeEntity memory node;
        NodeType memory tierSpec = nodeCollection[tier];

        for (uint256 i = 0; i < _nameNodesOfUser[account][tier].length; i++) {
            node = _nodesOfUser[account][nodeNames[i]];

            //if (node.signTime != 0 && node.signTime + _timeToNextSignature < block.timestamp) continue;

            rewardCount += PowerLib._calculateReward(node, tierSpec.claimTime, tierSpec.reward).sub(
                node.rewardTaken
            );
            
            node.lastClaimTime = block.timestamp;
            node.rewardTaken = 0;
            _nodesOfUser[account][nodeNames[i]] = node;
        }
        return rewardCount;
    }

    function cashoutAll(string memory tier) external noBlacklisted(_msgSender()) {
        address sender = _msgSender();

        uint256 rewardAmount = _cashoutAllNodesReward(sender, tier);//getRewardAmountOf(sender, tier);
        require(rewardAmount > 0, "ST:12");

        uint tax = _calcTax(tier, sender, rewardAmount);
        //uint  tax = 0;
        rewardAmount -= tax;
        IERC20(power).transferFrom(distributionPool, sender, rewardAmount);
        _claimDate[sender][tier] = block.timestamp;
    }

    function haveAllNodeTiers(address user) external view returns (bool) {
        for (uint256 i; i < nodeCollectionCount.length; i++) {
            if (_nameNodesOfUser[user][nodeCollectionCount[i]].length == 0) return false;
        }
        return true;
    }

    function fixTier(address account, string memory tier) external onlyOwner {
        string[] storage nodeNames = _nameNodesOfUser[account][tier];
        NodeEntity memory node;
        NodeType memory tierSpec = nodeCollection[tier];

        for (uint256 i = 0; i < _nameNodesOfUser[account][tier].length; i++) {
            node = _nodesOfUser[account][nodeNames[i]];

            if (node.signTime != 0 && node.signTime + _timeToNextSignature < block.timestamp) continue;

            node.rewardTaken = PowerLib._calculateReward(node, tierSpec.claimTime, tierSpec.reward);
        
            _nodesOfUser[account][nodeNames[i]] = node;
        }
    }


