    function _createNode(
        string memory name,
        string memory tier,
        address user,
        uint256 creationTime,
        uint256 lastClaimTime,
        uint256 rewardTaken
    ) internal {
        if (_nodesOfUser[user][name].creationTime != 0) {
            uint256 i;
            while (_nodesOfUser[user][string(abi.encodePacked(name, PowerLib.uint2str(i)))].creationTime != 0) {
                i++;
            }

            name = string(abi.encodePacked(name, PowerLib.uint2str(i)));
        }

        if (_claimDate[user][tier] == 0) _claimDate[user][tier] = block.timestamp;

        _nameNodesOfUser[user][tier].push(name);
        _nodesOfUser[user][name] = NodeEntity(
            creationTime,
            lastClaimTime,
            rewardTaken,
            tier,
            block.timestamp
        );
        nodeCollection[tier].totalCreated += 1;
    }

