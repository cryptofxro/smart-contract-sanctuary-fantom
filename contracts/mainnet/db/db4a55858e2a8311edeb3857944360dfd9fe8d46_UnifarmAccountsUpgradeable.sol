        require(_token != address(0), "INVALID_ADDRESS");
        require(_amount > 0, "ZERO_AMOUNT");

        uint len = spamProtectionTokens.length;
        uint8 count;
        for(uint256 i = 0; i < len; i++) {
            // token should be allowed by the admin
            if(spamProtectionTokens[i] == _token) {
