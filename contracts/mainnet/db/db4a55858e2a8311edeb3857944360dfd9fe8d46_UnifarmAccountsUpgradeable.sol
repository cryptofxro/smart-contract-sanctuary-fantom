        require(_token != address(0), "INVALID_ADDRESS");

        uint len = spamProtectionTokens.length;
        for(uint256 i = 0; i < len; i++) {
            require(spamProtectionTokens[i] != _token, "TOKEN_ALREADY_EXISTS");
        }
        spamProtectionTokens.push(_token);

