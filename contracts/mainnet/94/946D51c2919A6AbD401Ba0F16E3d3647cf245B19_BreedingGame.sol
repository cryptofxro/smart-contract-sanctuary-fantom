        string memory json = string(abi.encodePacked("["));
        string memory svg = string(
            abi.encodePacked(
                '<svg width="100%" height="100%" version="1.1" viewBox="0 0 40 40" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">'
