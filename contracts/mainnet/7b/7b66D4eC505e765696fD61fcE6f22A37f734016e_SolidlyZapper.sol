pragma solidity >=0.7.0;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./libraries/Babylonian.sol";
import "./libraries/LowGasSafeMath.sol";
import "./interfaces/IUniswapV2Router01.sol";
import "./interfaces/IUniswapV2Pair.sol";
import "./interfaces/IWETH.sol";
import "./interfaces/IChef.sol";
import "./interfaces/IERC20Decimals.sol";

import "hardhat/console.sol";


contract SolidlyZapper {
    using LowGasSafeMath for uint256;
    using SafeERC20 for IERC20;

    
    IUniswapRouterSolidly public immutable router;
    address public immutable WETH;
    uint256 public constant minimumAmount = 1000;
    
    constructor(address _router, address _wrappedNative) {
        router = IUniswapRouterSolidly(_router);
        WETH = _wrappedNative;
    }

    receive() external payable {
        assert(msg.sender == WETH);
    }

    function zapIn(address masterChef, uint256 tokenAmountOutMin, address tokenIn, uint256 tokenInAmount, bool deposit) external payable {
        if(msg.value > 0){
            require(msg.value >= minimumAmount, 'minamount');
            IWETH(WETH).deposit{value: msg.value}();
            tokenIn = WETH;
        } else {
            require(tokenInAmount >= minimumAmount, 'minamount');
            require(IERC20(tokenIn).allowance(msg.sender, address(this)) >= tokenInAmount, 'allowance');
            IERC20(tokenIn).safeTransferFrom(msg.sender, address(this), tokenInAmount);
        }

        (IUniswapV2Pair pair) = _getPair(masterChef);
        IChef _masterChef = IChef(masterChef);

        (uint256 reserveA, uint256 reserveB,) = pair.getReserves();
        require(reserveA > minimumAmount && reserveB > minimumAmount, 'low liquidity');

        bool isInputA = pair.token0() == tokenIn;
        require(isInputA || pair.token1() == tokenIn, 'tokenIn do not exists');
      
        
        address[] memory path = new address[](2);
        path[0] = tokenIn;
        path[1] = isInputA ? pair.token1() : pair.token0() ;
                
        uint256 _balance = IERC20(tokenIn).balanceOf(address(this));
        uint256 swapAmountIn;
        
        if (isInputA) {
            swapAmountIn = _getSwapAmount(pair, _balance, reserveA, reserveB, path[0], path[1]);
        } else {
            swapAmountIn = _getSwapAmount(pair, _balance, reserveB, reserveA, path[0], path[1]);
        }
        
        _approveTokenIfNeeded(swapAmountIn, path[0], address(router));
        uint256[] memory swapedAmounts = router
            .swapExactTokensForTokensSimple(swapAmountIn, tokenAmountOutMin, path[0], path[1], pair.stable(), address(this), block.timestamp);

        _approveTokenIfNeeded(swapedAmounts[1], path[1], address(router));
        (,, uint256 amountLiquidity) = router
            .addLiquidity(path[0], path[1], pair.stable(), _balance.sub(swapedAmounts[0]), swapedAmounts[1], 1, 1, address(this), block.timestamp);
        
        if(deposit){
            _approveTokenIfNeeded(amountLiquidity, address(pair), address(_masterChef));
            _masterChef.deposit(amountLiquidity, msg.sender);
        } else {
            IERC20(_masterChef.lpToken()).safeTransfer(msg.sender, amountLiquidity);
        }
        _returnAssets(path);
    }

    function _returnAssets(address[] memory tokens) private {
        uint256 balance;
        for (uint256 i; i < tokens.length; i++) {
            balance = IERC20(tokens[i]).balanceOf(address(this));
            if (balance > 0) {
                if (tokens[i] == WETH) {
                    IWETH(WETH).withdraw(balance);
                    (bool success,) = msg.sender.call{value: balance}(new bytes(0));
                    require(success, 'ETH transfer failed');
                } else {
                    IERC20(tokens[i]).safeTransfer(msg.sender, balance);
                }
            }
        }
    }

    function _getPair(address _chef) private view returns (IUniswapV2Pair pair) {
        address lpToken = IChef(_chef).lpToken();
        pair = IUniswapV2Pair(lpToken);
    }

    function _getSwapAmount(IUniswapV2Pair pair, uint256 amountIn, uint256 reserveA, uint256 reserveB, address tokenA, address tokenB) private view returns (uint256 swapAmount) {
        uint256 toSwap = amountIn / 2;

        if (pair.stable()) {
            swapAmount = _getStableSwap(pair, amountIn, toSwap, tokenA, tokenB);
        } else {
            uint256 nominator = pair.getAmountOut(toSwap, tokenA);
            uint256 denominator = toSwap * reserveB.sub(nominator) / reserveA.add(toSwap);
            swapAmount = amountIn.sub(Babylonian.sqrt(toSwap * toSwap * nominator / denominator));
        }
    }

    function _getStableSwap(IUniswapV2Pair pair, uint256 amountIn, uint256 toSwap, address tokenA, address tokenB) private view returns (uint256 swapAmount) {
        uint out = pair.getAmountOut(toSwap, tokenA);
        (uint amountA, uint amountB,) = router.quoteAddLiquidity(tokenA, tokenB, pair.stable(), toSwap, out);
                
        amountA = amountA * 1e18 / 10**IERC20Decimals(tokenA).decimals();
        amountB = amountB * 1e18 / 10**IERC20Decimals(tokenB).decimals();
        out = out * 1e18 / 10**IERC20Decimals(tokenB).decimals();
        toSwap = toSwap * 1e18 / 10**IERC20Decimals(tokenA).decimals();  
        uint ratio = out * 1e18 / toSwap * amountA / amountB; 
        return amountIn * 1e18 / (ratio + 1e18);
    }

    function estimateSwap(address lpToken, address tokenIn, uint256 fullInvestmentIn) public view returns(uint256 swapAmountIn, uint256 swapAmountOut, address swapTokenOut) {
        checkWETH();
        (IUniswapV2Pair pair) = _getPair(lpToken);

        bool isInputA = pair.token0() == tokenIn;
        require(isInputA || pair.token1() == tokenIn, 'wrong in token');

        (uint256 reserveA, uint256 reserveB,) = pair.getReserves();
        (reserveA, reserveB) = isInputA ? (reserveA, reserveB) : (reserveB, reserveA);

        swapTokenOut = isInputA ? pair.token1() : pair.token0();
        swapAmountIn = _getSwapAmount(pair, fullInvestmentIn, reserveA, reserveB, tokenIn, swapTokenOut);
        swapAmountOut = pair.getAmountOut(swapAmountIn, tokenIn); 
    }

    function checkWETH() public view returns (bool isValid) {
        isValid = WETH == router.wftm();
        require(isValid, 'WETH address not matching Router.weth()');
    }

    function _approveTokenIfNeeded(uint256 amount, address token, address spender) private {
        if (IERC20(token).allowance(address(this), spender) == 0 || IERC20(token).allowance(address(this), spender)  < amount) {
            IERC20(token).safeApprove(spender, type(uint256).max);
        }
    }

}

