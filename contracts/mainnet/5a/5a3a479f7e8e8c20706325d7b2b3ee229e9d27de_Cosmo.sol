    using SafeMath for uint256;

    // Initial distribution for the first 24h genesis pools
    uint256 public constant INITIAL_GENESIS_POOL_DISTRIBUTION = 3000 ether;
    // Initial distribution for the day 2-5 COSMO-WFTM LP -> COSMO pool
