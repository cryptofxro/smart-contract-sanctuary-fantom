// File contracts/utils/FeeManager.sol

//UNLICENSED

pragma solidity 0.8.6;

abstract contract FeeManager is StratManager {
    uint constant public MAX_FEE = 10000;
    uint constant public TOTAL_FEE_CAP = 2500;
    uint constant public WITHDRAWAL_FEE_CAP = 200;
    uint constant public WITHDRAWAL_MAX = 10000;
    uint public withdrawalFee = 10;

    uint public partnerFee = 0;
    address public partnerAddress = address(0);
    address public yeastAddress = address(0);
    uint public yeastFee = MAX_FEE - partnerFee;

    uint public totalFee = 500; // Fee % of rewards

    function setWithdrawalFee(uint256 _fee) public onlyManager {
        require(_fee <= WITHDRAWAL_FEE_CAP, "!cap");

        withdrawalFee = _fee;
    }
    
    function setPartnetFee(uint256 _fee) public onlyManager {
        require(_fee <= MAX_FEE, "!cap");

        partnerFee = _fee;
        yeastFee = MAX_FEE - partnerFee;
    }

    function setTotalFee(uint256 _fee) public onlyManager {
        require(_fee <= TOTAL_FEE_CAP, "!cap");

        totalFee = _fee;
    }

    function setYeastAddress(address user) public onlyManager {
        yeastAddress = user;
    }

    function setPartnerAddress(address user) public onlyManager {
        partnerAddress = user;
    }
}


