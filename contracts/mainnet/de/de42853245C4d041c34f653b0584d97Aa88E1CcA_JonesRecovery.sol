pragma solidity 0.6.12;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/SafeERC20.sol";
import "./interfaces/IUniswapV2Router.sol";
import "./owner/Operator.sol";
import "./distribution/DJonesRewardPool.sol";
import "./DummyToken.sol";
import "./Jones.sol";
import "./DJones.sol";

contract JonesRecovery is Operator {
    using SafeERC20 for IERC20;

    Jones public constant jones = Jones(0x01f793Ec4dB0ffAf350526ba7828D93C180dd97E);

    IUniswapV2Router public constant spookyswapRouter = IUniswapV2Router(0xF491e7B69E4244ad4002BC14e878a34207E38c29);

    DJonesRewardPool public djonesPool;

    uint256 public pid;

    DummyToken public farmingToken;

    IERC20 public djones;

    constructor(DJonesRewardPool _rewardPool, uint256 _pid) public {
        djonesPool = _rewardPool;
        pid = _pid;
        farmingToken = _createRecoveryToken();

        djones = djonesPool.tshare();
    }

    function startRecovery() public onlyOperator {
        uint256 recoveryTokens = farmingToken.balanceOf(address(this));

        farmingToken.approve(address(djonesPool), 0);
        farmingToken.approve(address(djonesPool), recoveryTokens);

        djonesPool.deposit(pid, recoveryTokens);
    }

    function pushPeg() public {
        djonesPool.deposit(pid, 0);

        uint256 djonesBal = djones.balanceOf(address(this));

        djones.approve(address(spookyswapRouter), 0);
        djones.approve(address(spookyswapRouter), djonesBal);

        address[] memory tokenRoute = new address[](2);
        tokenRoute[0] = address(djones);
        tokenRoute[1] = address(jones);

        spookyswapRouter.swapExactTokensForTokens(djonesBal, 0, tokenRoute, address(this), block.timestamp + 800);

        uint256 jonesHeld = jones.balanceOf(address(this));
        jones.burn(jonesHeld);
    }

    function endRecoveryProcess() public onlyOperator {
        pushPeg();
        selfdestruct(msg.sender);
    }

    function pendingDjones() public view returns (uint256) {
        return djonesPool.pendingShare(pid, address(this));
    }

    function _createRecoveryToken() private returns (DummyToken) {
        DummyToken token = new DummyToken();
        return token;
    }
}

