pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

contract NFTStakingBeta is Initializable, OwnableUpgradeable, IERC721Receiver {
    using SafeMath for uint256;

    IERC721 public NekoContract;

    function initialize(IERC721 _neko) public initializer {
        __Ownable_init();
        NekoContract = _neko;
    }

    struct TokenInfo {
        address owner;
        uint256 timestamp;
    }

    // tokenId => TokenInfo
    mapping(uint256 => TokenInfo) public tokenInfos;

    // address => tokenIds
    mapping(address => uint256[]) public stakedPool;

    event Staked(address indexed staker, uint256 indexed tokenId);
    event Unstaked(address indexed staker, uint256 indexed tokenId);

    // TODO: unused
    mapping(address => uint256[]) public stakingPools;

    // map[address]number_of_current_staking
    mapping(address => uint256) public stakingAmount;

    // map[address]isExistedStaker
    mapping(address => bool) public existedStakers;

    // map[address][rewardIndex] = amount
    // TODO: remove
    mapping(address => mapping(uint256 => uint256)) public balances;

    // map[erc20Address]index
    mapping(address => uint256) public rewardIndex;

    // map[erc20Address]existedAddress
    mapping(address => bool) public rewardChecker;

    address[] public stakers;
    address[] public rewards;
    uint256 public totalNekoStaking;
    bool public disableStaking;

    mapping(address => uint256) public availableReward;

    // uint256 public currentAward;
    // mapping(address => bool) claimableAward;
    // mapping(address => bool) didClaim;

    function unstakeNft(uint256 tokenId) public {
        require(!disableStaking);
        TokenInfo memory tokenInfo = tokenInfos[tokenId];
        require(
            tokenInfo.owner == msg.sender,
            "NFTStakingV1: token not staked or incorrect owner"
        );
        // clear data
        tokenInfo.owner = address(0);
        tokenInfos[tokenId] = tokenInfo;
        stakingAmount[msg.sender] = stakingAmount[msg.sender] - 1;
        totalNekoStaking = totalNekoStaking - 1;
        NekoContract.safeTransferFrom(address(this), msg.sender, tokenId);
        emit Unstaked(msg.sender, tokenId);
    }

    function withdrawAll() public {
        for (uint256 i = 0; i < rewards.length; i++) {
            uint256 amount = balances[msg.sender][i];
            if (amount > 0) {
                IERC20(rewards[i]).transfer(msg.sender, amount);
            }
            balances[msg.sender][i] = 0;
        }
    }

    function stakeBatchNft(uint256[] calldata tokenIds) external {
        require(!disableStaking);
        if (!existedStakers[msg.sender]) {
            existedStakers[msg.sender] = true;
            stakers.push(msg.sender);
        }
        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(
                NekoContract.ownerOf(tokenIds[i]) != address(this),
                "NFTStakingV1: neko already staked"
            );
            require(
                msg.sender == NekoContract.ownerOf(tokenIds[i]),
                "NFTStakingV1: not owner of one or more nekos"
            );
            stakingAmount[msg.sender] = stakingAmount[msg.sender] + 1;
            totalNekoStaking = totalNekoStaking + 1;
            tokenInfos[tokenIds[i]] = TokenInfo(msg.sender, block.timestamp);
            NekoContract.safeTransferFrom(
                msg.sender,
                address(this),
                tokenIds[i]
            );
        }
    }

    function unstakeBatchNft(uint256[] calldata tokenIds) external {
        require(!disableStaking);
        for (uint256 i = 0; i < tokenIds.length; i++) {
            stakingAmount[msg.sender] = stakingAmount[msg.sender] - 1;
            totalNekoStaking = totalNekoStaking - 1;
            unstakeNft(tokenIds[i]);
        }
    }

    ///////////////////
    // VIEW
    ///////////////////
    function stakerLength() public view returns (uint256) {
        return stakers.length;
    }

    function rewardLength() public view returns (uint256) {
        return rewards.length;
    }

    //////////////////////
    // ADMIN function
    /////////////////////
    function addReward(address _erc20, uint256 _amount) public onlyOwner {
        if (!rewardChecker[_erc20]) {
            rewardChecker[_erc20] = true;
            rewards.push(_erc20);
            rewardIndex[_erc20] = rewards.length - 1;
        }
        availableReward[_erc20] = availableReward[_erc20] + _amount;
        IERC20(_erc20).transferFrom(msg.sender, address(this), _amount);
    }

    function ownerUnstake(uint256 tokenId) public onlyOwner {
        TokenInfo memory tokenInfo = tokenInfos[tokenId];
        NekoContract.safeTransferFrom(address(this), tokenInfo.owner, tokenId);
        stakingAmount[tokenInfo.owner] = stakingAmount[tokenInfo.owner] - 1;
        emit Unstaked(tokenInfo.owner, tokenId);

        tokenInfo.owner = address(0);
        tokenInfos[tokenId] = tokenInfo;
        totalNekoStaking = totalNekoStaking - 1;
    }

    function calculateReward(
        address _erc20,
        uint256 startIndex,
        uint256 endIndex,
        uint256 amountPerNeko
    ) public onlyOwner {
        for (uint256 i = startIndex; i <= endIndex; i++) {
            if (stakingAmount[stakers[i]] > 0) {
                balances[stakers[i]][rewardIndex[_erc20]] += amountPerNeko.mul(
                    stakingAmount[stakers[i]]
                );
            }
        }
    }

    function setStaker(address staker, uint256 amount) external onlyOwner {
        // require(!existedStakers[staker]);
        // if (!existedStakers[staker]) {
        //     existedStakers[staker] = true;
        //     stakers.push(staker);
        // }
        stakingAmount[staker] = amount;
        // totalNekoStaking = totalNekoStaking + amount;
    }

    function setDisable(bool _disable) external onlyOwner {
        disableStaking = _disable;
    }

    function ownerUnstakeBatchNft(uint256[] calldata tokenIds)
        external
        onlyOwner
    {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            ownerUnstake(tokenIds[i]);
        }
    }

    // we override the receiver method
    function onERC721Received(
        address,
        address from,
        uint256 tokenId,
        bytes memory
    ) public virtual override returns (bytes4) {
        require(!disableStaking);
        require(
            address(NekoContract) == msg.sender,
            "NFTStakingV1: not Neko contract"
        );
        tokenInfos[tokenId] = TokenInfo(from, block.timestamp);
        stakingAmount[from] = stakingAmount[from] + 1;
        totalNekoStaking = totalNekoStaking + 1;

        if (!existedStakers[from]) {
            existedStakers[from] = true;
            stakers.push(from);
        }

        emit Staked(from, tokenId);
        return this.onERC721Received.selector;
    }
}

