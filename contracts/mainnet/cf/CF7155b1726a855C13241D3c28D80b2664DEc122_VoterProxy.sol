interface IGaugeProxy {
    function getTreasury() external view returns (address);
    function getDepositFeeRate() external view returns (uint256);
    function collect(uint256 _amount) external;
    function vote(address[] calldata _tokenVote, uint256[] calldata _weights, address _owner) external ;
    function reset(address[] calldata tokens, uint256[] calldata amounts) external;
    function epochLength() external view returns (uint256) ;

    function tokens() external view returns (address[] memory);

    function getGauge(address _token) external view returns (address);

    function length() external view returns (uint);


    function getGaugeWeight(address _token) external view returns(uint256);
    function getGaugeWeightEpoch(address _token, uint256 epoch) external view returns (uint256);

    function getTotalWeight() external view returns(uint256);
    function getTotalWeightEpoch(uint256 epoch) external view returns(uint256);
}

