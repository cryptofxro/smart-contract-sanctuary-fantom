pragma solidity 0.6.12;

import "@openzeppelin/contracts/math/SafeMath.sol";

import "hardhat/console.sol";

abstract contract DelegateRegistry {
    using SafeMath for uint256;
  

    //  User delegation info:
    //  User Address -> Epoch -> bytes: [delegationAddress + AmountDelegated]
    mapping(address => mapping( uint256 => bytes )) public userDelegationRegistry;

    // User delegated amount info:
    // Delegation Address -> Epoch -> Amount under delegation
    mapping(address => mapping( uint256 => uint256 )) public userDelegatedAmountRegistry;

    // User automation address registry:
    // User address -> Keeper address -> true/false
    mapping(address => mapping(address => bool)) public userAutomateDelegationRegistryWhitelist;


    // Delegation events
    event SetDelegate(address indexed delegator, address indexed delegate, uint256 indexed epoch, uint256 weight);
    event ClearDelegate(address indexed delegator, address indexed delegate, uint256 indexed epoch, uint256 weight);




    /// @dev Sets a delegate for the _user
    /// @param _delegate Address of the delegate
    /// @param _user Address of the user that wants to delegate
    /// @param _weight total weight to delegate
    /// @param _epoch current epoch for delegation
    function setDelegate(address _user, address _delegate, uint256 _weight, uint256 _epoch) internal {        
        require(_delegate != _user, "Can't delegate to self");
        require(_delegate != address(0), "Can't delegate to 0x0");
        require(_weight > 0, 'no lockedTokens');
        address currentDelegate;
        
        bytes memory currentDelegateData = userDelegationRegistry[_user][_epoch];
        
        // check if data.len is != 0, if y check currDel != address(0)
        if(currentDelegateData.length != 0){
            (currentDelegate, ) = abi.decode ( userDelegationRegistry[_user][_epoch] , (address, uint256) );  
            console.log(currentDelegate);

            // clear before set a new a delgation, this avoid loosing track of any ev. delegated amount 
            require(currentDelegate == address(0), 'call clearDelegate()');
        }

        userDelegationRegistry[_user][_epoch] = abi.encode(_delegate, _weight);
        userDelegatedAmountRegistry[_delegate][_epoch] = userDelegatedAmountRegistry[_delegate][_epoch].add(_weight);
    
        emit SetDelegate(_user, _delegate, _epoch, _weight);
    }




    /// @dev Clears a delegate for the _user
    /// @param _user    Id for which the delegate should be set 
    /// @param _epoch   current epoch for delegation
    /// @param currentDelegate  current delegate of the user, need to know to clear info
    /// @param amount   delegated amount by user 
    function clearDelegate(address _user, uint256 _epoch, address  currentDelegate, uint256 amount) internal {
        require(currentDelegate != address(0), "No delegate set");
        require(amount > 0, 'amount delegated 0');

        // update delegatorRegistry mapping
        userDelegationRegistry[_user][_epoch] = abi.encode(address(0), 0);
        userDelegatedAmountRegistry[currentDelegate][_epoch].sub(amount);

        emit ClearDelegate(_user, currentDelegate,  _epoch, amount);
    }


 
}

