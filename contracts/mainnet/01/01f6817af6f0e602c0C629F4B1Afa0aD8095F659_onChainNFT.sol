contract onChainNFT is ERC721Enumerable, Ownable {
    using Strings for uint256;
    PonyPostImages images;
    address public _happyfpony;

    address public _pfpony;
    uint256 public fullPrice = 5 ether;
    uint256 public ponyPrice = 2 ether;
    uint256 public happyPonyPrice = 1 ether;

    mapping(uint256 => IPonyPostImages.Message) public msgToTokenId;

    uint256 public stringLimit = 20;

    constructor(PonyPostImages _images) ERC721("PonyPost", "PPOST") {
        images = _images;
    }

    //Settings
    function setPFPony(address pfpony_) external onlyOwner {
        _pfpony = pfpony_;
    }

    function setHappyFPony(address happyfpony_) external onlyOwner {
        _happyfpony = happyfpony_;
    }

    // public
    function mint(address _to, string memory _userText1, string memory _userText2, string memory _userText3, uint256 _gift) public payable {
        if (ERC721Enumerable(_pfpony).balanceOf(msg.sender) < 1) {
            require(msg.value == _gift * 10 ** 18 + fullPrice);
        } else {
            if (IHapPFPony(_happyfpony).hasHappyPony(msg.sender)) {
                require(msg.value == _gift * 10 ** 18 + happyPonyPrice);
            } else {
                require(msg.value == _gift * 10 ** 18 + ponyPrice);
            }
        }

        uint256 supply = totalSupply();
        bytes memory strBytes1 = bytes(_userText1);
        bytes memory strBytes2 = bytes(_userText2);
        bytes memory strBytes3 = bytes(_userText3);
        require(strBytes1.length <= stringLimit, "String input exceeds limit.");
        require(strBytes2.length <= stringLimit, "String input exceeds limit.");
        require(strBytes3.length <= stringLimit, "String input exceeds limit.");

        IPonyPostImages.Message memory newMessage = IPonyPostImages.Message(
            string(abi.encodePacked("PonyPost' message #", uint256(supply + 1).toString())),
            "A message from the PonyPost' !",
            _userText1,
            _userText2,
            _userText3,
            _gift
        );

        if (msg.sender != owner()) {
            require(msg.value >= 1 ether);
        }

        msgToTokenId[supply + 1] = newMessage;
        //Add msg to mapping @tokenId
        _safeMint(_to, supply + 1);
        (bool success,) = payable(_to).call{
        value : _gift * 10 ** 18
        }("");
        require(success);
    }

    function tokenURI(uint256 _tokenId)
    public
    view
    virtual
    override
    returns (string memory)
    {
        require(
            _exists(_tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );
        IPonyPostImages.Message memory currentMessage = msgToTokenId[_tokenId];
        return images.buildMetadata(_tokenId, currentMessage);
    }

    //only owner
    function withdraw() public payable onlyOwner {
        (bool success,) = payable(msg.sender).call{
        value : address(this).balance
        }("");
        require(success);
    }
}