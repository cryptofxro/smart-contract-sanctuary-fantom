pragma solidity 0.8.9;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

import "./interfaces/IStrategy.sol";
import "./interfaces/IOneRingCurveRouter.sol";
import "./interfaces/IUniswapV2Pair.sol";
import "./interfaces/IUniswapV2Router02.sol";
import "./interfaces/IMasterChef.sol";

import "./mixins/Ownable.sol";

contract MasterChefStrategy is IStrategy, Ownable {

  using SafeERC20 for IERC20;

  // The address of the LP token
  address public immutable underlying;
  // The address of the reward token
  address public immutable rewardToken;
  // MasterChef staking pool to use
  address public immutable masterChefPool;
  // Exchange using
  address public immutable dexRouter;
  // The id of the pool in masterchef
  uint256 public immutable poolId;

  address public constant usdcContract = 0x04068DA6C83AFCFA0e13ba15A6696662335D5B75;

  address public curveRouter;
  address public strategyManager;
  address public vault;
  string public name;

  event MasterChefStrategyInitialized(
    address indexed dex, 
    address indexed masterChefPool,
    address indexed vault,
    address underlying, 
    address rewardToken, 
    uint256 poolId,
    address StablesRouter
  );

  error MasterChefStrategy_Cannot_Initializad_Again();
  error MasterChefStrategy_Invalid_Address();
  error MasterChefStrategy_Invalid_Underlying_Or_PoolId();

  modifier onlyManager {
    require(msg.sender == strategyManager);
    _;
  }

  modifier StrategyManagerOrOwner {
    require(msg.sender == strategyManager || msg.sender == owner());
    _;
  }

  constructor(
    address _strategyManager, 
    string memory _name,
    address _underlying,
    address _rewardToken,
    address _dexRouter,
    address _masterChefPool,
    uint256 _poolId,
    address _curveRouter,
    address _vault
  )

  { 
    if(_strategyManager == address(0)) {
      revert MasterChefStrategy_Invalid_Address();
    }

    if(_underlying == address(0)) {
      revert MasterChefStrategy_Invalid_Address();
    }

    if(_rewardToken == address(0)) {
      revert MasterChefStrategy_Invalid_Address();
    }

    if(_dexRouter == address(0)) {
      revert MasterChefStrategy_Invalid_Address();
    }

    if(_masterChefPool == address(0)) {
      revert MasterChefStrategy_Invalid_Address();
    }

    if(_curveRouter == address(0)) {
      revert MasterChefStrategy_Invalid_Address();
    }


    strategyManager = _strategyManager;
    name = _name;
    underlying = _underlying;
    rewardToken = _rewardToken;
    poolId = _poolId;
    dexRouter = _dexRouter;
    masterChefPool = _masterChefPool;
    curveRouter = _curveRouter;
    vault = _vault;

    (address expectedUnderlying, , , ) = IMasterChef(masterChefPool).poolInfo(poolId);

    if(expectedUnderlying != underlying) {
      revert MasterChefStrategy_Invalid_Underlying_Or_PoolId();
    }

    address _token0 = IUniswapV2Pair(underlying).token0();
    address _token1 = IUniswapV2Pair(underlying).token1();

    IERC20(underlying).safeApprove(masterChefPool, type(uint256).max);
    IERC20(_token0).safeApprove(dexRouter, type(uint256).max);
    IERC20(_token1).safeApprove(dexRouter, type(uint256).max);
    IERC20(underlying).safeApprove(dexRouter, type(uint256).max);
    
    emit MasterChefStrategyInitialized(
      _dexRouter, masterChefPool, _vault, _underlying, _rewardToken, _poolId, _curveRouter
    );
  }

  // This function must return the exactly amount of underlyings that it got
  function tokenToUnderlying(address token, uint256 amount) external onlyManager returns(uint256) {
    address _token0 = IUniswapV2Pair(underlying).token0();
    address _token1 = IUniswapV2Pair(underlying).token1();

    return _tokenToUnderlying(token, amount, _token0, _token1);
  }

  function _tokenToUnderlying(address token, uint256 amount, address _token0, address _token1) internal returns(uint256) {
    uint256 _balance = IERC20(token).balanceOf(address(this));
    uint256 _liquidity;

    if(_balance >= amount) {

      uint256 _halfToToken0 = amount/2;
      // Division in Solidity always round to the least integer.
      // Because of this, this cannot underflow.
      uint256 _halfToToken1 = amount - _halfToToken0;

      uint256 _token0Amount = IOneRingCurveRouter(curveRouter).swap(token, _token0, _halfToToken0);
      uint256 _token1Amount = IOneRingCurveRouter(curveRouter).swap(token, _token1, _halfToToken1);

      // we provide liquidity

      (, , _liquidity) = IUniswapV2Router02(dexRouter).addLiquidity(
        _token0,
        _token1,
        _token0Amount,
        _token1Amount,
        1, // We are willing to take whatever the pair gives us
        1, // We are willing to take whatever the pair gives us
        address(this),
        block.timestamp
      );
    }

    return _liquidity;

  }

  function investUnderlying() external onlyManager returns(address, address, uint256) {
    uint256 balanceUnderlying = IERC20(underlying).balanceOf(address(this));

    IMasterChef(masterChefPool).deposit(poolId, balanceUnderlying);

    return(masterChefPool, underlying, balanceUnderlying);
  }

  /// This function must return the exactly amount of USDC sent to recipient
  function withdrawAllUSDCToRecipient(address recipient) external StrategyManagerOrOwner returns(uint256) {
    _exitAllMasterChef();

    uint256 balance = IERC20(underlying).balanceOf(address(this));

    uint256 usdcWithdrawed = usdcFromUnderlying(balance);

    IERC20(usdcContract).safeTransfer(recipient, usdcWithdrawed);

    return usdcWithdrawed;
  }
  
  /// This function must return the exactly amount of USDC sent to recipient
  function withdrawUSDCToRecipient(uint256 amountUSDC, address recipient) external StrategyManagerOrOwner returns(uint256) {

    uint256 _usdBalance = getUSDBalanceFromUnderlying(masterChefUnderlyingBalanace());

    if (_usdBalance == 0) {
      return 0;
    }

    uint256 amountInUSDToWithdraw = amountUSDC/(10**6);

    uint256 amountOfLPToWithdraw = (masterChefUnderlyingBalanace()*amountInUSDToWithdraw)/_usdBalance;

    /// Withdraw 1% more
    uint256 amountOfLPToWithdrawModified = (amountOfLPToWithdraw*101)/100;

    IMasterChef(masterChefPool).withdraw(poolId, amountOfLPToWithdrawModified);

    uint256 usdcWithdrawed = usdcFromUnderlying(amountOfLPToWithdrawModified);

    IERC20(usdcContract).safeTransfer(recipient, usdcWithdrawed);

    return usdcWithdrawed;
  }

  function usdcFromUnderlying(uint256 amountUnderlying) internal returns(uint256) {
    address _token0 = IUniswapV2Pair(underlying).token0();
    address _token1 = IUniswapV2Pair(underlying).token1();

    IUniswapV2Router02(dexRouter).removeLiquidity(
      _token0,
      _token1,
      amountUnderlying,
      1, // we are willing to take whatever the pair gives us
      1, // we are willing to take whatever the pair gives us
      address(this),
      block.timestamp
    );

    uint256 _token0Amount = IERC20(_token0).balanceOf(address(this));
    uint256 _token1Amount = IERC20(_token1).balanceOf(address(this));

    uint256 _USDCToken0Amount = IOneRingCurveRouter(curveRouter).swap(_token0, usdcContract, _token0Amount);
    uint256 _USDCToken1Amount = IOneRingCurveRouter(curveRouter).swap(_token1, usdcContract, _token1Amount);

    return (_USDCToken0Amount + _USDCToken1Amount);
  }

  /// Used to withdraw the rewards
  function withdrawRewardsToRecipient(address recipient) external StrategyManagerOrOwner {
    IMasterChef(masterChefPool).withdraw(poolId,0);

    uint256 rewardBalance = IERC20(rewardToken).balanceOf(address(this));

    IERC20(rewardToken).safeTransfer(recipient,rewardBalance);
  }

  function _exitAllMasterChef() internal {
    uint256 _balance = masterChefUnderlyingBalanace();
    if (_balance != 0) {
      IMasterChef(masterChefPool).withdraw(poolId, _balance);
    }
  }

  //////////////////////////////////////// VIEW FUNCTIONS /////////////////////////////////////////

  /// Reading directly the reserves can cause an attack?
  /// On the vault contract we tried to protect us against flash loans
  function getUSDBalanceFromUnderlying(uint256 _amountUnderlying) public view returns(uint256) {
    if (_amountUnderlying > 0) {
      address _token0 = IUniswapV2Pair(underlying).token0();
      address _token1 = IUniswapV2Pair(underlying).token1();

      (uint256 _reserves0, uint256 _reserves1, ) = IUniswapV2Pair(underlying).getReserves();

      uint256 _totalSupply = IERC20(underlying).totalSupply();

      uint256 _amount0 = (_reserves0*_amountUnderlying)/(_totalSupply);
      uint256 _amount1 = (_reserves1*_amountUnderlying)/(_totalSupply);

      uint256 _token0Unit = uint256(10) ** uint256(ERC20(_token0).decimals());
      uint256 _token1Unit = uint256(10) ** uint256(ERC20(_token1).decimals());

      /// _amount0 and _amount1 will always be greater than the unit, sice we only allow
      /// deposites greater or equal than 10 USD.
      uint256 _totalAmountInUSD = (_amount0/_token0Unit) + (_amount1/_token1Unit);

      return _totalAmountInUSD;
    }

    return 0;
  }

  function investedBalanceInUSD() public view returns(uint256) {
    uint256 _underlyingBalance = underlyingBalance();

    return getUSDBalanceFromUnderlying(_underlyingBalance);
  }

  function masterChefUnderlyingBalanace() public view returns(uint256) {
    (uint256 _balance, ) = IMasterChef(masterChefPool).userInfo(poolId, address(this));

    return _balance;
  }

  function underlyingBalance() public view returns (uint256) {
    uint256 balanceUnderlyingInMasterChef = masterChefUnderlyingBalanace();

    return balanceUnderlyingInMasterChef + IERC20(underlying).balanceOf(address(this));
  }

  ///////////////////////////////// MANAGEMENT ///////////////////////////////////// 

  function changeStrategyManager(address newStrategyManager) external onlyOwner {
    require(newStrategyManager != address(0));

    strategyManager = newStrategyManager;
  }

  function increaseAllowance(address token, address spender) external onlyOwner {
    IERC20(token).safeApprove(spender, 0);
    IERC20(token).safeApprove(spender, type(uint256).max);
  }

  function approveCurveRouter(address[] calldata _stableCoins) external onlyOwner {
    for (uint256 i = 0; i < _stableCoins.length; i++) {
      IERC20(_stableCoins[i]).safeApprove(curveRouter, type(uint256).max);
    }
  }

  function withdrawERC20(address erc20Contract) external onlyOwner {
    require(erc20Contract != address(0));

    uint256 balance = IERC20(erc20Contract).balanceOf(address(this));
    IERC20(erc20Contract).safeTransfer(msg.sender, balance);
  }
}

