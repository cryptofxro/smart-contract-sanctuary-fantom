contract Arbitrage_dexs_getBalances is Ownable{

  struct rout_tokens_balances{
    address router;
    address token1;
    address token2;
    uint balance1;
    uint balance2;
  }

  function sortTokens(address tokenA, address tokenB) private pure returns (
                                                                      address token0, 
                                                                      address token1) {
        require(tokenA != tokenB, 'PancakeLibrary: IDENTICAL_ADDRESSES');
        (token0, token1) = tokenA < tokenB ? (tokenA, tokenB) : (tokenB, tokenA);
        require(token0 != address(0), 'PancakeLibrary: ZERO_ADDRESS');
  }

  function pairFor(address factory, 
                   address tokenA, 
                   address tokenB) internal view returns (address pair) {
        (address token0, address token1) = sortTokens(tokenA, tokenB);
        pair = IPancakeFactory(factory).getPair(token0, token1);
    }

  function getReserves(address factory, 
                       address tokenA, 
                       address tokenB
                       ) public view returns (uint reserveA, 
                                              uint reserveB) {
        (address token0,) = sortTokens(tokenA, tokenB);
        pairFor(factory, tokenA, tokenB);
        (uint reserve0, uint reserve1,) = IPancakePair(pairFor(factory, tokenA, tokenB)).getReserves();
        (reserveA, reserveB) = tokenA == token0 ? (reserve0, reserve1) : (reserve1, reserve0);
    }

  function getBalances(
                       address[] calldata routers, 
                       address[2][] calldata tokens
                      ) public view returns (rout_tokens_balances[] memory){
          rout_tokens_balances[] memory balances = new rout_tokens_balances[](tokens.length);
          uint bestAmountOut;
          uint amountOut;
          address bestRouter;
          uint reserve0;
          uint reserve1;
          address[] memory path_swap = new address[](2);
          
          for (uint i = 0; i < tokens.length; i++){
             bestAmountOut = 0;
             path_swap[0] = tokens[i][0];
             path_swap[1] = tokens[i][1];

             for (uint j = 0; j < routers.length; j++){
                amountOut = IPancakeRouter02(routers[j]).getAmountsOut(0.01 ether, path_swap)[1];
                if (amountOut > bestAmountOut){
                  bestAmountOut = amountOut;
                  bestRouter = routers[j];
                }
             }
             
             (reserve0, reserve1) = getReserves(IPancakeRouter02(bestRouter).factory(), tokens[i][0], tokens[i][1]);
             balances[i] = rout_tokens_balances(bestRouter, tokens[i][0], tokens[i][1], reserve0, reserve1);
          }
          return balances;          
  }
}

