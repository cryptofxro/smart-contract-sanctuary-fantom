// ---------------------------------------------- ShadeMonkeys -----------------------------------------------
// -----------------------------------------------------------------------------------------------------------
contract ShadeMonkeys is ERC721Enumerable, Ownable {
    uint256 public productionMode;
    
    struct Token {
        uint256 id;
        string uri;
        string image;
        string imageHq;
        uint256 rarity;
    }
    
    string public title;   
    uint256 private rarityTypesNumber;
    mapping (uint256 => string) private rarityTypes;
    
    mapping (uint256 => Token) public tokens; 
    mapping (uint256 => uint256[]) public listByRarityTypes;
    
    mapping (address => uint256[]) public tokensOfUser;
    mapping (address => mapping (uint256 => uint256)) public raritysOfUser;
    
    uint256 constant public MAX_SUPPLY = 250;
    uint256 constant public MAX_MINT = 10;
    uint256 constant public MAX_PER_BUY = 10;
    uint256 constant public MAX_PER_PAGE = 10;
    uint256 public maxPerUser;
    
    uint256 public price;
    address payable public wallet; 
    uint256 private seed;
    
    event Buy(address indexed account, uint256 indexed tokenId);
    
    constructor() ERC721 ("Shade Monkeys", "Monkeys") {
        title = 'Shade Monkeys are a collection of 250 unique pictures of marmoset monkeys. Each picture is a real photograph of the monkeys with many unique attributes';
                       
        rarityTypesNumber = 4;
        rarityTypes[0] = 'Common';
        rarityTypes[1] = 'Uncommon';
        rarityTypes[2] = 'Rare';
        rarityTypes[3] = 'Ultra';
        
        _owner = _msgSender();
        wallet = payable(_owner);
        price = 250000000000000000000;
        productionMode = 1637262000;
    }
    
    function giveaway(uint256 tokenId, address to) public onlyOwner { 
        require(ERC721.balanceOf(address(this)) != 0, "No available tokens for giveaway");
        _approve(msg.sender, tokenId);
        ERC721.safeTransferFrom(address(this), to, tokenId);
    }

    function buy(uint256 amount) public payable {
        require(isProductionMode(), "Allowed only in production mode");
        
        require(ERC721.balanceOf(address(this)) >= amount, "Not enough available tokens");
        require(amount <= MAX_PER_BUY, "You can't buy more then max per buy amount");
        
        require(msg.value == price * amount, "Wrong payment value sent");
        
        for (uint256 i = 0; i < amount; i++) {
            uint256 sellIndex;
            
            uint256 availableForSell = ERC721.balanceOf(address(this));
            
            if (availableForSell > 1) {
                uint256 random = uint256(
    				keccak256(abi.encodePacked( 
    				    block.timestamp +
    				    block.difficulty +
    				    ((uint256(keccak256(abi.encodePacked(block.coinbase)))) / block.timestamp) +
    				    block.gaslimit +
    				    ((uint256(keccak256(abi.encodePacked(msg.sender)))) / block.timestamp) +
    				    block.number +
    				    seed +
    				    i
    			    ))
    			);
    			sellIndex = random % availableForSell;
    			seed = seed + msg.sender.balance;
    		}
    		
    		uint256[] storage _tokensOfUser = tokensOfUser[address(this)];
    		uint256 tokenId = _tokensOfUser[sellIndex];
    		
            for (uint256 n = sellIndex; n < _tokensOfUser.length - 1; n ++){
                _tokensOfUser[n] = _tokensOfUser[n + 1];
            }
            _tokensOfUser.pop(); 
    		
    		_approve(msg.sender, tokenId);
            ERC721.safeTransferFrom(address(this), msg.sender, tokenId);
            emit Buy(msg.sender, tokenId);
        }

        (bool sent,) = wallet.call{ value: msg.value }("");
        require(sent, "FTM send failed");
    }
    
    function _beforeTokenTransfer(address from, address to, uint256 tokenId) internal override virtual {
        super._beforeTokenTransfer(from, to, tokenId);
        
        if (from != address(0)) {
            require(to != address(this), "Can't receive sold tokens");
            if (maxPerUser != 0) {
                require(ERC721.balanceOf(to) < maxPerUser, "You can't hold more then max per user amount");
            }
            
            if (from != address(this)) {
                uint256[] storage _tokensOfUser = tokensOfUser[from];
                uint256 index;
                for (uint256 i = 0; i < _tokensOfUser.length; i ++){
                    if (_tokensOfUser[i] == tokenId) {
                        index = i;
                        break;
                    }
                }
                for (uint256 i = index; i < _tokensOfUser.length - 1; i ++){
                    _tokensOfUser[i] = _tokensOfUser[i+1];
                }
                _tokensOfUser.pop();   
            }
            
            raritysOfUser[from][tokens[tokenId].rarity] -= 1;            
        }
        
        tokensOfUser[to].push(tokenId);        
        raritysOfUser[to][tokens[tokenId].rarity] += 1;        
    }
    
    function mint(string[] memory uri, string[] memory image, string[] memory imageHq, uint256[] memory rarity) public onlyOwner {
        uint256 length = uri.length;
        require(length <= MAX_MINT, "Allowed MAX_MINT per once");
        require(totalSupply() + length <= MAX_SUPPLY, "MAX_SUPPLY reached");
        
        require(length == image.length, "Image array must be same length as uri array");
        require(length == imageHq.length, "Image Hq type array must be same length as uri array");        
        require(length == rarity.length, "Rarity array must be same length as uri array");

        for (uint32 index = 0; index < length; index ++) {  
            createToken(uri[index], image[index], imageHq[index], rarity[index]);
        }
    }
    
    function transferOwnership(address newOwner) public onlyOwner {
   	    wallet = payable(newOwner);
        _transferOwnership(newOwner);
    }
    
    function setPrice(uint256 newPrice) external onlyOwner {
        price = newPrice;
    }
    
    function setWallet(address payable newWallet) external onlyOwner {
        wallet = newWallet;
    }
    
    function setMaxPerUser(uint256 newMaxPerUser) external onlyOwner {
        require(!isProductionMode(), "Not allowed in production mode mode");
        maxPerUser = newMaxPerUser;
    }
    
    function setProductionMode(uint256 startTime) external onlyOwner {
        if (startTime == 0) {
            startTime = block.timestamp;
        }
        require(startTime >= block.timestamp, "Start time must be 0 for start rihgt now or next to now");
        require(!isProductionMode(), "Not allowed in production mode mode");
        productionMode = startTime;
    }
    
    function isProductionMode() public view returns (bool) {
        return productionMode != 0 && productionMode <= block.timestamp;
    }
    
    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        require(ERC721._exists(tokenId), "Token not exist");
        return tokens[tokenId].uri;
    }
    
    function checkTokenData(string memory uri, string memory image, string memory imageHq, uint256 rarity) internal view {
        require(bytes(uri).length <= 120, "Too long uri length");
        require(bytes(image).length <= 46, "Too long image length");
        require(bytes(imageHq).length <= 46, "Too long imageHq length");        
		require(rarity < rarityTypesNumber, "No such rarity id");
    }

    function createToken(string memory uri, string memory image, string memory imageHq, uint256 rarity) internal onlyOwner {
        require(!isProductionMode(), "Not allowed in production mode mode");
        require(totalSupply() < MAX_SUPPLY, "Max supply reached");
        
        checkTokenData(uri, image, imageHq, rarity);
        
        uint256 tokenId = totalSupply() + 1;
        tokens[tokenId] = Token(tokenId, uri, image, imageHq, rarity);
        
        ERC721._mint(address(this), tokenId);
        listByRarityTypes[rarity].push(tokenId);
        
        seed = seed + msg.sender.balance;
    }
    
    function updateToken(uint256 tokenId, string memory uri, string memory image, string memory imageHq) external onlyOwner {
        require(!isProductionMode(), "Not allowed in production mode mode");
        require(ERC721._exists(tokenId), "Token not exist");
        
        checkTokenData(uri, image, imageHq, 0);
        
        Token storage token = tokens[tokenId];
        token.uri = uri;
        token.image = image;
        token.imageHq = imageHq;
    }
       
	function userTokens(address account) public view returns (uint256 [] memory){
        return tokensOfUser[account];
    }
        
    function paginatedList(uint256 rarity, uint256 page) public view returns (Token [] memory){
        require(rarity < rarityTypesNumber, "No such rarity id");
        
        uint256 start = page * MAX_PER_PAGE;        
        uint256[] memory rarityList = listByRarityTypes[rarity];        
        require(start < rarityList.length, "Out of start range");
        
        uint256 end = start + MAX_PER_PAGE;
        if (end > rarityList.length) {
            end = rarityList.length;
        }
        
        Token[] memory list = new Token [](end - start);
        uint256 index;
        for (uint256 i = start; i < end; i ++) {
            list[index] = tokens[rarityList[i]];
            index ++;
        }
        return list;
    }
        
    function contractInfo() public view returns (
        string memory, // title
        string[] memory rarityTypesList,
        uint256[] memory, // tokensByRarityIdsList 0
        uint256[] memory, // tokensByRarityIdsList 1
        uint256[] memory, // tokensByRarityIdsList 2
        uint256[] memory,  // tokensByRarityIdsList 3
        uint256, // productionMode
        uint256[] memory contractVariables,
        uint256[] memory raritysOfContract,
        bool // isProductionMode
        ) {
            // rarity
            rarityTypesList = new string[](rarityTypesNumber);
            for (uint256 i = 0; i < rarityTypesNumber; i ++) {
                rarityTypesList[i] = rarityTypes[i];
            }            
            
            contractVariables = new uint256[](6);
            contractVariables[0] = totalSupply();
            contractVariables[1] = ERC721.balanceOf(address(this));
            contractVariables[2] = price;
            contractVariables[3] = maxPerUser;
            contractVariables[4] = MAX_PER_PAGE;
            contractVariables[5] = MAX_PER_BUY;
            
            raritysOfContract = new uint256[](4);
            raritysOfContract[0] = raritysOfUser[address(this)][0];
            raritysOfContract[1] = raritysOfUser[address(this)][1];
            raritysOfContract[2] = raritysOfUser[address(this)][2];
            raritysOfContract[3] = raritysOfUser[address(this)][3];
            
        return (
            title,  
            rarityTypesList,
            listByRarityTypes[0],
            listByRarityTypes[1],
            listByRarityTypes[2],
            listByRarityTypes[3],
            productionMode,
            contractVariables,
            raritysOfContract,
            isProductionMode()
        );
    }
    
    function tokenInfo(uint256 tokenId) public view returns (Token memory, address) {
        require(ERC721._exists(tokenId), "Token not exist");
        return (
            tokens[tokenId],
            ERC721.ownerOf(tokenId)
        );
    }
    
    function userInfo(address account) public view returns (
            uint256[] memory, // tokensOfUser,
            uint256, // raritysOfUser 0,
            uint256, // raritysOfUser 1,
            uint256, // raritysOfUser 2,
            uint256  // raritysOfUser 3,
        ) {
            
        return (
            tokensOfUser[account],
            raritysOfUser[account][0],
            raritysOfUser[account][1],
            raritysOfUser[account][2],
            raritysOfUser[account][3]
        );
    }
    
}