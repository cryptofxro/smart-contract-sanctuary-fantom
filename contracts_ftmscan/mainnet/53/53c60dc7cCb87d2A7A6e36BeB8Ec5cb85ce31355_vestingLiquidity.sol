pragma solidity 0.8.9;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";


contract vestingLiquidity {

  uint256 public constant END_DATE = 1675004400;  // 01/29/2023 15:00 UTC


  address public constant liquidityTokenAddress = 0x84fc84B998A01e34c8A9714A600aeBDe8b4Cc671;
  address public constant receiverAddress = 0x370880694995Aa8A53F71645F7Bec3b0e7bb25d9;



  function claim() external {
    require(block.timestamp > END_DATE);

    uint256 lpBalance = IERC20(liquidityTokenAddress).balanceOf(receiverAddress);
    IERC20(liquidityTokenAddress).transfer(receiverAddress, lpBalance);
  }

}

